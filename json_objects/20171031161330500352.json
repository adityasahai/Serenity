{u'recipes': [{u'sourceName': u'The Comfort of Cooking', u'cuisines': [u'mediterranean', u'european', u'italian'], u'image': u'https://spoonacular.com/recipeImages/590692-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 590692, u'veryPopular': True, u'lowFodmap': False, u'title': u'Easy Parmesan Risotto', u'diets': [u'gluten free'], u'pricePerServing': 152.07, u'creditText': u'The Comfort of Cooking', u'extendedIngredients': [{u'originalString': u'1 cup Arborio rice', u'aisle': u'Pasta and Rice', u'name': u'arborio rice', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rice-white-uncooked.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 10020052, u'unit': u'cup'}, {u'originalString': u'1/2 cup dry white wine', u'aisle': u'Alcoholic Beverages', u'name': u'dry white wine', u'metaInformation': [u'dry white'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-wine.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'liquid', u'id': 14106, u'unit': u'cup'}, {u'originalString': u'2 tablespoons chopped fresh basil', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh basil', u'metaInformation': [u'fresh', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/basil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 2044, u'unit': u'tablespoons'}, {u'originalString': u'kosher salt and black pepper', u'aisle': u'Spices and Seasonings', u'name': u'kosher salt', u'metaInformation': [u'black'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 5.0, u'consistency': u'solid', u'id': 1082047, u'unit': u'servings'}, {u'originalString': u'3 1/2 cups low-sodium chicken broth, plus more if needed', u'aisle': u'Canned and Jarred', u'name': u'low sodium chicken broth', u'metaInformation': [u'low-sodium', u'plus more if needed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chicken-broth.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.5, u'consistency': u'liquid', u'id': 6970, u'unit': u'cups'}, {u'originalString': u'1 medium onion, finely chopped', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'medium', u'finely chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u''}, {u'originalString': u'1/2 cup grated Parmesan (2 ounces), plus more for serving', u'aisle': u'Cheese', u'name': u'parmesan', u'metaInformation': [u'grated', u'plus more for serving'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 2.0, u'consistency': u'solid', u'id': 1033, u'unit': u'ounces'}, {u'originalString': u'3 tablespoons unsalted butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 1145, u'unit': u'tablespoons'}], u'healthScore': 4.0, u'imageType': u'jpg', u'spoonacularScore': 48.0, u'sourceUrl': u'http://www.thecomfortofcooking.com/2010/11/easy-parmesan-risotto.html', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 10, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Melt 2 tablespoons of the butter in a medium-sized pot over medium heat.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Add the onion, \xbd teaspoon salt, and \xbc teaspoon pepper and cook, stirring occasionally, until softened, 6 to 8 minutes.', u'length': {u'number': 6, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [], u'step': u'Add the rice and cook, stirring, for 2 minutes.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 3, u'ingredients': []}, {u'equipment': [], u'step': u'Add the wine and simmer until absorbed.', u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Add half the broth (1 \xbe cups) and simmer, stirring once, until absorbed, 8 to 10 minutes.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'Add the remaining broth and simmer, stirring once, until the rice is tender and creamy, 8 to 10 minutes. (If the rice is not cooked through and the mixture is dry, add more broth and continue to cook until tender.)Stir in the Parmesan and the remaining tablespoon of butter. Sprinkle with the basil and additional Parmesan, if desired.Enjoy!', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/basil.jpg', u'id': 2044, u'name': u'basil'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'Risotto works really well with Chianti, Trebbiano, and Verdicchio. Italians know food and they know wine. Trebbiano and Verdicchio are Italian white wines that pair well with fish and white meat, while Chianti is a great Italian red for heavier, bolder dishes. You could try Tiamo Chianti. Reviewers quite like it with a 4.3 out of 5 star rating and a price of about 10 dollars per bottle.', u'pairedWines': [u'chianti', u'trebbiano', u'verdicchio'], u'productMatches': [{u'description': u'Intense ruby red with garnet tones. Intense, fruity with persistent aromas of spices and licorice. Full with good acidity and balanced tannins.Tiamo is bottled at Chianti DOCG, and certified as made with 100% organic grapes.', u'title': u'Tiamo Chianti', u'price': u'$9.95', u'id': 441645, u'link': u'https://www.wine.com/product/tiamo-chianti-2009/107710', u'imageUrl': u'https://spoonacular.com/productImages/441645-312x231.jpg'}]}, u'aggregateLikes': 4470, u'instructions': u'Melt 2 tablespoons of the butter in a medium-sized pot over medium heat.Add the onion, \xbd teaspoon salt, and \xbc teaspoon pepper and cook, stirring occasionally, until softened, 6 to 8 minutes.Add the rice and cook, stirring, for 2 minutes. Add the wine and simmer until absorbed.Add half the broth (1 \xbe cups) and simmer, stirring once, until absorbed, 8 to 10 minutes.Add the remaining broth and simmer, stirring once, until the rice is tender and creamy, 8 to 10 minutes. (If the rice is not cooked through and the mixture is dry, add more broth and continue to cook until tender.)Stir in the Parmesan and the remaining tablespoon of butter. Sprinkle with the basil and additional Parmesan, if desired.Enjoy!', u'spoonacularSourceUrl': u'https://spoonacular.com/easy-parmesan-risotto-590692', u'dishTypes': [u'side dish'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 5, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}