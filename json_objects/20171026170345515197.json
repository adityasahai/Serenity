{u'recipes': [{u'sourceName': u'Girl Versus Dough', u'cuisines': [u'mediterranean', u'european', u'italian'], u'image': u'https://spoonacular.com/recipeImages/605465-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 255, u'id': 605465, u'veryPopular': True, u'lowFodmap': False, u'title': u'slow cooker italian meatballs', u'diets': [], u'preparationMinutes': 15, u'pricePerServing': 44.32, u'creditText': u'Girl Versus Dough', u'extendedIngredients': [{u'originalString': u'12 ounces bulk mild Italian sausage', u'aisle': u'Meat', u'name': u'bulk pork sausage', u'metaInformation': [u'italian'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raw-pork-sausage.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 12.0, u'consistency': u'liquid', u'id': 7063, u'unit': u'ounces'}, {u'originalString': u'4 eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 4.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'\xbc teaspoon garlic powder', u'aisle': u'Spices and Seasonings', u'name': u'garlic powder', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic-powder.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 1022020, u'unit': u'teaspoon'}, {u'originalString': u'2 lbs ground beef', u'aisle': u'Meat', u'name': u'ground beef', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'liquid', u'id': 10023572, u'unit': u'lbs'}, {u'originalString': u'1 \xbc cups Italian seasoned breadcrumbs', u'aisle': u'Bakery/Bread;Oil, Vinegar, Salad Dressing;Baking', u'name': u'italian seasoned breadcrumbs', u'metaInformation': [u'italian'], u'image': u'https://sp