{u'recipes': [{u'sourceName': u'Simply Sugar and Gluten Free', u'cuisines': [u'mediterranean', u'european', u'italian'], u'image': u'https://spoonacular.com/recipeImages/539846-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 539846, u'veryPopular': False, u'lowFodmap': False, u'title': u'Stuffed Italian Chicken', u'diets': [u'gluten free', u'primal'], u'pricePerServing': 243.47, u'creditText': u'Simply Sugar and Gluten Free', u'extendedIngredients': [{u'originalString': u'3/4 cup canned diced tomatoes, undrained', u'aisle': u'Canned and Jarred', u'name': u'canned diced tomatoes', u'metaInformation': [u'diced', u'canned', u'undrained'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomatoes-canned.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.75, u'consistency': u'solid', u'id': 11531, u'unit': u'cup'}, {u'originalString': u'2 tablespoons shredded carrots', u'aisle': u'Produce', u'name': u'carrots', u'metaInformation': [u'shredded'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 11124, u'unit': u'tablespoons'}, {u'originalString': u'1/2 cup celery, thinly sliced', u'aisle': u'Produce', u'name': u'celery', u'metaInformation': [u'thinly sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/celery.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 11143, u'unit': u'cup'}, {u'originalString': u'pinch of cinnamon', u'aisle': u'Spices and Seasonings', u'name': u'cinnamon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'unitLong': u'pinch', u'unitShort': u'pinch', u'amount': 1.0, u'consistency': u'solid', u'id': 2010, u'unit': u'pinch'}, {u'originalString': u'1/4 cup cottage cheese', u'aisle': u'Cheese', u'name': u'cottage cheese', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/