{u'recipes': [{u'sourceName': u'Kraft Recipes', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/544975-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 45, u'id': 544975, u'veryPopular': False, u'lowFodmap': False, u'title': u'Grilled Chicken Legs with Roasted Corn Packets', u'diets': [u'gluten free', u'dairy free'], u'preparationMinutes': 15, u'pricePerServing': 87.9, u'creditText': u'Kraft Recipes', u'extendedIngredients': [{u'originalString': u'6 chicken drumsticks (1-1/2 lb.)', u'aisle': u'Meat', u'name': u'chicken drumsticks', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chicken-drumsticks.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 5066, u'unit': u'lb'}, {u'originalString': u'2 pkg. (10 oz. each) frozen corn', u'aisle': u'Frozen', u'name': u'frozen corn', u'metaInformation': [u'frozen'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/corn.png', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 20.0, u'consistency': u'solid', u'id': 11913, u'unit': u'oz'}, {u'originalString': u'1 lime, cut into 6 wedges', u'aisle': u'Produce', u'name': u'lime', u'metaInformation': [u'cut into 6 wedges'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lime-wedge.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'liquid', u'id': 9159, u'unit': u''}, {u'originalString': u'1/3 cup KRAFT Real Mayo Mayonnaise', u'aisle': u'Condiments', u'name': u'mayo', u'metaInformation': [u'kraft'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mayonnaise.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.3333333333333333, u'consistency': u'liquid', u'id': 4025, u'unit': u'cup'}, {u'originalString': u'1 jalape\xc3\xb1o pepper, seeded, sliced', u'aisle': u'Spices and Seasonings', u'name': u'pepper', u'metaInformation': [u'seeded', u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredie