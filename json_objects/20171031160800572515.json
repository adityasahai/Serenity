stency': u'liquid', u'id': 4053, u'unit': u'tbsp'}, {u'originalString': u'1 onion, sliced thick', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'thick', u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u''}, {u'originalString': u'2 tbsp. poultry seasoning (I used Stubbs)', u'aisle': u'Spices and Seasonings', u'name': u'poultry seasoning', u'metaInformation': [u'(I used Stubbs)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/seasoning.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 2034, u'unit': u'tbsp'}, {u'originalString': u'1 lbs. red potatoes, washed (if large, cut in half)', u'aisle': u'Produce', u'name': u'red potatoes', u'metaInformation': [u'red', u'washed', u'large', u'cut in half)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-potatoes.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 10011355, u'unit': u'lbs'}, {u'originalString': u'salt & pepper', u'aisle': u'Spices and Seasonings', u'name': u'Salt & Pepper', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 4.0, u'consistency': u'solid', u'id': 1102047, u'unit': u'servings'}, {u'originalString': u'4 1/2 lbs. whole chicken', u'aisle': u'Meat', u'name': u'whole chicken', u'metaInformation': [u'whole'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 4.5, u'consistency': u'solid', u'id': 5006, u'unit': u'lbs'}], u'healthScore': 30.0, u'imageType': u'jpg', u'spoonacularScore': 93.0, u'sourceUrl': u'http://www.peanutbutterandpeppers.com/2014/03/06/skillet-chicken-roast/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 21, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat oven to 425 degrees', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Let chicken rest at room temperature for 30 minutes.Wash chicken and pat dry. Sprinkle the cavity of the chicken with salt and pepper and add garlic cloves.', u'length': {u'number': 30, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'whole garlic cloves'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/kitchen-string.jpg', u'id': 404733, u'name': u'kitchen twine'}], u'step': u'Roll lemon with your hand to make it juicy, cut it in half and add to the cavity of the chicken.Trust the chicken. To trust the chicken, cut about 3 feet of baking twine.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon.jpg', u'id': 9150, u'name': u'lemon'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u"Place the chicken so that breast side is up, and the legs are pointed towards you. Tuck the wing tips under the chicken. Wrap the string under the neck end of the bird, pulling the string ends up over the breast, toward you, plumping up the breast. Then cross the string under the breast. Wrap each end around the closest leg end, and tie tightly so that the legs come together.Rub butter over the chicken and sprinkle with STUBB'S chicken seasoning and a little ground pepper.In a heavy cast iron skillet, layer the bottom of the skillet with onions and carrots.", u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/seasoning.png', u'id': 1012034, u'name': u'dry seasoning rub'}]}, {u'equipment': [], u'step': u'Drizzle with olive oil and give it a mix.Top the onions and carrots with the chicken.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [], u'step': u'Add potatoes around the side of the chicken.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Place in oven and roast for 60 minutes or until the internal temperature reaches 160 degrees.', u'length': {u'number': 60, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/cutting-board.jpg', u'id': 404716, u'name': u'cutting board'}], u'step': u'Place the chicken on a cutting board and cover with aluminium foil.', u'number': 8, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Let the chicken rest for 10 minutes, so that all the juices run back into the chicken. To keep your vegetables warm, turn off the oven and placing the skillet of vegetables back in the oven.', u'length': {u'number': 10, u'unit': u'minutes'}, u'number': 9, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}]}, {u'equipment': [], u'step': u'Cut the chicken into pieces and serve with vegetables.', u'number': 10, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1025006, u'name': u'chicken'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'Chicken works really well with Chardonnay, Pinot Noir, and Sauvignon Blanc. White meat is typically paired with white wine as to not overpower the flavor of the meat, so chardonnay and sauvignon blanc are safe choices. Chicken prepared with a tomato sauce or certainly a red wine sauce can and should be served with a red like a pinot noir. You could try Chehalem INOX Chardonnay. Reviewers quite like it with a 4.1 out of 5 star rating and a price of about 13 dollars per bottle.', u'pairedWines': [u'chardonnay', u'pinot noir', u'sauvignon blanc'], u'productMatches': [{u'description': u"INOX\xae takes its name from the abbreviation of the French word for stainless steel, inoxydable. The wine was created differently from most Chardonnay you've had. We think we've succeeded in expressing the crisp, steely, and fruit-rich side that we love about some Old-World Chardonnays. What makes this possible is the use of exclusively Dijon clones, exceptionally well suited to Oregon's cool climate and exhibiting a richness that does not depend on oak. INOX screams of the hallmarks of a cool climate-brightness, pinpoint fruit, and explosive aromas and flavors. We intend INOX for a full range of use, from hot weather chilling to elegant dinner complements.Quintessential INOX, with lovely white aromas of gardenia and other flowers, peach, apricot, pear, pineapple, and green apple candy showing on the nose and palate; it shows a great balance with relatively low alcohol for the year and bright acid; the length is lovely and the weight rich, with a supple, silky texture; flavors linger, with cherry and peach accents. Very pleased.", u'title': u'Chehalem INOX Chardonnay', u'price': u'$12.99', u'id': 442371, u'link': u'https://www.wine.com/product/chehalem-inox-chardonnay-2009/109305', u'imageUrl': u'https://spoonacular.com/productImages/442371-312x231.jpg'}]}, u'aggregateLikes': 218, u'instructions': u"Preheat oven to 425 degreesLet chicken rest at room temperature for 30 minutes.Wash chicken and pat dry. Sprinkle the cavity of the chicken with salt and pepper and add garlic cloves.Roll lemon with your hand to make it juicy, cut it in half and add to the cavity of the chicken.Trust the chicken. To trust the chicken, cut about 3 feet of baking twine. Place the chicken so that breast side is up, and the legs are pointed towards you. Tuck the wing tips under the chicken. Wrap the string under the neck end of the bird, pulling the string ends up over the breast, toward you, plumping up the breast. Then cross the string under the breast. Wrap each end around the closest leg end, and tie tightly so that the legs come together.Rub butter over the chicken and sprinkle with STUBB'S chicken seasoning and a little ground pepper.In a heavy cast iron skillet, layer the bottom of the skillet with onions and carrots. Drizzle with olive oil and give it a mix.Top the onions and carrots with the chicken. Add potatoes around the side of the chicken.Place in oven and roast for 60 minutes or until the internal temperature reaches 160 degrees.Place the chicken on a cutting board and cover with aluminium foil. Let the chicken rest for 10 minutes, so that all the juices run back into the chicken. To keep your vegetables warm, turn off the oven and placing the skillet of vegetables back in the oven.Cut the chicken into pieces and serve with vegetables.", u'spoonacularSourceUrl': u'https://spoonacular.com/skillet-chicken-roast-511017', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 70, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}