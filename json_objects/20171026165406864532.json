{u'recipes': [{u'cuisines': [u'middl eastern'], u'image': u'https://spoonacular.com/recipeImages/721574-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 721574, u'veryPopular': True, u'lowFodmap': False, u'title': u'Crunchy Falafel', u'diets': [u'gluten free', u'dairy free', u'lacto ovo vegetarian', u'whole 30', u'vegan'], u'pricePerServing': 174.76, u'extendedIngredients': [{u'originalString': u'1 tsp baking soda', u'aisle': u'Baking', u'name': u'baking soda', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 18372, u'unit': u'tsp'}, {u'originalString': u'Organic rapeseed / canola oil to fry the falafel in', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'canola oil', u'metaInformation': [u'organic'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vegetable-oil.jpg', u'unitLong': u'serving', u'unitShort': u'serving', u'amount': 1.0, u'consistency': u'liquid', u'id': 4582, u'unit': u'serving'}, {u'originalString': u'1 tsp cumin', u'aisle': u'Spices and Seasonings', u'name': u'cumin', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-cumin.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1002014, u'unit': u'tsp'}, {u'originalString': u'1 handful of fresh coriander (optional)', u'aisle': u'Produce', u'name': u'fresh coriander', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cilantro.png', u'unitLong': u'handful', u'unitShort': u'handful', u'amount': 1.0, u'consistency': u'solid', u'id': 11165, u'unit': u'handful'}, {u'originalString': u'2 - 3 handful of fresh parsley', u'aisle': u'Produce', u'name': u'fresh parsley', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsley.jpg', u'unitLong': u'handfuls', u'unitShort': u'handful', u'amount': 2.0, u'consistency': u'solid', u'id': 11297, u'unit': u'handful'}, {u'originalString': u'2 minced garlic cloves', u'aisle': u'Produce', u'name': u'garlic cloves', u'metaInformation': [u'minced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 11215, u'unit': u''}, {u'originalString': u'1 tbsp lemon', u'aisle': u'Produce', u'name': u'lemon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 9150, u'unit': u'tbsp'}, {u'originalString': u'4 tbsp olive oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'olive oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 4.0, u'consistency': u'liquid', u'id': 4053, u'unit': u'tbsp'}, {u'originalString': u'1 onion', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u''}, {u'originalString': u'1/2 - 1 tbsp of salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 0.5, u'consistency': u'solid', u'id': 2047, u'unit': u'tbsp'}, {u'originalString': u'2 cups of chickpeas soaked in water overnight', u'aisle': u'Beverages', u'name': u'water', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 2.0, u'consistency': u'liquid', u'id': 14412, u'unit': u'cups'}], u'healthScore': 6.0, u'imageType': u'jpg', u'spoonacularScore': 60.0, u'sourceUrl': u'http://www.superhealthykids.com/crunchy-falafel/', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 23, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}], u'step': u"Drain and wash the uncooked chickpeas thoroughly and blend it well in a food processor with the rest of the ingredients until it turns into a coarse pastry. The texture of the chickpea mixture should be firm enough to easily hold its shape.You don't need to cook the chickpeas, that happens during the frying or baking process in the last step of the recipe.Form the falafel into small balls or flat mini burgers it's up to you. Just don't make it too thick in order for it to cook properly in the middle.", u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 375.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Heat the oil in a small or medium sized sauce pan and fry them until the surface becomes crispy (around 5 min). You can also bake the falafels for a healthier option.For a baking option - preheat the oven to 375 F while in the meantime you just place the falafels on a baking sheet, butter the falafels with some olive oil on both sides.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vegetable-oil.jpg', u'id': 4582, u'name': u'cooking oil'}]}, {u'equipment': [], u'step': u"Bake it for a total of 30-40 minutes, flipping once at the halfway through the cooking process. the longer you bake them the firmer they'll become.If the mixture becomes too loose add some breadcrumbs or wheat flour to the pastry. If the mixture becomes too firm add more olive oil.", u'length': {u'number': 40, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}], u'name': u''}], u'aggregateLikes': 5316, u'instructions': u"Drain and wash the uncooked chickpeas thoroughly and blend it well in a food processor with the rest of the ingredients until it turns into a coarse pastry. The texture of the chickpea mixture should be firm enough to easily hold its shape.You don't need to cook the chickpeas, that happens during the frying or baking process in the last step of the recipe.Form the falafel into small balls or flat mini burgers it's up to you. Just don't make it too thick in order for it to cook properly in the middle.Heat the oil in a small or medium sized sauce pan and fry them until the surface becomes crispy (around 5 min). You can also bake the falafels for a healthier option.For a baking option - preheat the oven to 375 F while in the meantime you just place the falafels on a baking sheet, butter the falafels with some olive oil on both sides. Bake it for a total of 30-40 minutes, flipping once at the halfway through the cooking process. the longer you bake them the firmer they'll become.If the mixture becomes too loose add some breadcrumbs or wheat flour to the pastry. If the mixture becomes too firm add more olive oil.", u'spoonacularSourceUrl': u'https://spoonacular.com/crunchy-falafel-721574', u'dishTypes': [], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 1, u'sustainable': False, u'dairyFree': True, u'whole30': True}]}