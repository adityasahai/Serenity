': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 1034053, u'unit': u'tablespoons'}, {u'originalString': u'3 tablespoons fresh parsley, chopped', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh parsley', u'metaInformation': [u'fresh', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsley.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 11297, u'unit': u'tablespoons'}, {u'originalString': u'1 tablespoon fresh garlic, minced', u'aisle': u'Produce', u'name': u'garlic', u'metaInformation': [u'fresh', u'minced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 11215, u'unit': u'tablespoon'}, {u'originalString': u'1 pound ground 80/20 beef', u'aisle': u'Meat', u'name': u'ground beef', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'liquid', u'id': 10023572, u'unit': u'pound'}, {u'originalString': u'1 pound ground pork', u'aisle': u'Meat', u'name': u'ground pork', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meat-ground.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 10219, u'unit': u'pound'}, {u'originalString': u'1 tablespoon Italian seasoning', u'aisle': u'Spices and Seasonings', u'name': u'italian seasoning', u'metaInformation': [u'italian'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/Herbes-de-Provence.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1022027, u'unit': u'tablespoon'}, {u'originalString': u'1 teaspoon kosher salt', u'aisle': u'Spices and Seasonings', u'name': u'kosher salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1082047, u'unit': u'teaspoon'}, {u'originalString': u'\xbc cup finely diced onion', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'diced', u'finely'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 11282, u'unit': u'cup'}, {u'originalString': u'1 cup onion diced fine', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'diced', u'fine'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u'cup'}, {u'originalString': u'\xbc cup freshly grated Parmesan cheese', u'aisle': u'Cheese', u'name': u'parmesan cheese', u'metaInformation': [u'freshly grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 1033, u'unit': u'cup'}, {u'originalString': u'Additional Parmesan cheese for serving', u'aisle': u'Cheese', u'name': u'parmesan cheese', u'metaInformation': [u'for serving'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 8.0, u'consistency': u'solid', u'id': 1033, u'unit': u'servings'}, {u'originalString': u'\u215b pound prosciutto, diced', u'aisle': u'Meat', u'name': u'prosciutto', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/proscuitto.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 0.125, u'consistency': u'solid', u'id': 10010123, u'unit': u'pound'}, {u'originalString': u'\u215b pound provolone, diced', u'aisle': u'Cheese', u'name': u'provolone', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/provolone-cheese.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 0.125, u'consistency': u'solid', u'id': 1035, u'unit': u'pound'}, {u'originalString': u'\u215b pound Genoa salami, diced', u'aisle': u'Meat', u'name': u'salami', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salami.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 0.125, u'consistency': u'solid', u'id': 7071, u'unit': u'pound'}, {u'originalString': u'1 teaspoon Italian seasoning blend (or half dried basil and half dried oregano)', u'aisle': u'Spices and Seasonings', u'name': u'seasoning blend', u'metaInformation': [u'dried', u'italian', u'(or half basil and half oregano)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spices.png', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1032027, u'unit': u'teaspoon'}, {u'originalString': u'\xbc pound fresh mozzarella, shredded or diced', u'aisle': u'Cheese', u'name': u'shredded mozzarella', u'metaInformation': [u'fresh', u'diced', u'shredded'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shredded-cheese-white.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 0.25, u'consistency': u'solid', u'id': 1001026, u'unit': u'pound'}, {u'originalString': u'1 quart of Italian Tomato Sauce (or your own favorite sauce) for serving, plus more if serving with pasta', u'aisle': u'Canned and Jarred', u'name': u'tomato sauce', u'metaInformation': [u'italian', u'with pasta', u'plus more if serving ', u'for serving, ', u'(or your own favorite sauce)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato-sauce-or-pasta-sauce.jpg', u'unitLong': u'quart', u'unitShort': u'quart', u'amount': 1.0, u'consistency': u'solid', u'id': 11549, u'unit': u'quart'}, {u'originalString': u'2 whole eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'whole eggs', u'metaInformation': [u'whole'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'\xbc cup whole milk', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'whole milk', u'metaInformation': [u'whole'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'liquid', u'id': 1077, u'unit': u'cup'}, {u'originalString': u'2 slices white or soft wheat bread, crusts removed', u'aisle': u'Bakery/Bread', u'name': u'whole wheat bread', u'metaInformation': [u'white', u'soft'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bread-whole-wheat.jpg', u'unitLong': u'slices', u'unitShort': u'slices', u'amount': 2.0, u'consistency': u'solid', u'id': 18075, u'unit': u'slices'}], u'healthScore': 25.0, u'imageType': u'jpg', u'spoonacularScore': 81.0, u'sourceUrl': u'http://www.afamilyfeast.com/stuffed-meatballs/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 23, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'To make the filling, in a medium saut pan, heat oil and saut onions with Italian seasonings over medium heat. After five minutes add prosciutto and cook for another three minutes.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/Herbes-de-Provence.jpg', u'id': 1022027, u'name': u'italian seasoning'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/proscuitto.jpg', u'id': 10010123, u'name': u'prosciutto'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [], u'step': u'Add spinach and cook for two more minutes.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Remove from heat and let cool.In a medium bowl, place capicola, salami, provolone, mozzarella and Parmesan cheese.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mozzarella-fresh.jpg', u'id': 1026, u'name': u'mozzarella'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/provolone-slices.jpg', u'id': 1035, u'name': u'provolone'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pancetta.jpg', u'id': 93679, u'name': u'capicola'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salami.jpg', u'id': 7071, u'name': u'salami'}]}, {u'equipment': [], u'step': u'Mix to combine.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Add cooled mixture from pan to meat and cheese and stir to combine. Do not clean the saut pan yet.Cover and refrigerate filling until a later step.To make meatballs, place bread in a large bowl and cover with milk, making sure bread is completely covered and soaks up the milk.In the same saut pan as before, over medium heat place olive oil and add onions. Cook for five minutes then add garlic and cook for one more minute.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meatballs.jpg', u'id': 10110219, u'name': u'meatballs'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [], u'step': u'Remove from heat to cool.', u'number': 6, u'ingredients': []}, {u'equipment': [], u'step': u'Add the whole eggs to the soaked bread and stir vigorously to combine.', u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [], u'step': u'Add Italian seasoning, parsley, salt, pepper and Parmesan cheese and again mix to combine all ingredients.', u'number': 8, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/Herbes-de-Provence.jpg', u'id': 1022027, u'name': u'italian seasoning'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsley.jpg', u'id': 11297, u'name': u'parsley'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'id': 1002030, u'name': u'pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/cutting-board.jpg', u'id': 404716, u'name': u'cutting board'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Add ground beef and pork and gently combine to incorporate but do not over mix.Divide this mixture into eight equal portions.Line a small sheet tray with parchment paper and set aside.Fill a bowl with water to wet your hands as you work. Also have a spatula and cutting board standing by.', u'number': 9, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'id': 10023572, u'name': u'ground beef'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pork-tenderloin-raw.png', u'id': 10010219, u'name': u'pork'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/cutting-board.jpg', u'id': 404716, u'name': u'cutting board'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}], u'step': u'Remove the filling from refrigeration and divide that into eight portions.Take one portion of meatball mix and with wet hands, roll into a ball then place on a cutting board and press down into the center. Compress a portion of filling in your hands and place on the meat. Use the spatula to lift it from the board to your hand and start pressing the sides up around the filling to the top, pinching closed at the top.', u'number': 10, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meatballs.jpg', u'id': 10110219, u'name': u'meatballs'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 425.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Place this pinched side down on the sheet tray.Repeat for the remaining meatballs.Cover and refrigerate for 30 minutes.Preheat oven to 425 degrees F. Once the oven is up to temperature, pull the tray from refrigeration and spray the tops and sides of each meatball with a kitchen pan spray or brush with oil and then place directly into the oven.', u'length': {u'number': 30, u'unit': u'minutes'}, u'number': 11, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meatballs.jpg', u'id': 10110219, u'name': u'meatballs'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404794, u'name': u'stove'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Bake for 20 minutes uncovered.While the meatballs are cooking, place a large skillet on a stove burner and heat up your tomato sauce to hot.When the meat balls come out of the oven, use a spatula and place all eight into the sauce, covering the tops with sauce. Cover the skillet and cook on a low simmer for 15 minutes. Discard any fat that collects on the sheet pan.', u'length': {u'number': 35, u'unit': u'minutes'}, u'number': 12, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato-sauce-or-pasta-sauce.jpg', u'id': 11549, u'name': u'tomato sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meatballs.jpg', u'id': 10110219, u'name': u'meatballs'}]}, {u'equipment': [], u'step': u'Remove meatballs from sauce after 15 minutes and serve with additional Parmesan cheese..', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 13, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/meatballs.jpg', u'id': 10110219, u'name': u'meatballs'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 45, u'instructions': u'To make the filling, in a medium saut pan, heat oil and saut onions with Italian seasonings over medium heat. After five minutes add prosciutto and cook for another three minutes. Add spinach and cook for two more minutes. Remove from heat and let cool.In a medium bowl, place capicola, salami, provolone, mozzarella and Parmesan cheese. Mix to combine.Add cooled mixture from pan to meat and cheese and stir to combine. Do not clean the saut pan yet.Cover and refrigerate filling until a later step.To make meatballs, place bread in a large bowl and cover with milk, making sure bread is completely covered and soaks up the milk.In the same saut pan as before, over medium heat place olive oil and add onions. Cook for five minutes then add garlic and cook for one more minute. Remove from heat to cool.Add the whole eggs to the soaked bread and stir vigorously to combine. Add Italian seasoning, parsley, salt, pepper and Parmesan cheese and again mix to combine all ingredients.Add ground beef and pork and gently combine to incorporate but do not over mix.Divide this mixture into eight equal portions.Line a small sheet tray with parchment paper and set aside.Fill a bowl with water to wet your hands as you work. Also have a spatula and cutting board standing by.Remove the filling from refrigeration and divide that into eight portions.Take one portion of meatball mix and with wet hands, roll into a ball then place on a cutting board and press down into the center. Compress a portion of filling in your hands and place on the meat. Use the spatula to lift it from the board to your hand and start pressing the sides up around the filling to the top, pinching closed at the top.Place this pinched side down on the sheet tray.Repeat for the remaining meatballs.Cover and refrigerate for 30 minutes.Preheat oven to 425 degrees F. Once the oven is up to temperature, pull the tray from refrigeration and spray the tops and sides of each meatball with a kitchen pan spray or brush with oil and then place directly into the oven. Bake for 20 minutes uncovered.While the meatballs are cooking, place a large skillet on a stove burner and heat up your tomato sauce to hot.When the meat balls come out of the oven, use a spatula and place all eight into the sauce, covering the tops with sauce. Cover the skillet and cook on a low simmer for 15 minutes. Discard any fat that collects on the sheet pan.Remove meatballs from sauce after 15 minutes and serve with additional Parmesan cheese..', u'spoonacularSourceUrl': u'https://spoonacular.com/stuffed-meatballs-711236', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 110, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}