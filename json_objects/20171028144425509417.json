hy': False, u'weightWatcherSmartPoints': 17, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'In a large bowl, mix the whipped topping, sour cream, and pudding mix until blended. Fold in pineapple. Fill a Ziploc bag with pudding mixture and cut off the corner of the bag.On a flat serving plate or large baking dish arrange four graham crackers in a rectangle. Pipe about 1 cup of the pudding mixture over the crackers; top with about 1/4 cup banana slices. Repeat layers five times. Cover and refrigerate for at least 8 hours or overnight. Graham crackers should be soft.Before serving, make the chocolate drizzle. For the Chocolate', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/graham-crackers.jpg', u'id': 18617, u'name': u'graham crackers'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whipped-cream.jpg', u'id': 42135, u'name': u'whipped topping'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'id': 1056, u'name': u'sour cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bananas.jpg', u'id': 9040, u'name': u'banana'}]}], u'name': u''}, {u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}], u'step': u'Heat the cream in the microwave until hot (about 20-30 seconds).', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place chocolate chips in a small bowl and pour hot cream over the chips.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}]}, {u'equipment': [], u'step': u'Let it sit for a second and then stir until chocolate is smooth.', u'number': 3, u'ingredients': []}, {u'equipment': [], u'step': u'Let cool slightly while preparing the bananas and strawberries for the topping.Arrange sliced fruit on top.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bananas.jpg', u'id': 9040, u'name': u'banana'}]}, {u'equipment': [], u'step': u'Drizzle with chocolate and serve.', u'number': 5, u'ingredients': []}], u'name': u'Drizzle'}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are great choices for Cake. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. You could try NV Johnson Estate Cream Sherry. Reviewers quite like it with a 5 out of 5 star rating and a price of about 19 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'oak , vanilla , hazelnut', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 27397, u'instructions': u'In a large bowl, mix the whipped topping, sour cream, and pudding mix until blended. Fold in pineapple. Fill a Ziploc bag with pudding mixture and cut off the corner of the bag.On a flat serving plate or large baking dish arrange four graham crackers in a rectangle. Pipe about 1 cup of the pudding mixture over the crackers; top with about 1/4 cup banana slices. Repeat layers five times. Cover and refrigerate for at least 8 hours or overnight. Graham crackers should be soft.Before serving, make the chocolate drizzle. For the Chocolate Drizzle: Heat the cream in the microwave until hot (about 20-30 seconds). Place chocolate chips in a small bowl and pour hot cream over the chips. Let it sit for a second and then stir until chocolate is smooth. Let cool slightly while preparing the bananas and strawberries for the topping.Arrange sliced fruit on top. Drizzle with chocolate and serve.', u'spoonacularSourceUrl': u'https://spoonacular.com/banana-split-icebox-cake-489096', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 10, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}