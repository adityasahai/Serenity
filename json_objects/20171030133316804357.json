kes 36 Cookies', u'spoonacularSourceUrl': u'https://spoonacular.com/starbucks-via-double-chocolate-cookies-260956', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 22, u'servings': 36, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}