mageType': u'jpeg', u'spoonacularScore': 5.0, u'sourceUrl': u'http://www.foodnetwork.com/recipes/food-network-kitchens/chocolate-cream-sandwich-cookies.html', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 3, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/plastic-wrap.jpg', u'id': 404730, u'name': u'plastic wrap'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Whisk the flour, 1/4 cup cocoa powder, the baking powder and salt in a bowl. Beat 1 stick butter and the granulated sugar in a large bowl with a mixer on medium speed until light and fluffy, 3 to 5 minutes. Beat in the egg and 1/2 teaspoon vanilla. Reduce the mixer speed to medium low; beat in the flour mixture until just incorporated. Divide the dough between 2 sheets of plastic wrap; form into flat rectangles. Wrap and refrigerate until firm, at least 1 hour.', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'granulated sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 18371, u'name': u'baking powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Position racks in the upper and lower thirds of the oven; preheat to 350 degrees F. Line 2 baking sheets with parchment. Working with 1 piece at a time, roll out the dough into a 10-inch square (1/8 inch thick) on a lightly floured surface.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Cut into 1-by-2 1/2-inch rectangles.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Transfer to the prepared baking sheets; prick with a fork and sprinkle with granulated sugar. Chill until firm, 15 minutes.', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'granulated sugar'}]}, {u'equipment': [], u'step': u'Bake, switching the pans halfway through, until the cookies are firm, 13 to 16 minutes.', u'length': {u'number': 13, u'unit': u'minutes'}, u'number': 5, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Let cool completely on the baking sheets.', u'number': 6, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Put the chocolate in a microwave-safe bowl; microwave in 30-second intervals, stirring, until smooth; let cool.', u'number': 7, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}], u'step': u'Add the remaining 1/2 stick butter; beat with a rubber spatula until incorporated.', u'number': 8, u'ingredients': []}, {u'equipment': [], u'step': u"Mix in the remaining 1 tablespoon cocoa powder, the confectioners' sugar and the remaining 1/2 teaspoon vanilla until smooth. Sandwich between the cookies.", u'number': 9, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'id': 19336, u'name': u'powdered sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}]}, {u'equipment': [], u'step': u'Photograph by Levi Brown', u'number': 10, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies works really well with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. You could try NV Solera Cream Sherry. Reviewers quite like it with a 4.5 out of 5 star rating and a price of about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 50, u'instructions': u"Whisk the flour, 1/4 cup cocoa powder, the baking powder and salt in a bowl. Beat 1 stick butter and the granulated sugar in a large bowl with a mixer on medium speed until light and fluffy, 3 to 5 minutes. Beat in the egg and 1/2 teaspoon vanilla. Reduce the mixer speed to medium low; beat in the flour mixture until just incorporated. Divide the dough between 2 sheets of plastic wrap; form into flat rectangles. Wrap and refrigerate until firm, at least 1 hour.                          Position racks in the upper and lower thirds of the oven; preheat to 350 degrees F. Line 2 baking sheets with parchment. Working with 1 piece at a time, roll out the dough into a 10-inch square (1/8 inch thick) on a lightly floured surface. Cut into 1-by-2 1/2-inch rectangles. Transfer to the prepared baking sheets; prick with a fork and sprinkle with granulated sugar. Chill until firm, 15 minutes. Bake, switching the pans halfway through, until the cookies are firm, 13 to 16 minutes. Let cool completely on the baking sheets.                          Put the chocolate in a microwave-safe bowl; microwave in 30-second intervals, stirring, until smooth; let cool. Add the remaining 1/2 stick butter; beat with a rubber spatula until incorporated. Mix in the remaining 1 tablespoon cocoa powder, the confectioners' sugar and the remaining 1/2 teaspoon vanilla until smooth. Sandwich between the cookies.                          Photograph by Levi Brown", u'spoonacularSourceUrl': u'https://spoonacular.com/chocolate-cream-sandwich-cookies-738492', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 150, u'servings': 40, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}