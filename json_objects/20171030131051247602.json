{u'recipes': [{u'sourceName': u'Slender Kitchen', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/510345-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 510345, u'veryPopular': False, u'lowFodmap': False, u'title': u'Slow Cooker Sunday: Chicken Barbacoa', u'diets': [u'gluten free', u'dairy free', u'whole 30'], u'pricePerServing': 314.75, u'creditText': u'Slender Kitchen', u'extendedIngredients': [{u'originalString': u'1/2-1 7 oz. can of chipotles in adobo sauce', u'aisle': u'Ethnic Foods', u'name': u'adobo sauce', u'metaInformation': [u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/harissa.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 27.0, u'consistency': u'solid', u'id': 6979, u'unit': u''}, {u'originalString': u'2 lbs. chicken breast', u'aisle': u'Meat', u'name': u'chicken breast', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chicken-breasts.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'solid', u'id': 5062, u'unit': u'lbs'}, {u'originalString': u'1/2 cup chicken broth', u'aisle': u'Canned and Jarred', u'name': u'chicken broth', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chicken-broth.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'liquid', u'id': 6194, u'unit': u'cup'}, {u'originalString': u'1/2 tbsp. cumin', u'aisle': u'Spices and Seasonings', u'name': u'cumin', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-cumin.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 0.5, u'consistency': u'solid', u'id': 1002014, u'unit': u'tbsp'}, {u'originalString': u'1 onion, quartered', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'quartered'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u's