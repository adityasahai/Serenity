zedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'In a small bowl, combine flour and salt; cut in the shortening until crumbly.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shortening.jpg', u'id': 4615, u'name': u'shortening'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [], u'step': u'Add vanilla. Gradually add water, tossing with a fork until dough forms a ball.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'id': 14412, u'name': u'water'}]}, {u'equipment': [], u'step': u'Roll out pastry to fit a 9-in. pie plate.', u'number': 3, u'ingredients': []}, {u'equipment': [], u'step': u'Transfer pastry to pie plate. Trim pastry to 1/2 in. beyond edge of pie plate; flute edges.', u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'For filling, combine the sugar, flour and cinnamon; sprinkle 3 tablespoons into crust. Layer with half of the apples; sprinkle with half of the remaining sugar mixture. Repeat layers.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'id': 2010, u'name': u'cinnamon'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'id': 9003, u'name': u'apple'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u'Pour cream over all.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/wire-rack.jpg', u'id': 405900, u'name': u'wire rack'}], u'step': u'Bake at 450\xb0 for 10 minutes. Reduce heat to 350\xb0 bake for 55-60 minutes or until apples are tender. Cool on a wire rack. Store in the refrigerator.', u'length': {u'number': 70, u'unit': u'minutes'}, u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'id': 9003, u'name': u'apple'}]}, {u'equipment': [], u'step': u'Serve with whipped cream if desired.', u'number': 8, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whipped-cream.jpg', u'id': 1054, u'name': u'whipped cream'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Lambrusco Dolce, Late Harvest Riesling, and Vin Santo are my top picks for Pie. These wines are all sweet, which is important since wine should usually be sweeter than the food you're pairing with it. The Vineyards Late Harvest Riesling with a 5 out of 5 star rating seems like a good match. It costs about 25 dollars per bottle.", u'pairedWines': [u'lambrusco dolce', u'late harvest riesling', u'vin santo'], u'productMatches': [{u'description': u'Pale yellow in color, the nose opens with aromas of fresh pineapple, juicy nectarine and citrus notes. The wine fully coats the palate expressing vibrant flavors of orange blossom, tart granny smith apple and honeycomb. A long finish displays the perfect balance of sweetness and mouth-watering acidity, inviting another sip.', u'title': u'Vineyards Late Harvest Riesling', u'price': u'$25.0', u'id': 431536, u'link': u'https://www.amazon.com/Willamette-Valley-Vineyards-Harvest-Riesling/dp/B00X4IQOV4', u'imageUrl': u'https://spoonacular.com/productImages/431536-312x231.jpg'}]}, u'aggregateLikes': 594, u'instructions': u'Directions                                                                                In a small bowl, combine flour and salt; cut in the shortening until crumbly. Add vanilla. Gradually add water, tossing with a fork until dough forms a ball. Roll out pastry to fit a 9-in. pie plate. Transfer pastry to pie plate. Trim pastry to 1/2 in. beyond edge of pie plate; flute edges.                                                                                           For filling, combine the sugar, flour and cinnamon; sprinkle 3 tablespoons into crust. Layer with half of the apples; sprinkle with half of the remaining sugar mixture. Repeat layers. Pour cream over all.                                                                                            Bake at 450\xb0 for 10 minutes. Reduce heat to 350\xb0 bake for 55-60 minutes or until apples are tender. Cool on a wire rack. Store in the refrigerator. Serve with whipped cream if desired.                                        Yield: 8 servings.                                                                                                                         Originally published as German Apple Pie in  CountryAugust/September 1994, p49                                                                                                                                                                                                        Nutritional Facts                                                                             1 piece equals 459 calories, 24 g fat (10 g saturated fat), 41 mg cholesterol, 160 mg sodium, 59 g carbohydrate, 3 g fiber, 4 g protein.                                                                                                                                                                     Print                                                                                    Add to Recipe Box                            Email a Friend', u'spoonacularSourceUrl': u'https://spoonacular.com/german-apple-pie-389623', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 85, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}