{u'recipes': [{u'sourceName': u'Crazy for Crust', u'cuisines': [u'mediterranean', u'european', u'french'], u'image': u'https://spoonacular.com/recipeImages/720812-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 720812, u'veryPopular': False, u'lowFodmap': False, u'title': u'No Bake Creme Brulee Pie', u'diets': [u'lacto ovo vegetarian'], u'pricePerServing': 29.54, u'creditText': u'Crazy for Crust', u'extendedIngredients': [{u'originalString': u'1 (9\u201d) Shortbread Crust, Graham Cracker Crust, or Golden Oreo Crust', u'aisle': u'Sweet Snacks', u'name': u'golden oreo', u'metaInformation': [u'(9\u201d)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-sandwich-cookies.png', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 10.0, u'consistency': u'solid', u'id': 10018210, u'unit': u'servings'}, {u'originalString': u'3/4 cup heavy whipping cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'heavy whipping cream', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.75, u'consistency': u'liquid', u'id': 1053, u'unit': u'cup'}, {u'originalString': u'1 (approximately 3.4 ounce) box instant vanilla pudding', u'aisle': u'Baking', u'name': u'instant vanilla pudding', u'metaInformation': [u'(approximately 3.4 ounce)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/instant-vanilla-pudding.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 19206, u'unit': u''}, {u'originalString': u'1 cup nonfat milk', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'nonfat milk', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'liquid', u'id': 1085, u'unit': u'cup'}, {u'originalString': u'Sugar, for topping', u'aisle': u'Baking', u'name': u'sugar', u'metaInformation': [u'for topping'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 10.0, u'consistency': u'solid', u'id': 19335, u'unit': u'servings'}, {u'originalString': u'1 tablespoon vanilla bean paste (extract may be substituted)', u'aisle': u'Gourmet', u'name': u'vanilla bean paste', u'metaInformation': [u'(extract may be substituted)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/molasses.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 93813, u'unit': u'tablespoon'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 13.0, u'sourceUrl': u'http://www.crazyforcrust.com/2016/03/no-bake-creme-brulee-pie-recipe/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 6, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Whisk pudding with whipping cream and milk for 2 minutes or until smooth. Stir in vanilla bean paste.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/molasses.jpg', u'id': 93813, u'name': u'vanilla paste'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1001053, u'name': u'whipping cream'}]}, {u'equipment': [], u'step': u'Let sit for 5 minutes.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}], u'name': u''}, {u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}], u'step': u'Add 1/4 cup granulated sugar to a small saucepan.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'granulated sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/wooden-spoon.jpg', u'id': 404732, u'name': u'wooden spoon'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Place over low heat and do not touch it, but watch it closely. Once it starts to turn to liquid, stir with a wooden spoon, being careful not to get it on the sides of the pan, until the sugar turns to crystals then to liquid again, and turns a light amber color.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u'Drizzle some onto each slice of pie. Note: this sugar hardens more than it does with the torch. Err on the light side of adding it to the top and be cautious eating it.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}], u'name': u'Spread pudding into the bottom of prepared pie crust. Chill for at least 4 hours before serving.Note on brulee its best to brle slices instead of the entire pie. Its harder to slice once it has the brle on top and it must be eaten immediately. Unless you plan to serve the entire pie immediately, slice, then continue.Sprinkle each slice with sugar and use a kitchen torch to brle the top.Alternate brle no-torch no-oven method'}], u'winePairing': {u'pairingText': u"Lambrusco Dolce, Late Harvest Riesling, and Vin Santo are great choices for Pie. These wines are all sweet, which is important since wine should usually be sweeter than the food you're pairing with it. You could try Vineyards Late Harvest Riesling. Reviewers quite like it with a 5 out of 5 star rating and a price of about 25 dollars per bottle.", u'pairedWines': [u'lambrusco dolce', u'late harvest riesling', u'vin santo'], u'productMatches': [{u'description': u'Pale yellow in color, the nose opens with aromas of fresh pineapple, juicy nectarine and citrus notes. The wine fully coats the palate expressing vibrant flavors of orange blossom, tart granny smith apple and honeycomb. A long finish displays the perfect balance of sweetness and mouth-watering acidity, inviting another sip.', u'title': u'Vineyards Late Harvest Riesling', u'price': u'$25.0', u'id': 431536, u'link': u'https://www.amazon.com/Willamette-Valley-Vineyards-Harvest-Riesling/dp/B00X4IQOV4', u'imageUrl': u'https://spoonacular.com/productImages/431536-312x231.jpg'}]}, u'aggregateLikes': 315, u'instructions': u'Whisk pudding with whipping cream and milk for 2 minutes or until smooth. Stir in vanilla bean paste. Let sit for 5 minutes.Spread pudding into the bottom of prepared pie crust. Chill for at least 4 hours before serving.Note on brulee: its best to brle slices instead of the entire pie. Its harder to slice once it has the brle on top and it must be eaten immediately. Unless you plan to serve the entire pie immediately, slice, then continue.Sprinkle each slice with sugar and use a kitchen torch to brle the top.Alternate brle no-torch no-oven method: Add 1/4 cup granulated sugar to a small saucepan. Place over low heat and do not touch it, but watch it closely. Once it starts to turn to liquid, stir with a wooden spoon, being careful not to get it on the sides of the pan, until the sugar turns to crystals then to liquid again, and turns a light amber color. Drizzle some onto each slice of pie. Note: this sugar hardens more than it does with the torch. Err on the light side of adding it to the top and be cautious eating it.', u'spoonacularSourceUrl': u'https://spoonacular.com/no-bake-creme-brulee-pie-720812', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 15, u'servings': 10, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}