ent': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Heat oven to 350F. On well-floured surface, roll dough 1/4 inch thick.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/cookie-cutters.jpg', u'id': 221429, u'name': u'cookie cutter'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Cut with 3- to 3 1/2-inch heart-shaped cookie cutter. On ungreased cookie sheet, place cutouts 2 inches apart.', u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'3', u'number': 6, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/wire-rack.jpg', u'id': 405900, u'name': u'wire rack'}], u'step': u'Bake 8 to 10 minutes or until edges are light golden brown. Cool 1 minute; remove from cookie sheet to cooling rack. Cool completely.', u'length': {u'number': 9, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}, {u'equipment': [], u'step': u'4', u'number': 8, u'ingredients': []}, {u'equipment': [], u'step': u'Turn cookies so pointed ends are up. Using desired tip, pipe frosting on upper 1/3 of cookie.', u'number': 9, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/frosting-or-icing.png', u'id': 19230, u'name': u'frosting'}]}, {u'equipment': [], u'step': u'Place 1 marshmallow half at tip of each cookie for hat; sprinkle with red sugar. Using small dots of frosting, attach 2 blue candies on each cookie for eyes. Pipe small frosting mustache below eyes; place red candy in center for mouth. Pipe frosting on lower 1/3 of each cookie for beard. Store in single layer, loosely covered.', u'number': 10, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/normal-marshmallows.jpg', u'id': 19116, u'name': u'marshmallows'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/frosting-or-icing.png', u'id': 19230, u'name': u'frosting'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Madeira, and Prosecco are great choices for Cookies. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. One wine you could try is NV Johnson Estate Cream Sherry. It has 5 out of 5 stars and a bottle costs about 19 dollars.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'Very aromatic with notes of hazelnut, vanilla, and a touch of oak followed by sweet raisins and a touch of yeast. Clean lasting finish. Good now but will reward those allow it to age"". A favorite pre-prandial beverage. Consider it with nuts before dinner as an aperitif, or after dinner with dessert, especially chocolates and fruit-based desserts. Also wonderful on cold afternoons, served with biscotti to dip in ""Italian-style"". "', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 6, u'instructions': u'1                               In large bowl, beat butter, brown sugar and vanilla with electric mixer on medium speed until creamy. Stir in flour, oats and baking powder. Gather dough into ball. Wrap in plastic wrap; refrigerate 30 minutes.                                                          2                               Heat oven to 350F. On well-floured surface, roll dough 1/4 inch thick. Cut with 3- to 3 1/2-inch heart-shaped cookie cutter. On ungreased cookie sheet, place cutouts 2 inches apart.                                                          3                               Bake 8 to 10 minutes or until edges are light golden brown. Cool 1 minute; remove from cookie sheet to cooling rack. Cool completely.                                                          4                               Turn cookies so pointed ends are up. Using desired tip, pipe frosting on upper 1/3 of cookie. Place 1 marshmallow half at tip of each cookie for hat; sprinkle with red sugar. Using small dots of frosting, attach 2 blue candies on each cookie for eyes. Pipe small frosting mustache below eyes; place red candy in center for mouth. Pipe frosting on lower 1/3 of each cookie for beard. Store in single layer, loosely covered.', u'spoonacularSourceUrl': u'https://spoonacular.com/santa-face-shortbread-cookies-622793', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 100, u'servings': 12, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}