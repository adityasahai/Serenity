{u'recipes': [{u'sourceName': u'Mels Kitchen Caf\xe9', u'cuisines': [u'mexican'], u'image': u'https://spoonacular.com/recipeImages/568626-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 568626, u'veryPopular': False, u'lowFodmap': False, u'title': u'Skillet Chicken with Mexican Green Rice', u'diets': [u'gluten free', u'dairy free'], u'pricePerServing': 354.9, u'creditText': u'Mels Kitchen Caf\xe9', u'extendedIngredients': [{u'originalString': u'2 tablespoons canola oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'canola oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vegetable-oil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 4582, u'unit': u'tablespoons'}, {u'originalString': u'1/2 cup chopped fresh cilantro', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh cilantro', u'metaInformation': [u'fresh', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cilantro.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 11165, u'unit': u'cup'}, {u'originalString': u'3 garlic cloves, finely minced', u'aisle': u'Produce', u'name': u'garlic cloves', u'metaInformation': [u'finely minced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11215, u'unit': u''}, {u'originalString': u"2 (10 oz.) cans green enchilada sauce (I recommend mild for those who don't want much heat)", u'aisle': u'Pasta and Rice;Ethnic Foods', u'name': u'green enchilada sauce', u'metaInformation': [u'green', u"for those who don't want much heat)", u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salsa-verde.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 20.0, u'consistency': u'solid', u'id': 6599, u'unit': u'oz'}, {u'originalString': u'1 teaspoon ground cumin', u'ai