metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'Tsp', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'Tsp'}], u'healthScore': 3.0, u'imageType': u'jpg', u'spoonacularScore': 50.0, u'sourceUrl': u'http://www.averiecooks.com/2009/10/raw-vegan-chocolate-chip-cookie-dough.html', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 1, u'analyzedInstructions': [{u'steps': [{u'equipment': [], u'step': u'Blend the cashews and oats in a Vita-', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cashews.jpg', u'id': 12087, u'name': u'cashews'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rolled-oats.jpg', u'id': 8120, u'name': u'oats'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}], u'step': u"Mix, food processor, or high speed blender until they're a fine powder. Don't overblend or you'll wind up with cashew butter very quickly.", u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u"Add the agave, maple, vanilla and blend until incorporated. Be careful to just blend in short bursts and only until combined; don't over-process. Stir in the chocolate chips by hand. Form into balls and serve. If dough is sticky, chilling it in the refrigerator or flash-chilling it in the freezer helps make it easier to work with.", u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/maple-syrup-or-agave-nectar.jpg', u'id': 19912, u'name': u'agave'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/maple-syrup-or-agave-nectar.jpg', u'id': 19911, u'name': u'maple syrup'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies works really well with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'nutty , butterscotch , caramel', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 125746, u'instructions': u"Blend the cashews and oats in a Vita-Mix, food processor, or high speed blender until they're a fine powder. Don't overblend or you'll wind up with cashew butter very quickly. Add the agave, maple, vanilla and blend until incorporated. Be careful to just blend in short bursts and only until combined; don't over-process. Stir in the chocolate chips by hand. Form into balls and serve. If dough is sticky, chilling it in the refrigerator or flash-chilling it in the freezer helps make it easier to work with.", u'spoonacularSourceUrl': u'https://spoonacular.com/raw-vegan-chocolate-chip-cookie-dough-bites-gluten-free-soy-free-520652', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 17, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}