r.", u'spoonacularSourceUrl': u'https://spoonacular.com/nutella-cream-cheese-crinkle-cookies-623144', u'dishTypes': [], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 273, u'servings': 30, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}