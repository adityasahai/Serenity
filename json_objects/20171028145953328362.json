{u'recipes': [{u'sourceName': u'Weary Chef', u'cuisines': [u'mexican'], u'image': u'https://spoonacular.com/recipeImages/723844-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 600, u'id': 723844, u'veryPopular': False, u'lowFodmap': False, u'title': u'Crockpot Pork Roast Burrito Bowls', u'diets': [u'gluten free', u'dairy free'], u'preparationMinutes': 5, u'pricePerServing': 294.67, u'creditText': u'Weary Chef', u'extendedIngredients': [{u'originalString': u'3 cans seasoned chili beans', u'aisle': u'Canned and Jarred', u'name': u'chili beans', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/kidney-beans.jpg', u'unitLong': u'cans', u'unitShort': u'cans', u'amount': 3.0, u'consistency': u'solid', u'id': 43112, u'unit': u'cans'}, {u'originalString': u'1 Smithfield Boneless Netted Pork Shoulder Blade Roast', u'aisle': u'Meat', u'name': u'pork shoulder roast', u'metaInformation': [u'boneless'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pork-shoulder.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 10072, u'unit': u''}, {u'originalString': u'15 ounces red enchilada sauce', u'aisle': u'Pasta and Rice;Ethnic Foods', u'name': u'red enchilada sauce', u'metaInformation': [u'red'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salsa-verde.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 15.0, u'consistency': u'solid', u'id': 6599, u'unit': u'ounces'}, {u'originalString': u'cooked rice optional', u'aisle': u'Pasta and Rice', u'name': u'rice', u'metaInformation': [u'cooked'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rice-white-uncooked.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 8.0, u'consistency': u'solid', u'id': 20444, u'unit': u'servings'}, {u'originalString': u'salad greens', u'aisle': u'Produce', u'name': u'salad greens', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mixed-greens-or-mesclun.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 8.0, u'consistency': u'solid', u'id': 21052, u'unit': u'servings'}, {u'originalString': u'diced tomatoes, shredded cheese, sour cream, salsa or other desired toppings', u'aisle': u'Produce', u'name': u'tomatoes', u'metaInformation': [u'diced', u'shredded', u'sour'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 8.0, u'consistency': u'solid', u'id': 11529, u'unit': u'servings'}, {u'originalString': u'1 yellow onion sliced', u'aisle': u'Produce', u'name': u'yellow onion', u'metaInformation': [u'yellow', u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 10511282, u'unit': u''}], u'healthScore': 59.0, u'imageType': u'jpg', u'spoonacularScore': 97.0, u'sourceUrl': u'http://wearychef.com/crockpot-pork-roast-burrito-bowls/', u'glutenFree': True, u'vegan': False, u'veryHealthy': True, u'weightWatcherSmartPoints': 10, u'analyzedInstructions': [{u'steps': [{u'equipment': [], u'step': u'Remove and discard net from pork roast.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/cutting-board.jpg', u'id': 404716, u'name': u'cutting board'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/slow-cooker.jpg', u'id': 404718, u'name': u'slow cooker'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/colander.jpg', u'id': 404639, u'name': u'colander'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place in crock of slow cooker. Arrange onion over the meat, and pour beans and enchilada sauce over the roast. Cover and cook on low for 8-10 hours. Flip roast over halfway through cooking if possible.When ready to serve, remove cooked roast from crockpot to a large cutting board. Carefully pour remaining contents of slow cooker into a colander to drain off the liquid. Shred cooked meat with two forks, removing and discarding large pieces of fat as you work. Return shredded meat and drained beans and onions to cooking vessel or a serving bowl.To assemble burrito bowls, place lettuce in the bottom of a large salad bowl.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/harissa.jpg', u'id': 6599, u'name': u'enchilada sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [], u'step': u'Place warm, cooked rice over the lettuce (optional), and place a generous helping of pork and beans over the rice. Pile on your desired toppings, and dig in!', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rice-white-uncooked.jpg', u'id': 20444, u'name': u'rice'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'Mexican can be paired with Pinot Noir, Riesling, and Sparkling ros\xe9. Acidic white wines like riesling or low-tannin reds like pinot noir can work well with Mexican dishes. Sparkling ros\xe9 is a safe pairing too. You could try Mark West Black Label Monterey County Pinot Noir. Reviewers quite like it with a 4.1 out of 5 star rating and a price of about 13 dollars per bottle.', u'pairedWines': [u'pinot noir', u'riesling', u'sparkling rose'], u'productMatches': [{u'description': u"The deepest, darkest, smoothest Pinot in Mark West's collection, with notes of black cherries, plums, mocha, vanilla, and caramel.Pairs perfectly with grilled lamb chops, gourmet flatbread pizza, and pulled pork sandwiches. View More", u'title': u'Mark West Black Label Monterey County Pinot Noir', u'price': u'$12.99', u'id': 437001, u'link': u'https://www.wine.com/product/mark-west-black-label-monterey-county-pinot-noir-2016/235737', u'imageUrl': u'https://spoonacular.com/productImages/437001-312x231.jpg'}]}, u'aggregateLikes': 106, u'instructions': u'Remove and discard net from pork roast. Place in crock of slow cooker. Arrange onion over the meat, and pour beans and enchilada sauce over the roast. Cover and cook on low for 8-10 hours. Flip roast over halfway through cooking if possible.When ready to serve, remove cooked roast from crockpot to a large cutting board. Carefully pour remaining contents of slow cooker into a colander to drain off the liquid. Shred cooked meat with two forks, removing and discarding large pieces of fat as you work. Return shredded meat and drained beans and onions to cooking vessel or a serving bowl.To assemble burrito bowls, place lettuce in the bottom of a large salad bowl. Place warm, cooked rice over the lettuce (optional), and place a generous helping of pork and beans over the rice. Pile on your desired toppings, and dig in!', u'spoonacularSourceUrl': u'https://spoonacular.com/crockpot-pork-roast-burrito-bowls-723844', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 605, u'servings': 8, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}