{u'recipes': [{u'sourceName': u'Lady Behind the Curtain', u'cuisines': [u'american'], u'image': u'https://spoonacular.com/recipeImages/571877-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 23, u'id': 571877, u'veryPopular': True, u'lowFodmap': False, u'title': u'Lemon Blueberry Swirl Cupcakes', u'diets': [], u'preparationMinutes': 20, u'pricePerServing': 56.41, u'creditText': u'Lady Behind the Curtain', u'extendedIngredients': [{u'originalString': u'3 tablespoons blueberry jelly', u'aisle': u'Nut butters, Jams, and Honey', u'name': u'blueberry jelly', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/blueberry-jam.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 11219297, u'unit': u'tablespoons'}, {u'originalString': u'1/2 cup butter, softened', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [u'softened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 1001, u'unit': u'cup'}, {u'originalString': u"4 cups confectioners' sugar", u'aisle': u'Baking', u'name': u"confectioners' sugar", u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 4.0, u'consistency': u'solid', u'id': 19336, u'unit': u'cups'}, {u'originalString': u'4 ounce cream cheese, softened', u'aisle': u'Cheese', u'name': u'cream cheese', u'metaInformation': [u'softened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 4.0, u'consistency': u'solid', u'id': 1017, u'unit': u'ounce'}, {u'originalString': u'3 eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.j