{u'recipes': [{u'sourceName': u'Simply Sugar and Gluten Free', u'cuisines': [u'southern'], u'image': u'https://spoonacular.com/recipeImages/539848-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 30, u'id': 539848, u'veryPopular': True, u'lowFodmap': False, u'title': u'Pie Crust', u'diets': [], u'preparationMinutes': 75, u'pricePerServing': 22.26, u'creditText': u'Simply Sugar and Gluten Free', u'extendedIngredients': [{u'originalString': u"1 1/2 cups Amy's Basic Flour Blend", u'aisle': u'Baking', u'name': u'cake flour blend', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.5, u'consistency': u'solid', u'id': 10020129, u'unit': u'cups'}, {u'originalString': u'6 - 7 tablespoons ice water', u'aisle': u'Frozen', u'name': u'ice', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ice-cubes.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 6.0, u'consistency': u'solid', u'id': 10014412, u'unit': u'tablespoons'}, {u'originalString': u'pinch of kosher salt', u'aisle': u'Spices and Seasonings', u'name': u'kosher salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'pinch', u'unitShort': u'pinch', u'amount': 1.0, u'consistency': u'solid', u'id': 1082047, u'unit': u'pinch'}, {u'originalString': u'1 tablespoon whole psyllium husks', u'aisle': u'Health Foods', u'name': u'psyllium husks', u'metaInformation': [u'whole'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/psyllium-husks.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 98892, u'unit': u'tablespoon'}, {u'originalString': u'1/4 cup + 3 tablespoons sweet rice flour', u'aisle': u'Gluten Free;Health Foods;Baking', u'name': u'rice flour', u'metaInformation': [u'sweet'], u'image': u'https://spoonacular.com/cdn/ingredients_10