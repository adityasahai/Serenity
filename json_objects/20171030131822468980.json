, u'metaInformation': [u'fresh', u'packed', u'rinsed', u'drained', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.5, u'consistency': u'solid', u'id': 10011457, u'unit': u'cups'}, {u'originalString': u'1/3 cup packed freshly, finely shredded parmesan cheese', u'aisle': u'?', u'name': u'freshly', u'metaInformation': [u'shredded', u'packed', u'finely'], u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.3333333333333333, u'unit': u'cup'}], u'healthScore': 32.0, u'imageType': u'jpg', u'spoonacularScore': 95.0, u'sourceUrl': u'http://www.cookingclassy.com/2016/06/skillet-chicken-creamy-spinach-artichoke-sauce/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 13, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/meat-mallet.jpg', u'id': 3846, u'name': u'meat tenderizer'}], u'step': u'Pound chicken to an even thickness using the flat side of a meat mallet. Season both sides with salt and pepper.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'id': 1065062, u'name': u'meat'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Heat 1 Tbsp olive oil in a large skillet over medium-high heat.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-thermometer.jpg', u'id': 404789, u'name': u'kitchen thermometer'}], u'step': u'Add chicken and cook until golden brown on bottom, about 4 - 5 minutes. Rotate chicken to opposite side, add remaining 1/2 Tbsp olive oil and continue to cook until chicken is golden brown on bottom and center registers 165 on an instant read thermometer, about 4 - 5 minutes longer.', u'length': {u'number': 10, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}], u'step': u'Transfer chicken to a plate, cover with foil to keep warm.Melt butter in same skillet used to cook chicken over medium heat.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Add garlic and flour and cook 30 seconds then add in spinach and artichokes and saute until spinach has wilted, about 1 minute.', u'length': {u'number': 1, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [], u'step': u"Pour in milk, while scraping up browned bits from bottom (don't worry if you can't get all of it, the golden bits will come up as it continues to cook).", u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Add in Neufchatel cheese and parmesan, season with salt and pepper to taste, and cook and stir until mixture has thickened slightly and cheeses have melted. Stir in sour cream then return chicken to skillet. Sprinkle with red pepper flakes and serve warm with cooked orzo if desired. Recipe source: Cooking Classy', u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/neufchatel.jpg', u'id': 1031, u'name': u'neufchatel'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'id': 1056, u'name': u'sour cream'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'Chicken on the menu? Try pairing with Chardonnay, Pinot Noir, and Sauvignon Blanc. White meat is typically paired with white wine as to not overpower the flavor of the meat, so chardonnay and sauvignon blanc are safe choices. Chicken prepared with a tomato sauce or certainly a red wine sauce can and should be served with a red like a pinot noir. One wine you could try is Raymond Reserve Selection Chardonnay. It has 4.4 out of 5 stars and a bottle costs about 18 dollars.', u'pairedWines': [u'chardonnay', u'pinot noir', u'sauvignon blanc'], u'productMatches': [{u'description': u'The Napa Valley Reserve Chardonnay opens with toasty aromas of vanilla bean, baked pears and a hint of cantaloupe. In the mouth, it offers a medley of harmonious flavors, orange zest, caramel, golden delicious apples, jasmine and lemon. This wine is beautifully balanced with crisp acidity and an elegant lingering finish.', u'title': u'Raymond Reserve Selection Chardonnay', u'price': u'$18.29', u'id': 432869, u'link': u'https://www.wine.com/product/raymond-reserve-selection-chardonnay-2004/87725', u'imageUrl': u'https://spoonacular.com/productImages/432869-312x231.jpg'}]}, u'aggregateLikes': 1239, u'instructions': u"Pound chicken to an even thickness using the flat side of a meat mallet. Season both sides with salt and pepper. Heat 1 Tbsp olive oil in a large skillet over medium-high heat. Add chicken and cook until golden brown on bottom, about 4 - 5 minutes. Rotate chicken to opposite side, add remaining 1/2 Tbsp olive oil and continue to cook until chicken is golden brown on bottom and center registers 165 on an instant read thermometer, about 4 - 5 minutes longer. Transfer chicken to a plate, cover with foil to keep warm.Melt butter in same skillet used to cook chicken over medium heat. Add garlic and flour and cook 30 seconds then add in spinach and artichokes and saute until spinach has wilted, about 1 minute. Pour in milk, while scraping up browned bits from bottom (don't worry if you can't get all of it, the golden bits will come up as it continues to cook). Add in Neufchatel cheese and parmesan, season with salt and pepper to taste, and cook and stir until mixture has thickened slightly and cheeses have melted. Stir in sour cream then return chicken to skillet. Sprinkle with red pepper flakes and serve warm with cooked orzo if desired. Recipe source: Cooking Classy", u'spoonacularSourceUrl': u'https://spoonacular.com/skillet-chicken-with-creamy-spinach-artichoke-sauce-779287', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}