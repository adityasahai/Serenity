{u'recipes': [{u'sourceName': u'Pinch of Yum', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/601409-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 601409, u'veryPopular': False, u'lowFodmap': False, u'title': u'Skinny Crab and Artichoke Dip with Jalape\xf1o', u'diets': [u'pescatarian'], u'pricePerServing': 132.77, u'creditText': u'Pinch of Yum', u'extendedIngredients': [{u'originalString': u'14 ounces canned or jarred artichoke hearts', u'aisle': u'Canned and Jarred', u'name': u'artichoke hearts', u'metaInformation': [u'jarred', u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/artichoke-hearts.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 14.0, u'consistency': u'solid', u'id': 93828, u'unit': u'ounces'}, {u'originalString': u'chips for dipping, extra cheese for topping', u'aisle': u'Savory Snacks', u'name': u'corn chips', u'metaInformation': [u'for dipping, extra cheese for topping'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fritos-or-corn-chips.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 12.0, u'consistency': u'solid', u'id': 19003, u'unit': u'servings'}, {u'originalString': u'2 tablespoons flour', u'aisle': u'Baking', u'name': u'flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 20081, u'unit': u'tablespoons'}, {u'originalString': u'1 jalapeno', u'aisle': u'Canned and Jarred;Produce;Ethnic Foods', u'name': u'jalapeno', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/jalapeno-pepper.png', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11979, u'unit': u''}, {u'originalString': u'1 tablespoon fresh lemon juice', u'aisle': u'Produce', u'name': u'lemon juice', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingr