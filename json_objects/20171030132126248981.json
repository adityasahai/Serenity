: True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 6, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/plastic-bag.jpg', u'id': 221671, u'name': u'ziploc bags'}], u'step': u'In a large resealable plastic bag, combine the molasses, mustard and vinegar; add pork. Seal bag and turn to coat; refrigerate for 8 hours or overnight.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/molasses.jpg', u'id': 19304, u'name': u'molasses'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/regular-mustard.jpg', u'id': 2046, u'name': u'mustard'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Prepare grill for indirect heat, using a drip pan.', u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/paper-towels.jpg', u'id': 405895, u'name': u'paper towels'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/tongs.jpg', u'id': 404641, u'name': u'tongs'}], u'step': u'Drain and discard marinade. Moisten a paper towel with cooking oil; using long-handled tongs, lightly coat the grill rack.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-thermometer.jpg', u'id': 404789, u'name': u'kitchen thermometer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Place pork over drip pan and grill, covered, over indirect medium-hot heat for 20-27 minutes or until a thermometer reads 145\xb0, turning occasionally.', u'length': {u'number': 27, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Let stand for 5 minutes before slicing.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 5, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Malbec, Pinot Noir, and Sangiovese are my top picks for Pork Tenderloin. Pinot noir's light body is great for lean cuts, medium bodied sangiovese complement meaty sauces, stews, and other multi-ingredient dishes, and full-bodied tannic malbec pairs with fatty cuts and barbecue. One wine you could try is Finca La Malena Malbec. It has 5 out of 5 stars and a bottle costs about 14 dollars.", u'pairedWines': [u'malbec', u'pinot noir', u'sangiovese'], u'productMatches': [{u'description': u'100% Malbec from eastern Mendoza. Limestone and clay soils, cool winters and warm summers provide great fruit concentration, and smooth tannins. Dark ruby in color reflecting light in an array of orange/purple hues. Pleasant nose filled with hints of red forest fruits and herbs. Young round tannins give it a silky sensuous texture that leads to a rich mouthfeel. Mouthwatering layers of red berries, plum, vanilla and spice open as it warms up, delivering a rich varietal character. A delicate finish that bridges the gap between drinks and lingers well after the last sip.', u'title': u'Finca La Malena Malbec', u'price': u'$13.99', u'id': 431374, u'link': u'https://www.amazon.com/2015-Finca-Malena-Malbec-750mL/dp/B01N9QTXGO', u'imageUrl': u'https://spoonacular.com/productImages/431374-312x231.jpg'}]}, u'aggregateLikes': 46, u'instructions': u'Directions                                                                                In a large resealable plastic bag, combine the molasses, mustard and vinegar; add pork. Seal bag and turn to coat; refrigerate for 8 hours or overnight.                                                                                           Prepare grill for indirect heat, using a drip pan. Drain and discard marinade. Moisten a paper towel with cooking oil; using long-handled tongs, lightly coat the grill rack.                                                                                            Place pork over drip pan and grill, covered, over indirect medium-hot heat for 20-27 minutes or until a thermometer reads 145\xb0, turning occasionally. Let stand for 5 minutes before slicing.                                        Yield: 4 servings.                                                                                                                         Originally published as Carolina Marinated Pork Tenderloin in  Simple & DeliciousAugust/September 2010, p52                                                                                                                                                                                                        Nutritional Facts                                                                             3 ounces cooked pork equals 160 calories, 4 g fat (1 g saturated fat), 63 mg cholesterol, 90 mg sodium, 7 g carbohydrate, 0 fiber, 22 g protein. Diabetic Exchanges: 3 lean meat, 1/2 starch.                                                                                                                                                                    Print                                                                                    Add to Recipe Box                            Email a Friend', u'spoonacularSourceUrl': u'https://spoonacular.com/carolina-marinated-pork-tenderloin-467933', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 30, u'servings': 4, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}