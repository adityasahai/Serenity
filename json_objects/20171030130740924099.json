alt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 1.25, u'consistency': u'solid', u'id': 2047, u'unit': u'teaspoons'}, {u'originalString': u'3 cups cooked udon noodles or whole-wheat spaghetti', u'aisle': u'Ethnic Foods', u'name': u'udon noodles', u'metaInformation': [u'whole-wheat', u'cooked'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/udon-noodles.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.0, u'consistency': u'solid', u'id': 98966, u'unit': u'cups'}, {u'originalString': u'2 tablespoons white miso (see Tip)', u'aisle': u'Ethnic Foods', u'name': u'white miso', u'metaInformation': [u'white', u'(see Tip)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/miso.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 10116112, u'unit': u'tablespoons'}], u'healthScore': 19.0, u'imageType': u'jpg', u'spoonacularScore': 73.0, u'sourceUrl': u'http://www.eatingwell.com/recipe/251277/japanese-chicken-noodle-soup/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 21, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Heat oil in a large pot over medium heat.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Add onion and garlic and cook, stirring occasionally, until softened, 2 to 3 minutes.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [], u'step': u'Add ginger; cook, stirring, for 1 minute.', u'length': {u'number': 1, u