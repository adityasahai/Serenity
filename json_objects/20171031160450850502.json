{u'recipes': [{u'sourceName': u'Simply Sugar and Gluten Free', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/539579-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 12, u'id': 539579, u'veryPopular': True, u'lowFodmap': False, u'title': u'Kristen\u2019s Chewy Chocolate Cookies', u'diets': [u'gluten free', u'dairy free', u'lacto ovo vegetarian'], u'preparationMinutes': 15, u'pricePerServing': 52.58, u'creditText': u'Simply Sugar and Gluten Free', u'extendedIngredients': [{u'originalString': u'1 cup raw unsalted creamy almond butter', u'aisle': u'Nut butters, Jams, and Honey', u'name': u'almond butter', u'metaInformation': [u'raw', u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/peanut-butter.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 12195, u'unit': u'cup'}, {u'originalString': u'\xbd teaspoon apple cider vinegar', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'apple cider vinegar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple-cider-vinegar.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 2048, u'unit': u'teaspoon'}, {u'originalString': u'\xbd teaspoon baking soda', u'aisle': u'Baking', u'name': u'baking soda', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'solid', u'id': 18372, u'unit': u'teaspoon'}, {u'originalString': u'2 tablespoons buckwheat flour', u'aisle': u'Gluten Free;Health Foods;Baking', u'name': u'buckwheat flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/buckwheat-flour.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 20011, u'unit': u'tablespoons'}, {u'originalString': u'\xbd cup xylitol or \xbe cup coconut sugar', u'aisle': u'Baking', u'name': u'coconut sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/palm-sugar.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 93831, u'unit': u'cup'}, {u'originalString': u'2 large organic eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [u'organic', u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'12 drops KAL liquid stevia', u'aisle': u'Baking', u'name': u'liquid stevia', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/stevia-drops.jpg', u'unitLong': u'drops', u'unitShort': u'drops', u'amount': 12.0, u'consistency': u'solid', u'id': 10811111, u'unit': u'drops'}, {u'originalString': u'\xbc teaspoon sea salt', u'aisle': u'Spices and Seasonings', u'name': u'sea salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 1012047, u'unit': u'teaspoon'}, {u'originalString': u'\xbc cup unsweetened cocoa powder', u'aisle': u'Baking', u'name': u'unsweetened cocoa powder', u'metaInformation': [u'unsweetened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 19165, u'unit': u'cup'}, {u'originalString': u'1 teaspoon vanilla extract', u'aisle': u'Baking', u'name': u'vanilla extract', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'teaspoon'}], u'healthScore': 6.0, u'imageType': u'jpg', u'spoonacularScore': 60.0, u'sourceUrl': u'http://simplysugarandglutenfree.com/kristens-chewy-chocolate-cookies/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 4, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat oven to 350\xb0. Line 2 cookie sheets (I use an 18 x 13 cookie sheet) with parchment paper. (I use the same parchment paper later to store them.)Beat eggs with an electric-mixer in a medium bowl and then add almond butter, vanilla, apple cider vinegar and liquid stevia.\xa0Do not mix the rest of the wet ingredients together.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple-cider-vinegar.jpg', u'id': 2048, u'name': u'apple cider vinegar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/almond-butter.jpg', u'id': 12195, u'name': u'almond butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/stevia-drops.jpg', u'id': 10811111, u'name': u'liquid stevia'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Add dry ingredients (xylitol, unsweetened cocoa powder, buckwheat flour, baking soda, sea salt and dried coconut, if you so desire)\xa0in a separate large bowl and whisk together.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'unsweetened cocoa powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/buckwheat-flour', u'id': 20011, u'name': u'buckwheat flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 18372, u'name': u'baking soda'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 1012047, u'name': u'sea salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}], u'step': u'Combine the wet and dry ingredients and mix well with an electric-mixer until thoroughly mixed. At first it will seem too dry, but keep mixing and it will form into a thick, fudge-like dough. Finish mixing with a spatula to make sure it is well mixed.I use a 1 \xbd tablespoon cookie scoop to evenly portion out the dough onto a cookie sheet. Flatten the cookies gently using the back of a spoon or your fingertips.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/wire-rack.jpg', u'id': 405900, u'name': u'wire rack'}], u'step': u'Bake for about 12 minutes. The cookies will be soft and moist on the inside. It\u2019s a little hard to tell when they are golden brown because they are already brown\u2026so I tend to cook them about 12 minutes, they will still be gooey and moist on the inside!\xa0Cook less when in doubt, they firm up as they cool\u2026 do NOT overcook. Cool the cookies on a cooling rack for 10-15 minutes.After they are completely cooled use parchment paper to separate them and store in a ziplock bag or airtight container.', u'length': {u'number': 39, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies can be paired with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. You could try NV Johnson Estate Cream Sherry. Reviewers quite like it with a 5 out of 5 star rating and a price of about 19 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'Very aromatic with notes of hazelnut, vanilla, and a touch of oak followed by sweet raisins and a touch of yeast. Clean lasting finish. Good now but will reward those allow it to age"". A favorite pre-prandial beverage. Consider it with nuts before dinner as an aperitif, or after dinner with dessert, especially chocolates and fruit-based desserts. Also wonderful on cold afternoons, served with biscotti to dip in ""Italian-style"". "', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 1762, u'instructions': u'Preheat oven to 350\xb0. Line 2 cookie sheets (I use an 18 x 13 cookie sheet) with parchment paper. (I use the same parchment paper later to store them.)Beat eggs with an electric-mixer in a medium bowl and then add almond butter, vanilla, apple cider vinegar and liquid stevia.\xa0Do not mix the rest of the wet ingredients together.Add dry ingredients (xylitol, unsweetened cocoa powder, buckwheat flour, baking soda, sea salt and dried coconut, if you so desire)\xa0in a separate large bowl and whisk together.Combine the wet and dry ingredients and mix well with an electric-mixer until thoroughly mixed. At first it will seem too dry, but keep mixing and it will form into a thick, fudge-like dough. Finish mixing with a spatula to make sure it is well mixed.I use a 1 \xbd tablespoon cookie scoop to evenly portion out the dough onto a cookie sheet. Flatten the cookies gently using the back of a spoon or your fingertips. Bake for about 12 minutes. The cookies will be soft and moist on the inside. It\u2019s a little hard to tell when they are golden brown because they are already brown\u2026so I tend to cook them about 12 minutes, they will still be gooey and moist on the inside!\xa0Cook less when in doubt, they firm up as they cool\u2026 do NOT overcook. Cool the cookies on a cooling rack for 10-15 minutes.After they are completely cooled use parchment paper to separate them and store in a ziplock bag or airtight container.', u'spoonacularSourceUrl': u'https://spoonacular.com/kristens-chewy-chocolate-cookies-539579', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 28, u'servings': 16, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}