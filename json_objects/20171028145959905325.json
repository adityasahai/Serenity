tion': [u'to taste'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 8.0, u'consistency': u'solid', u'id': 1102047, u'unit': u'servings'}, {u'originalString': u'2 cups Mexican style shredded cheese, Monterey Jack etc.', u'aisle': u'Cheese', u'name': u'shredded cheese', u'metaInformation': [u'shredded', u'mexican style'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 2.0, u'consistency': u'solid', u'id': 1011026, u'unit': u'cups'}, {u'originalString': u'1 teaspoon chicken or vegetable soup base', u'aisle': u'Canned and Jarred', u'name': u'beef base', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/stock-cube-isolated.png', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1016076, u'unit': u'teaspoon'}, {u'originalString': u'10 cups of water', u'aisle': u'Beverages', u'name': u'water', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 10.0, u'consistency': u'liquid', u'id': 14412, u'unit': u'cups'}], u'healthScore': 6.0, u'imageType': u'jpg', u'spoonacularScore': 50.0, u'sourceUrl': u'http://www.cookiemadness.net/2012/08/hatch-chile-baked-penne/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 19, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}], u'step': u'Roast the peppers. Slit the chili peppers down the center, scrape out the seeds and arrange on a large foil lined baking sheet.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Bake at 450F for about 10 minutes or until the pepper\u2019s skins have blistered. Turn and bake for a few more minutes.', u'length': {u'number': 10, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/tongs.jpg', u'id': 404641, u'name': u'tongs'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Remove from the oven and using tongs, transfer to a heavy duty freezer bag or a bowl with a lid. Seal the hot peppers in the bag (or cover bowl) and let cool. When cool, remove from bags, peel away skins, chop.Bring 10 cups of water to a boil. Stir in the soup base. You can also add a little salt to the water if you want.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'id': 14412, u'name': u'water'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}], u'step': u'Add pasta and boil until al dente (7 to 12 minutes depending on type).Meanwhile, in a food processor or blender, process the sour cream, and chopped chile peppers.', u'length': {u'number': 7, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fusilli.jpg', u'id': 20420, u'name': u'pasta'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/casserole-dish.png', u'id': 404635, u'name': u'casserole dish'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}], u'step': u'Drain the pasta and toss it with the sour cream mixture and 1 cup of the shredded cheese. If you feel like it, add a little cream. At this point you can add some salt and pepper to taste, but I never do.Arrange in a greased 9\xd713 inch casserole dish and cover with foil.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'id': 1011026, u'name': u'shredded cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fusilli.jpg', u'id': 20420, u'name': u'pasta'}]}, {u'equipment': [], u'step': u'Bake at 375 F. for about 10 minutes, then sprinkle remaining cheese over the top.', u'length': {u'number': 10, u'unit': u'minutes'}, u'number': 6, u'ingredients': []}, {u'equipment': [], u'step': u'Bake uncovered for another 10-15 minutes or until cheese is melted.', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Chili can be paired with Cava, Grenache, and Shiraz. These juicy reds don't have too much tannin (important for spicy foods), but a sparkling wine like cava can tame the heat even better. One wine you could try is Bohigas Brut Reserva Cava. It has 4.3 out of 5 stars and a bottle costs about 17 dollars.", u'pairedWines': [u'cava', u'grenache', u'shiraz'], u'productMatches': [{u'description': u'Pale yellow colour. Constant and small bubble. Foam of great persistence. Very fruity aromas, especially of white fruit (acid apple and pear) on shades of peel of citrics and white flowers. In the background, some spicy notes owed to ageing (cinnamon) and cookies. It is a well-structured "cava" in the mouth. Well picked up over its acidity that tenses it overall from beginning to end. The bubble lends a creamy feeling, and it gives volume and softness to the structure that returns acidity and widens the aromas of white fruits and spices that are repeated in the mouth. Its balanced end harmonizes its acidity, and leaves a memory of freshness and volume owing to its light sugar content View More', u'title': u'Bohigas Brut Reserva Cava', u'price': u'$16.99', u'id': 440298, u'link': u'https://www.wine.com/product/bohigas-brut-reserva-cava/131248', u'imageUrl': u'https://spoonacular.com/productImages/440298-312x231.jpg'}]}, u'aggregateLikes': 55, u'instructions': u'Roast the peppers. Slit the chili peppers down the center, scrape out the seeds and arrange on a large foil lined baking sheet. Bake at 450F for about 10 minutes or until the pepper\u2019s skins have blistered. Turn and bake for a few more minutes. Remove from the oven and using tongs, transfer to a heavy duty freezer bag or a bowl with a lid. Seal the hot peppers in the bag (or cover bowl) and let cool. When cool, remove from bags, peel away skins, chop.Bring 10 cups of water to a boil. Stir in the soup base. You can also add a little salt to the water if you want. Add pasta and boil until al dente (7 to 12 minutes depending on type).Meanwhile, in a food processor or blender, process the sour cream, and chopped chile peppers.Drain the pasta and toss it with the sour cream mixture and 1 cup of the shredded cheese. If you feel like it, add a little cream. At this point you can add some salt and pepper to taste, but I never do.Arrange in a greased 9\xd713 inch casserole dish and cover with foil. Bake at 375 F. for about 10 minutes, then sprinkle remaining cheese over the top. Bake uncovered for another 10-15 minutes or until cheese is melted.', u'spoonacularSourceUrl': u'https://spoonacular.com/hatch-chile-baked-penne-257623', u'dishTypes': [u'side dish', u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}