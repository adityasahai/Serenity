aInformation': [u'light', u'packed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 19334, u'unit': u'cup'}, {u'originalString': u'2 cups mini pretzels, measured loosely and crushed slightly', u'aisle': u'Savory Snacks', u'name': u'pretzels', u'metaInformation': [u'mini', u'crushed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pretzels.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 2.0, u'consistency': u'solid', u'id': 19047, u'unit': u'cups'}, {u'originalString': u'1/2 cup unsalted butter, melted (1 stick)', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted', u'melted', u'(1 stick)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 1145, u'unit': u'cup'}, {u'originalString': u'1 tablespoon vanilla extract', u'aisle': u'Baking', u'name': u'vanilla extract', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'tablespoon'}], u'healthScore': 2.0, u'imageType': u'jpg', u'spoonacularScore': 36.0, u'sourceUrl': u'http://www.averiecooks.com/2013/04/salted-caramel-pretzel-blondies.html', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 21, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u"Preheat oven to 350F. Line an 8-by-8-inch pan with aluminum foil, spray with cooking spray; set aside. In a medium microwave-safe bowl, melt the butter, about 1 minute. To the melted butter (allow it to cool slightly so you don't scramble the egg), add the egg, brown sugar, vanilla, and whisk vigorously to combine.", u'length': {u'number': 1, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-dark.jpg', u'id': 19334, u'name': u'brown sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [], u'step': u'Add the flour and stir until just combined, taking care not to over-mix or the blondies will be tough. Fold in the pretzels (I crush them in the palm of my hand by squeezing them).', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pretzels.jpg', u'id': 19047, u'name': u'pretzels'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Pour batter into prepared pan, smoothing it lightly with a spatula or offset knife.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Drizzle about 1/3 cup caramel sauce over the pan in a zig-zag pattern.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sauce-caramel.jpg', u'id': 19364, u'name': u'caramel sauce'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/toothpicks.jpg', u'id': 404644, u'name': u'toothpicks'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u"Bake for about 28 to 32 minutes, or until center is just set. Watch the actual blondies/dough, not the caramel sauce, because it won't fully set. A toothpick should come out mostly clean if you can find a patch of dough to test without hitting a pretzel or caramel sauce. Don't overbake; these are meant to be gooey and firm up as they cool. Immediately after removing pan from oven, drizzle about 1/4 cup caramel sauce over the top in a zig-zag pattern, following the original zig-zag pattern if possible. Optionally, sprinkle with coarse salt to play up the salty-and-sweet contrast. Allow blondies to cool completely in pan and for caramel sauce to soak in somewhat before slicing and serving, at least one hour. Blondies may be stored in an airtight container at room temperature or in the refrigerator for up to 1 week, or in the freezer for up to 4 months.\xa0To keep gluten-free, use your favorite gluten-free flour blend, taking care that all other ingredients used are acceptable for your dietary needs.", u'length': {u'number': 28, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sauce-caramel.jpg', u'id': 19364, u'name': u'caramel sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'coarse salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pretzels.jpg', u'id': 19047, u'name': u'pretzels'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 14683, u'instructions': u"Preheat oven to 350F. Line an 8-by-8-inch pan with aluminum foil, spray with cooking spray; set aside. In a medium microwave-safe bowl, melt the butter, about 1 minute. To the melted butter (allow it to cool slightly so you don't scramble the egg), add the egg, brown sugar, vanilla, and whisk vigorously to combine. Add the flour and stir until just combined, taking care not to over-mix or the blondies will be tough. Fold in the pretzels (I crush them in the palm of my hand by squeezing them). Pour batter into prepared pan, smoothing it lightly with a spatula or offset knife. Drizzle about 1/3 cup caramel sauce over the pan in a zig-zag pattern. Bake for about 28 to 32 minutes, or until center is just set. Watch the actual blondies/dough, not the caramel sauce, because it won't fully set. A toothpick should come out mostly clean if you can find a patch of dough to test without hitting a pretzel or caramel sauce. Don't overbake; these are meant to be gooey and firm up as they cool. Immediately after removing pan from oven, drizzle about 1/4 cup caramel sauce over the top in a zig-zag pattern, following the original zig-zag pattern if possible. Optionally, sprinkle with coarse salt to play up the salty-and-sweet contrast. Allow blondies to cool completely in pan and for caramel sauce to soak in somewhat before slicing and serving, at least one hour. Blondies may be stored in an airtight container at room temperature or in the refrigerator for up to 1 week, or in the freezer for up to 4 months.\xa0To keep gluten-free, use your favorite gluten-free flour blend, taking care that all other ingredients used are acceptable for your dietary needs.", u'spoonacularSourceUrl': u'https://spoonacular.com/salted-caramel-pretzel-blondies-520240', u'dishTypes': [u'side dish'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 35, u'servings': 6, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}