{u'recipes': [{u'sourceName': u'Crock Pot Ladies', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/484631-556x370.png', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 435, u'id': 484631, u'veryPopular': True, u'lowFodmap': False, u'title': u'Crock-Pot Baked Pineapple Ham', u'diets': [u'gluten free', u'dairy free'], u'preparationMinutes': 15, u'pricePerServing': 45.72, u'creditText': u'Crock Pot Ladies', u'extendedIngredients': [{u'originalString': u'1 1/3 cups Brown Sugar', u'aisle': u'Baking', u'name': u'brown sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.3333333333333333, u'consistency': u'solid', u'id': 19334, u'unit': u'cups'}, {u'originalString': u'2 Tablespoons of Cornstarch', u'aisle': u'Baking', u'name': u'cornstarch', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'Tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 20027, u'unit': u'Tablespoons'}, {u'originalString': u'Boneless 3 1/2 Half Pound Ham, Unsliced', u'aisle': u'Meat', u'name': u'ham', u'metaInformation': [u'boneless'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ham-whole.jpg', u'unitLong': u'slices', u'unitShort': u'slices', u'amount': 3.0, u'consistency': u'solid', u'id': 10151, u'unit': u'slices'}, {u'originalString': u'2 Tablespoon of Cider or Lemon Juice', u'aisle': u'Produce', u'name': u'lemon juice', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon-juice.jpg', u'unitLong': u'Tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 9152, u'unit': u'Tablespoon'}, {u'originalString': u'2 Teaspoon Mustard', u'aisle': u'Condiments', u'name': u'mustard', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dijon-mustard.jpg', u'u