{u'recipes': [{u'sourceName': u'Fountain Venue Kitchen', u'cuisines': [u'mexican'], u'image': u'https://spoonacular.com/recipeImages/711276-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 711276, u'veryPopular': False, u'lowFodmap': False, u'title': u'Make-Ahead Pineapple Blueberry Breakfast Parfaits', u'diets': [u'gluten free', u'lacto ovo vegetarian'], u'pricePerServing': 13.88, u'creditText': u'Fountain Venue Kitchen', u'extendedIngredients': [{u'originalString': u'1 \u2013 1 1/3 cups frozen blueberries (no need to thaw; fresh may be used when available)', u'aisle': u'Produce', u'name': u'blueberries', u'metaInformation': [u'fresh', u'frozen', u'thaw', u'(no need to ; may be used when available)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/blueberries.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.3333333333333333, u'consistency': u'solid', u'id': 9050, u'unit': u'cups'}, {u'originalString': u'1 teaspoon cinnamon', u'aisle': u'Spices and Seasonings', u'name': u'cinnamon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2010, u'unit': u'teaspoon'}, {u'originalString': u'4 tablespoons ground flaxseed', u'aisle': u'Health Foods;Baking', u'name': u'ground flaxseed', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flax-seeds.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 4.0, u'consistency': u'solid', u'id': 12220, u'unit': u'tablespoons'}, {u'originalString': u'1 1/3 cup old-fashioned rolled oats (uncooked)*', u'aisle': u'Cereal', u'name': u'old fashioned rolled oats', u'metaInformation': [u'uncooked', u'()'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rolled-oats.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.3333333333333333, u'consistency': u'solid', u'id': 8120, u'unit': u'cup'}, {u'origin