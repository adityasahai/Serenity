{u'recipes': [{u'sourceName': u'The Gunny Sack', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/572117-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 572117, u'veryPopular': True, u'lowFodmap': False, u'title': u'Flower Pot Cookies', u'diets': [], u'pricePerServing': 14.81, u'creditText': u'The Gunny Sack', u'extendedIngredients': [{u'originalString': u'1/2 cup heavy cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'heavy cream', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'liquid', u'id': 1053, u'unit': u'cup'}, {u'originalString': u'8 Oreos', u'aisle': u'Sweet Snacks', u'name': u'oreos', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oreos.png', u'unitLong': u'', u'unitShort': u'', u'amount': 8.0, u'consistency': u'liquid', u'id': 10018166, u'unit': u''}, {u'originalString': u'1 cup semi-sweet chocolate chips', u'aisle': u'Baking', u'name': u'semi sweet chocolate chips', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chocolate-chips.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 10019903, u'unit': u'cup'}, {u'originalString': u'16 oz (24 count) Pillsbury Sugar Cookies', u'aisle': u'Baking', u'name': u'sugar', u'metaInformation': [u'(24 count)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 16.0, u'consistency': u'solid', u'id': 19335, u'unit': u'oz'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 10.0, u'sourceUrl': u'http://www.thegunnysack.com/2014/03/flower-pot-cookies.html', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 8, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mini-muffin-tray.jpg', u'id': 405905, u'name': u'mini muffin tray'}], u'step': u'Grease a mini muffin tin, separate Pillsbury sugar cookie dough into the 24 pieces and put one into each cup.Gently press the center with your thumb to make and indentation.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/measuring-spoons.png', u'id': 404719, u'name': u'measuring spoon'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}], u'step': u'Bake at 375 degrees for 11 to 15 minutes, until golden brown.Use the rounded back of a small measuring spoon to make an indentation in the cups if necessary.Leave the cookies in the muffin tin and allow them to cool completely.Carefully remove the cookie cups from the muffin tin. I used the tip of a thin knife to gently pop them out.Bring 1/2 cups heavy cream to a boil.', u'length': {u'number': 11, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}], u'step': u'Pour over semi-sweet chocolate chips.Allow them to sit for 3 minutes and stir until smooth.Fill the cups with chocolate ganache.Crush Oreos in a food processor or food chopper and sprinkle over the chocolate ganache.', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-chocolate-pieces.jpg', u'id': 19903, u'name': u'semisweet chocolate'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oreos.png', u'id': 10018166, u'name': u'oreo cookies'}]}, {u'equipment': [], u'step': u'Add a flower cupcake topper.', u'number': 4, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies on the menu? Try pairing with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 14092, u'instructions': u'Grease a mini muffin tin, separate Pillsbury sugar cookie dough into the 24 pieces and put one into each cup.Gently press the center with your thumb to make and indentation.Bake at 375 degrees for 11 to 15 minutes, until golden brown.Use the rounded back of a small measuring spoon to make an indentation in the cups if necessary.Leave the cookies in the muffin tin and allow them to cool completely.Carefully remove the cookie cups from the muffin tin. I used the tip of a thin knife to gently pop them out.Bring 1/2 cups heavy cream to a boil.Pour over semi-sweet chocolate chips.Allow them to sit for 3 minutes and stir until smooth.Fill the cups with chocolate ganache.Crush Oreos in a food processor or food chopper and sprinkle over the chocolate ganache.Add a flower cupcake topper.', u'spoonacularSourceUrl': u'https://spoonacular.com/flower-pot-cookies-572117', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 24, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}