u'veryHealthy': False, u'weightWatcherSmartPoints': 30, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'1 Preheat oven to 350\xb0F.  Butter two 9 inch cake pans.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/wax-paper.jpg', u'id': 404739, u'name': u'wax paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Cut out rounds of wax paper and place at bottoms of cake pans. Butter the top of the wax paper rounds.2 Sift dry ingredients into a bowl.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Add oil, eggs, and vanilla.  Beat well.  Fold in chopped walnuts, coconut, carrots and pineapple.3', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/walnuts.jpg', u'id': 12155, u'name': u'walnut pieces'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pineapple.jpg', u'id': 9266, u'name': u'pineapple'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/toothpicks.jpg', u'id': 404644, u'name': u'toothpicks'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Pour batter into pans.  Set on the middle rack of oven and bake for 45-50 minutes (shift positions of cakes front-to-back if necessary about halfway through), until edges have pulled away from sides and a toothpick or sharp knife tip inserted into the center of the cake comes out clean.  Cool on a cake rack.4 To prepare frosting, cream together the cream cheese and butter in a mixing bowl.  Slowly sift in the confectioners sugar and beat until mixture is free of lumps.  Stir in vanilla and lemon juice.5 Once cakes have cooled, frost. Sprinkle top with chopped walnuts or arrange walnut halves in a crown around the top.', u'length': {u'number': 50, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'id': 19336, u'name': u'powdered sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/walnuts.jpg', u'id': 12155, u'name': u'walnut pieces'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/walnuts.jpg', u'id': 12155, u'name': u'walnut halves'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'id': 1017, u'name': u'cream cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon-juice.jpg', u'id': 9152, u'name': u'lemon juice'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Carrot Cake works really well with Cream Sherry, Moscato d'Asti, and Port. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 1069, u'instructions': u'1 Preheat oven to 350\xb0F.  Butter two 9 inch cake pans. Cut out rounds of wax paper and place at bottoms of cake pans. Butter the top of the wax paper rounds.2 Sift dry ingredients into a bowl.  Add oil, eggs, and vanilla.  Beat well.  Fold in chopped walnuts, coconut, carrots and pineapple.3 Pour batter into pans.  Set on the middle rack of oven and bake for 45-50 minutes (shift positions of cakes front-to-back if necessary about halfway through), until edges have pulled away from sides and a toothpick or sharp knife tip inserted into the center of the cake comes out clean.  Cool on a cake rack.4 To prepare frosting, cream together the cream cheese and butter in a mixing bowl.  Slowly sift in the confectioners sugar and beat until mixture is free of lumps.  Stir in vanilla and lemon juice.5 Once cakes have cooled, frost. Sprinkle top with chopped walnuts or arrange walnut halves in a crown around the top.', u'spoonacularSourceUrl': u'https://spoonacular.com/carrot-cake-245722', u'dishTypes': [], u'occasions': [u'easter'], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 12, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}