{u'recipes': [{u'sourceName': u'Iowa Girl Eats', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/631397-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 631397, u'veryPopular': True, u'lowFodmap': False, u'title': u'Lighter Buffalo Chicken Mac and Cheese - Iowa Girl Eats', u'diets': [], u'pricePerServing': 169.82, u'creditText': u'Iowa Girl Eats', u'extendedIngredients': [{u'originalString': u'1/4 cup + 2 Tablespoons buffalo wing sauce, divided', u'aisle': u'Condiments', u'name': u'buffalo wing sauce', u'metaInformation': [u'divided'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/hot-sauce-or-tabasco.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 98878, u'unit': u'cup'}, {u'originalString': u'2 Tablespoons butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'Tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 1001, u'unit': u'Tablespoons'}, {u'originalString': u'1lb chicken breasts, chopped into bite-sized pieces', u'aisle': u'Meat', u'name': u'chicken breasts', u'metaInformation': [u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chicken-breasts.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 5062, u'unit': u'lb'}, {u'originalString': u'2 garlic cloves, minced', u'aisle': u'Produce', u'name': u'garlic cloves', u'metaInformation': [u'minced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 11215, u'unit': u''}, {u'originalString': u'2 Tablespoons gluten-free or all-purpose flour', u'aisle': u'Gluten Free;Health Foods;Baking', u'name': u'gluten-free flour', u'metaInformation': [u'gluten-free', u'all-purpose'], u'image': u'https://