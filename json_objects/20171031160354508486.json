': u'https://spoonacular.com/chocolate-hummus-cookie-sandwiches-674099', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 15, u'servings': 5, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}