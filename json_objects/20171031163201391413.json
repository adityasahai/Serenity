, u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 2050, u'unit': u'teaspoon'}], u'healthScore': 4.0, u'imageType': u'jpg', u'spoonacularScore': 41.0, u'sourceUrl': u'http://www.the-baker-chick.com/2013/11/pumpkin-frangelico-swirl-cheesecake/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 36, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350F.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Combine the graham cracker crumbs and sugar in a small bowl.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/graham-crackers.jpg', u'id': 10018617, u'name': u'graham cracker crumbs'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stand-mixer.jpg', u'id': 404665, u'name': u'stand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Add the melted butter and liqueur and stir until moistened. Press crust firmly into the bottom of a 7-inch spring-form pan, and bake for 10-12 minutes or until golden. In a large bowl of a stand mixer (or with a hand mixer,) cream together the cream cheese and sugar until smooth.', u'length': {u'number': 12, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'id': 1017, u'name': u'cream cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u"Add the flour, and mix until just combined. (Don't over-mix.)", u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [], u'step': u'Add the vanilla extract and eggs,( one at a time,) mixing until incorporated before adding the next.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'id': 2050, u'name': u'vanilla extract'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [], u'step': u'Remove 1 cup of the cheesecake batter, stir in the tablespoon of Frangelico and set aside. Gently stir the pumpkin pur\xe9e and pumpkin pie spice into the remaining cheesecake batter.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garam-masala.jpg', u'id': 1002035, u'name': u'pumpkin pie spice'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/wooden-skewers.jpg', u'id': 3065, u'name': u'skewers'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}], u'step': u'Pour the pumpkin cheesecake batter into the crust and then spoon the Frangelico batter into dots or circles over the top. Run a skewer or knife back and forth to swirl the two batters together.', u'number': 7, u'ingredients': []}, {u'equipment': [], u'step': u'Place cheesecake on a middle rack and reduce heat to 300F.', u'number': 8, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u"Bake for about 40 minutes or until the outside seems set while the inside of the cheesecake seems to giggle when you shake or tap the oven. (It is VERY important that you don't peek or open the oven the entire time it's baking.)Turn the oven off and allow cheesecake to slowly cool in the oven for 2 hours. (This will prevent cracking.) Cool the rest of the way on the countertop and then wrap with plastic and chill before serving.", u'length': {u'number': 40, u'unit': u'minutes'}, u'number': 9, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are my top picks for Cake. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. You could try NV Solera Cream Sherry. Reviewers quite like it with a 4.5 out of 5 star rating and a price of about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 40, u'instructions': u"Preheat oven to 350F. Combine the graham cracker crumbs and sugar in a small bowl. Add the melted butter and liqueur and stir until moistened. Press crust firmly into the bottom of a 7-inch spring-form pan, and bake for 10-12 minutes or until golden. In a large bowl of a stand mixer (or with a hand mixer,) cream together the cream cheese and sugar until smooth. Add the flour, and mix until just combined. (Don't over-mix.)Add the vanilla extract and eggs,( one at a time,) mixing until incorporated before adding the next.Remove 1 cup of the cheesecake batter, stir in the tablespoon of Frangelico and set aside. Gently stir the pumpkin pur\xe9e and pumpkin pie spice into the remaining cheesecake batter.Pour the pumpkin cheesecake batter into the crust and then spoon the Frangelico batter into dots or circles over the top. Run a skewer or knife back and forth to swirl the two batters together.Place cheesecake on a middle rack and reduce heat to 300F. Bake for about 40 minutes or until the outside seems set while the inside of the cheesecake seems to giggle when you shake or tap the oven. (It is VERY important that you don't peek or open the oven the entire time it's baking.)Turn the oven off and allow cheesecake to slowly cool in the oven for 2 hours. (This will prevent cracking.) Cool the rest of the way on the countertop and then wrap with plastic and chill before serving.", u'spoonacularSourceUrl': u'https://spoonacular.com/pumpkin-frangelico-swirl-cheesecake-571474', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 5, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}