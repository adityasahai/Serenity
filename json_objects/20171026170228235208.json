{u'recipes': [{u'sourceName': u'Handle the Heat', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/559598-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 559598, u'veryPopular': True, u'lowFodmap': False, u'title': u'Peppermint Oreo Truffles', u'diets': [], u'pricePerServing': 38.69, u'creditText': u'Handle the Heat', u'extendedIngredients': [{u'originalString': u'6 regular-size candy canes, finely crushed', u'aisle': u'Sweet Snacks', u'name': u'candy canes', u'metaInformation': [u'crushed', u'finely'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/candy-canes.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 6.0, u'consistency': u'solid', u'id': 93759, u'unit': u''}, {u'originalString': u'8 ounces cream cheese, at room temperature', u'aisle': u'Cheese', u'name': u'cream cheese', u'metaInformation': [u'at room temperature'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 8.0, u'consistency': u'solid', u'id': 1017, u'unit': u'ounces'}, {u'originalString': u'1 (16.6 ounce) package regular Oreo cookies', u'aisle': u'Sweet Snacks', u'name': u'oreo cookies', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oreos.png', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 16.6, u'consistency': u'liquid', u'id': 10018166, u'unit': u'ounce'}, {u'originalString': u'16 ounces semisweet chocolate, finely chopped', u'aisle': u'Sweet Snacks', u'name': u'semisweet chocolate', u'metaInformation': [u'finely chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-chocolate-pieces.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 16.0, u'consistency': u'solid', u'id': 19903, u'unit': u'ounces'}, {u'originalString': u'1 tablespoon coconut oil or vegetable oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'vegetable oil', u'metaInformation': [], u'image': u'https://spoonacular.com/c