{u'recipes': [{u'sourceName': u'Foodnetwork', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/742350-556x370.jpeg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 188, u'id': 742350, u'veryPopular': False, u'lowFodmap': False, u'title': u'Girdlebuster Pie', u'diets': [], u'preparationMinutes': 30, u'pricePerServing': 158.79, u'creditText': u'Foodnetwork', u'extendedIngredients': [{u'originalString': u'2 tablespoons bourbon', u'aisle': u'Alcoholic Beverages', u'name': u'bourbon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bourbon.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 10014037, u'unit': u'tablespoons'}, {u'originalString': u'3 ounces (75g) soft butter, plus 3 ounces', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [u'soft'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 75.0, u'consistency': u'solid', u'id': 1001, u'unit': u'g'}, {u'originalString': u'2 ounces (50g) dark chocolate pieces or chips', u'aisle': u'Sweet Snacks', u'name': u'candy coated chocolate pieces', u'metaInformation': [u'dark'], u'image': u"https://spoonacular.com/cdn/ingredients_100x100/candy-coated-chocolate-pieces-or-M&M's.jpg", u'unitLong': u'grams', u'unitShort': u'g', u'amount': 50.0, u'consistency': u'solid', u'id': 19157, u'unit': u'g'}, {u'originalString': u'13 ounces (375g) digestive biscuits', u'aisle': u'Sweet Snacks', u'name': u'digestive biscuits', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/digestive-biscuits.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 375.0, u'consistency': u'solid', u'id': 18173, u'unit': u'g'}, {u'originalString': u'1/2 cup (125ml) double cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'double cream', u'metaInformation': [], u'image': u'https://spoonacul