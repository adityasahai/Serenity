{u'recipes': [{u'sourceName': u'Oh Sweet Basil', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/670482-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 0, u'id': 670482, u'veryPopular': True, u'lowFodmap': False, u'title': u'Frito Salad', u'diets': [u'gluten free'], u'preparationMinutes': 5, u'pricePerServing': 122.85, u'creditText': u'Oh Sweet Basil', u'extendedIngredients': [{u'originalString': u'1 Bell Pepper, chopped', u'aisle': u'Produce', u'name': u'bell pepper', u'metaInformation': [u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bell-pepper-orange.png', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 10211821, u'unit': u''}, {u'originalString': u'1 Regular Bottle Catalina dressing', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'catalina dressing', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/catalina-dressing.png', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'liquid', u'id': 93649, u'unit': u''}, {u'originalString': u'2 cups Grated cheese', u'aisle': u'Cheese', u'name': u'cheese', u'metaInformation': [u'grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 2.0, u'consistency': u'solid', u'id': 1041009, u'unit': u'cups'}, {u'originalString': u'1 can chili beans and sauce (do not drain)', u'aisle': u'Canned and Jarred', u'name': u'chili beans', u'metaInformation': [u'(do not drain)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/kidney-beans.jpg', u'unitLong': u'can', u'unitShort': u'can', u'amount': 1.0, u'consistency': u'solid', u'id': 43112, u'unit': u'can'}, {u'originalString': u'Cilantro for garnish', u'aisle': u'Produce;Spices and Seasonings', u'name': u'cilantro', u'metaInformation': [u'for garnish'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cilantro.png', u'un