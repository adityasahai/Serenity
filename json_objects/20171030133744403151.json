{u'recipes': [{u'sourceName': u'Blender Babes', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/581074-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 581074, u'veryPopular': False, u'lowFodmap': False, u'title': u'Roasted Carrot and Parsnip Soup', u'diets': [u'caveman', u'gluten free', u'dairy free', u'lacto ovo vegetarian', u'primal', u'whole 30', u'vegan'], u'pricePerServing': 112.21, u'creditText': u'Blender Babes', u'extendedIngredients': [{u'originalString': u'2 pounds carrots, peeled and cut into 1 inch slices', u'aisle': u'Produce', u'name': u'carrots', u'metaInformation': [u'peeled', u'cut into 1 inch slices'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'solid', u'id': 11124, u'unit': u'pounds'}, {u'originalString': u'2 tablespoons olive oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'olive oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 4053, u'unit': u'tablespoons'}, {u'originalString': u'2 pounds parsnips, peeled and cut into 1 inch slices', u'aisle': u'Produce', u'name': u'parsnips', u'metaInformation': [u'peeled', u'cut into 1 inch slices'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsnip.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'solid', u'id': 11298, u'unit': u'pounds'}, {u'originalString': u'1 teaspoon Celtic salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2047, u'unit': u'teaspoon'}, {u'originalString': u'8 cups low sodium chicken or vegetable stock (or more for a thinner soup)', u'aisle': u'Canned and Jarred', u'name': u'vegetable stock', u'metaInformation': [u'low sodium', u'for a thinner soup)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 8.0, u'consistency': u'liquid', u'id': 6615, u'unit': u'cups'}, {u'originalString': u'1 large yellow onion, chopped', u'aisle': u'Produce', u'name': u'yellow onion', u'metaInformation': [u'yellow', u'large', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 10511282, u'unit': u''}], u'healthScore': 25.0, u'imageType': u'jpg', u'spoonacularScore': 84.0, u'sourceUrl': u'http://www.blenderbabes.com/lifestyle-diet/dairy-free/roasted-carrot-and-parsnip-soup-recipe/', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 1, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Spray sheet pan with nonstick spray. Toss the parsnips, carrots, and onions in olive oil and celtic salt.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsnip.jpg', u'id': 11298, u'name': u'parsnip'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404794, u'name': u'stove'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u"Place parsnips, carrots and onions on the sheet pan.Roast vegetables for approximately 35 to 40 minutes, turning half way through, and allowing them to become tender and caramelize (the onions should be translucent).Fill your blender jar halfway with the vegetable mixture and fill the other half with the stock. You may have to do this only once, or multiple times, depending on whether you are doing the full recipe or a lesser amount.For Vitamix: Start on Variable/Speed 1 and ramp up to Variable/Speed 4 for approximately 10 seconds, then up to Variable/Speed 7 for 40 seconds or until well combined and smooth. Repeat as necessary in batches, pouring the blended mixture into a soup pot to heat on the stove. If doing only one jar's worth of soup, blend for 4-5 minutes to heat the stock, stopping when a thick steam starts to escape the lid.For Blendtec: If doing large amounts, do in 2 or 3 batches, and run the SOUP cycle once, then pour blended mixture into a soup pot and heat on the stove, repeating in batches. If doing only one jar's worth of soup use the SOUP cycle 3 times.If warming the soup on the stove, bring it to a simmer over Medium-High heat.", u'length': {u'number': 40, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsnip.jpg', u'id': 11298, u'name': u'parsnip'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'id': 1006615, u'name': u'stock'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Pour into bowls, serve warm, and enjoy!', u'number': 3, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'', u'pairedWines': [], u'productMatches': []}, u'aggregateLikes': 24, u'instructions': u"Spray sheet pan with nonstick spray. Toss the parsnips, carrots, and onions in olive oil and celtic salt. Place parsnips, carrots and onions on the sheet pan.Roast vegetables for approximately 35 to 40 minutes, turning half way through, and allowing them to become tender and caramelize (the onions should be translucent).Fill your blender jar halfway with the vegetable mixture and fill the other half with the stock. You may have to do this only once, or multiple times, depending on whether you are doing the full recipe or a lesser amount.For Vitamix: Start on Variable/Speed 1 and ramp up to Variable/Speed 4 for approximately 10 seconds, then up to Variable/Speed 7 for 40 seconds or until well combined and smooth. Repeat as necessary in batches, pouring the blended mixture into a soup pot to heat on the stove. If doing only one jar's worth of soup, blend for 4-5 minutes to heat the stock, stopping when a thick steam starts to escape the lid.For Blendtec: If doing large amounts, do in 2 or 3 batches, and run the SOUP cycle once, then pour blended mixture into a soup pot and heat on the stove, repeating in batches. If doing only one jar's worth of soup use the SOUP cycle 3 times.If warming the soup on the stove, bring it to a simmer over Medium-High heat.Pour into bowls, serve warm, and enjoy!", u'spoonacularSourceUrl': u'https://spoonacular.com/roasted-carrot-and-parsnip-soup-581074', u'dishTypes': [u'soup'], u'occasions': [u'fall', u'winter'], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 10, u'sustainable': False, u'dairyFree': True, u'whole30': True}]}