{u'recipes': [{u'sourceName': u'Cup Cake Project', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/518771-556x370.png', u'cheap': False, u'gaps': u'no', u'id': 518771, u'veryPopular': True, u'lowFodmap': False, u'title': u'Chocolate Mousse Pie with Coconut Macaroon Crust', u'diets': [u'gluten free'], u'pricePerServing': 84.59, u'creditText': u'Cup Cake Project', u'extendedIngredients': [{u'originalString': u'4 large egg whites', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'egg whites', u'metaInformation': [u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 4.0, u'consistency': u'solid', u'id': 1124, u'unit': u''}, {u'originalString': u'1 large egg yolk', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'egg yolk', u'metaInformation': [u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-yolk.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 1125, u'unit': u''}, {u'originalString': u'1/3 cup heavy whipping cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'heavy whipping cream', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.3333333333333333, u'consistency': u'liquid', u'id': 1053, u'unit': u'cup'}, {u'originalString': u'6 ounces milk chocolate', u'aisle': u'Sweet Snacks', u'name': u'milk chocolate', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk-chocolate.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 6.0, u'consistency': u'solid', u'id': 19081, u'unit': u'ounces'}, {u'originalString': u'1/4 teaspoon salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2047, u'unit': u'teaspoon'}, {u'originalString': u'2 tablespoons sugar', u'aisle': u'Baking', u'name': u'sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 19335, u'unit': u'tablespoons'}, {u'originalString': u'3 cups dried shredded coconut (I prefer to use unsweetened coconut, but sweetened would also work.)', u'aisle': u'Baking', u'name': u'sweetened coconut', u'metaInformation': [u'dried', u'shredded', u'unsweetened', u'sweetened', u'(I prefer to use coconut, but would also work.)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/coconut-shredded.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.0, u'consistency': u'solid', u'id': 12109, u'unit': u'cups'}, {u'originalString': u'2 teaspoons vanilla', u'aisle': u'Baking', u'name': u'vanilla', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'teaspoons'}], u'healthScore': 1.0, u'imageType': u'png', u'spoonacularScore': 28.0, u'sourceUrl': u'http://www.cupcakeproject.com/2014/04/the-dessert-that-wont-be-passed-over.html', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 16, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Bring a pot of water to a simmer.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stand-mixer.jpg', u'id': 404665, u'name': u'stand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Mix all ingredients in a heat-proof mixing bowl (I use the bowl from my stand mixer) and set it over the pot of water.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Heat for 5-7 minutes until the mixture is warm and the egg whites have thickened - stir while heating to prevent burning.', u'length': {u'number': 7, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'id': 1124, u'name': u'egg whites'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pie-pan.png', u'id': 405915, u'name': u'pie form'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Remove from heat and let the mixture sit for 30 minutes so that the coconut can absorb more of the moisture.Preheat oven to 350 F.Grease a 9 inch pie pan with non-stick cooking spray (I would not recommend a deep dish pan for this recipe).  Press coconut mixture into the pie pan, evenly distributing it and being sure to push it all the way up the sides of the pan.', u'length': {u'number': 30, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/double-boiler.jpg', u'id': 404699, u'name': u'double boiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404794, u'name': u'stove'}], u'step': u"Bake for 5-7 minutes until the edges of the crust just start to get golden.Reduce heat to 325 F and bake for another 10-15 minutes until the edge of the crust is all a nice golden brown.Melt chocolate in a double boiler or in the microwave.Cool until it's warm to the touch, but not burning hot.Bring the heavy whipping cream to a boil (you can do this on the stove or in the microwave).", u'length': {u'number': 22, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk-chocolate.jpg', u'id': 19081, u'name': u'chocolate'}]}, {u'equipment': [], u'step': u'Pour the heavy whipping cream over the melted chocolate and mix thoroughly.  If the heavy whipping cream is not hot, it will cause the chocolate to harden and get chunky - not what you want for your mousse.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk-chocolate.jpg', u'id': 19081, u'name': u'chocolate'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Mix in the egg yolk.Whip the egg whites in a separate bowl until you get soft peaks - they will have the texture of shaving cream.', u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'id': 1124, u'name': u'egg whites'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-yolk.jpg', u'id': 1125, u'name': u'egg yolk'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}]}, {u'equipment': [], u'step': u"Mix the sugar into the egg whites a little bit at a time.Fold the egg whites into the chocolate in very small spoonfuls.  Be extremely gentle.  You don't want to smash the fluffiness out of the egg whites.", u'number': 8, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'id': 1124, u'name': u'egg whites'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk-chocolate.jpg', u'id': 19081, u'name': u'chocolate'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u'Pour the mousse into the cooked pie crust.Chill for at least two hours prior to serving.', u'number': 9, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Lambrusco Dolce, Late Harvest Riesling, and Vin Santo are great choices for Pie. These wines are all sweet, which is important since wine should usually be sweeter than the food you're pairing with it. One wine you could try is Angel Late Harvest Riesling. It has 5 out of 5 stars and a bottle costs about 19 dollars.", u'pairedWines': [u'lambrusco dolce', u'late harvest riesling', u'vin santo'], u'productMatches': [{u'description': u"Messina Hof's Angel is a crisp, sweet dessert wine, harvested from the last pick of Riesling when the grapes are almost raisins. Perfect with fruit, cheesecake and light, sweet chocolate desserts.", u'title': u'Angel Late Harvest Riesling', u'price': u'$18.99', u'id': 431447, u'link': u'https://www.amazon.com/Messina-Hof-Wine-Cellars-Inc/dp/B01MEE7NSP', u'imageUrl': u'https://spoonacular.com/productImages/431447-312x231.jpg'}]}, u'aggregateLikes': 857, u'instructions': u"Bring a pot of water to a simmer.Mix all ingredients in a heat-proof mixing bowl (I use the bowl from my stand mixer) and set it over the pot of water.Heat for 5-7 minutes until the mixture is warm and the egg whites have thickened - stir while heating to prevent burning.  Remove from heat and let the mixture sit for 30 minutes so that the coconut can absorb more of the moisture.Preheat oven to 350 F.Grease a 9 inch pie pan with non-stick cooking spray (I would not recommend a deep dish pan for this recipe).  Press coconut mixture into the pie pan, evenly distributing it and being sure to push it all the way up the sides of the pan.Bake for 5-7 minutes until the edges of the crust just start to get golden.Reduce heat to 325 F and bake for another 10-15 minutes until the edge of the crust is all a nice golden brown.Melt chocolate in a double boiler or in the microwave.Cool until it's warm to the touch, but not burning hot.Bring the heavy whipping cream to a boil (you can do this on the stove or in the microwave).Pour the heavy whipping cream over the melted chocolate and mix thoroughly.  If the heavy whipping cream is not hot, it will cause the chocolate to harden and get chunky - not what you want for your mousse.Mix in the egg yolk.Whip the egg whites in a separate bowl until you get soft peaks - they will have the texture of shaving cream.Mix the sugar into the egg whites a little bit at a time.Fold the egg whites into the chocolate in very small spoonfuls.  Be extremely gentle.  You don't want to smash the fluffiness out of the egg whites.  Pour the mousse into the cooked pie crust.Chill for at least two hours prior to serving.", u'spoonacularSourceUrl': u'https://spoonacular.com/chocolate-mousse-pie-with-coconut-macaroon-crust-518771', u'dishTypes': [u'dessert'], u'occasions': [u"valentine's day"], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}