itShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 11215, u'unit': u''}, {u'originalString': u'1\xbd cups chopped mushrooms', u'aisle': u'Produce', u'name': u'mushrooms', u'metaInformation': [u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mushrooms.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.5, u'consistency': u'solid', u'id': 11260, u'unit': u'cups'}, {u'originalString': u'1 tablespoon olive oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'olive oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 4053, u'unit': u'tablespoon'}, {u'originalString': u'\xbd cup diced onions', u'aisle': u'Produce', u'name': u'onions', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 11282, u'unit': u'cup'}, {u'originalString': u'\xbc teaspoon pepper', u'aisle': u'Spices and Seasonings', u'name': u'pepper', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 1002030, u'unit': u'teaspoon'}, {u'originalString': u'\xbd teaspoon salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'solid', u'id': 2047, u'unit': u'teaspoon'}, {u'originalString': u'1 teaspoon Worcestershire', u'aisle': u'Condiments', u'name': u'worcestershire', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-sauce.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 6971, u'unit': u'teaspoon'}, {u'originalString': u'1 cup diced zucchini', u'aisle': u'Produce', u'name': u'zucchini', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/zucchini.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 11477, u'unit': u'cup'}], u'healthScore': 11.0, u'imageType': u'jpg', u'spoonacularScore': 67.0, u'sourceUrl': u'http://www.nutritiouseats.com/quinoa-veggie-burger-gluten-free/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 4, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'In a large non stick skillet, heat 1 teaspoons of olive oil on medium heat.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [], u'step': u'Add onions, zucchini, carrots, mushrooms and garlic. Saut for ~7-8 minutes.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mushrooms.jpg', u'id': 11260, u'name': u'mushrooms'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/zucchini.jpg', u'id': 11477, u'name': u'zucchini'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Transfer to a large bowl. Stir in 2 cups cooked quinoa, 1 teaspoon Worcestershire sauce, salt, pepper, eggs and corn starch and stir well to combine.Wipe skillet clean and add 1 teaspoons olive oil and heat on medium heat. Divide quinoa/veggie mixture into 6 equal portions. Using your hand form a patty, Carefully place patty in skillet. Using a rubber spatula or spoon, pat veggie burger around the sides and on top to pack all the ingredients in. This will help them stick together when flipping. After ~4 minutes, lift slightly to check the underside of the patty, if it feels like it will hold together and has a nice brown crust, flip it over and cook the other side for ~4 minutes or until done.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-sauce.jpg', u'id': 6971, u'name': u'worcestershire sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cooked-quinoa.jpg', u'id': 20137, u'name': u'cooked quinoa'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 20027, u'name': u'corn starch'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'id': 1002030, u'name': u'pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [], u'step': u'Serve on a bun (gluten free if needed) or wrapped in lettuce.', u'number': 4, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Malbec, Merlot, and Zinfandel are great choices for Burger. Merlot will be perfectly adequate for a classic burger with standard toppings. Bolder toppings call for bolder wines, such as a malbec or peppery zinfandel. One wine you could try is Alamos Malbec Mendoza Seleccion. It has 4.2 out of 5 stars and a bottle costs about 17 dollars.', u'pairedWines': [u'malbec', u'merlot', u'zinfandel'], u'productMatches': [{u'description': u'Malbec is the "signature wines of the Mendoza region, representing the greatest achievements of Argentine winemaking, and Alamos Seleccion Malbec is the pinnacle of Alamos winemaking. The nose is full of ripe black fruits with floral tones and touches of leather, vanilla, and spice. The mouthfeel is full yet supple, with juicy black raspberry, black cherry and black currant fruit flavors layered with notes of violets, black pepper and anise. The finish is long and lingering, with soft, velvety tannins and bright, refreshing acidity.', u'title': u'Alamos Malbec Mendoza Seleccion', u'price': u'$16.95', u'id': 443245, u'link': u'https://www.wine.com/product/alamos-malbec-mendoza-seleccion-2009/106171', u'imageUrl': u'https://spoonacular.com/productImages/443245-312x231.jpg'}]}, u'aggregateLikes': 66, u'instructions': u'In a large non stick skillet, heat 1 teaspoons of olive oil on medium heat. Add onions, zucchini, carrots, mushrooms and garlic. Saut for ~7-8 minutes. Transfer to a large bowl. Stir in 2 cups cooked quinoa, 1 teaspoon Worcestershire sauce, salt, pepper, eggs and corn starch and stir well to combine.Wipe skillet clean and add 1 teaspoons olive oil and heat on medium heat. Divide quinoa/veggie mixture into 6 equal portions. Using your hand form a patty, Carefully place patty in skillet. Using a rubber spatula or spoon, pat veggie burger around the sides and on top to pack all the ingredients in. This will help them stick together when flipping. After ~4 minutes, lift slightly to check the underside of the patty, if it feels like it will hold together and has a nice brown crust, flip it over and cook the other side for ~4 minutes or until done. Serve on a bun (gluten free if needed) or wrapped in lettuce.', u'spoonacularSourceUrl': u'https://spoonacular.com/quinoa-veggie-burger-712898', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 30, u'servings': 6, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}