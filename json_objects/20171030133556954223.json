{u'recipes': [{u'sourceName': u'Serious Eats', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/196684-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 196684, u'veryPopular': False, u'lowFodmap': False, u'title': u'Flourless Chocolate Cake with Spiced Pears', u'diets': [u'gluten free'], u'pricePerServing': 414.57, u'creditText': u'Serious Eats', u'extendedIngredients': [{u'originalString': u'1 medium beet, peeled and sliced (see notes)', u'aisle': u'Produce', u'name': u'beet', u'metaInformation': [u'medium', u'peeled', u'sliced', u'(see notes)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/beets.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11080, u'unit': u''}, {u'originalString': u'16 ounces bittersweet chocolate, finely chopped', u'aisle': u'Sweet Snacks', u'name': u'bittersweet chocolate', u'metaInformation': [u'finely chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-chocolate-pieces.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 16.0, u'consistency': u'liquid', u'id': 19903, u'unit': u'ounces'}, {u'originalString': u'1 teaspoon black peppercorns', u'aisle': u'Spices and Seasonings', u'name': u'black peppercorns', u'metaInformation': [u'black'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/black-pepper.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1022030, u'unit': u'teaspoon'}, {u'originalString': u'4 firm Bosc pears, peeled, cored, and cut into 8 pieces lengthwise', u'aisle': u'Produce', u'name': u'bosc pears', u'metaInformation': [u'firm', u'cored', u'peeled', u'cut into 8 pieces lengthwise'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pears-bosc.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 4.0, u'consistency': u'solid', u'id': 9252, u'unit': u''}, {u'originalString': u'4 ounces (1 stick) plus 1 tablespoon unsalted butter, cut into 1 tabl