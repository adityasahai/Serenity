llars.', u'pairedWines': [u'chardonnay', u'gruener veltliner', u'sauvignon blanc'], u'productMatches': [{u'description': u'Our 2012 vintage has aromas of tropical fruit and butterscotch that lead into a lively blend of pear, pineapple and citrus flavors', u'title': u'District 7 Estate Chardonnay', u'price': u'$17.99', u'id': 430702, u'link': u'https://www.amazon.com/District-Monterey-Estate-Chardonnay-White/dp/B01EXEX4P6', u'imageUrl': u'https://spoonacular.com/productImages/430702-312x231.jpg'}]}, u'aggregateLikes': 539, u'instructions': u'1. Whisk together lemon juice and oil in bowl. Stir in leek, and season with salt and pepper, if desired. Set aside.2. Roll 7 or 8 spinach leaves into tight cylinder, then slice into thin strips. Repeat with remaining spinach leaves.3. Combine spinach, chickpeas, carrots, bell pepper, and sunflower seeds in large bowl. Stir in leek mixture, and toss to coat. Season with salt and pepper, if desired.4. Divide romaine among 8 plates, top with chickpea salad, and arrange tomatoes around salad, if using. Garnish each serving with 1 Tbs. feta, and drizzle with balsamic vinegar.', u'spoonacularSourceUrl': u'https://spoonacular.com/chickpea-and-feta-salad-758873', u'dishTypes': [u'side dish', u'salad'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}