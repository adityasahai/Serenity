{u'recipes': [{u'sourceName': u'BBC Good Food', u'cuisines': [u'european', u'english', u'british', u'scottish'], u'image': u'https://spoonacular.com/recipeImages/222267-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 12, u'id': 222267, u'veryPopular': False, u'lowFodmap': False, u'title': u'Buttermilk & sultana scones', u'diets': [], u'preparationMinutes': 20, u'pricePerServing': 84.37, u'creditText': u'BBC Good Food', u'extendedIngredients': [{u'originalString': u'1 tbsp gluten-free baking powder', u'aisle': u'Baking', u'name': u'baking powder', u'metaInformation': [u'gluten-free'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 18371, u'unit': u'tbsp'}, {u'originalString': u'100g butter, diced', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [u'diced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 100.0, u'consistency': u'solid', u'id': 1001, u'unit': u'g'}, {u'originalString': u'450g gluten-free self-raising flour blend, plus extra for sprinkling', u'aisle': u'Baking', u'name': u'cake flour blend', u'metaInformation': [u'gluten-free', u'for sprinkling'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 450.0, u'consistency': u'solid', u'id': 10020129, u'unit': u'g'}, {u'originalString': u'85g golden caster sugar, plus extra for sprinkling', u'aisle': u'Baking', u'name': u'golden brown sugar', u'metaInformation': [u'for sprinkling'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 85.0, u'consistency': u'solid', u'id': 19334, u'unit': u'g'}, {u'originalString': u'100ml milk, plus extra for brushing', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'milk', u'metaInformation': [u'for brushing'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'unitLong': u'milliliters', u'unitShort': u'ml', u'amount': 100.0, u'consistency': u'liquid', u'id': 1077, u'unit': u'ml'}, {u'originalString': u'284ml pot buttermilk', u'aisle': u'Meat', u'name': u'pot roast', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pot-roast.jpg', u'unitLong': u'milliliters', u'unitShort': u'ml', u'amount': 284.0, u'consistency': u'solid', u'id': 23612, u'unit': u'ml'}, {u'originalString': u'50g sultanas, plumped up in boiling water for 10 mins, then drained', u'aisle': u'Dried Fruits;Produce;Baking', u'name': u'sultanas', u'metaInformation': [u'boiling', u'drained', u'for 10 mins, then '], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/golden-raisins.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 50.0, u'consistency': u'liquid', u'id': 9297, u'unit': u'g'}, {u'originalString': u'2 tsp xanthan gum', u'aisle': u'Gluten Free;Health Foods;Baking', u'name': u'xanthan gum', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 2.0, u'consistency': u'solid', u'id': 93626, u'unit': u'tsp'}], u'healthScore': 5.0, u'imageType': u'jpg', u'spoonacularScore': 46.0, u'sourceUrl': u'http://www.bbcgoodfood.com/recipe/buttermilk-sultana-scones', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 13, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 220.0, u'unit': u'Celsius'}, u'name': u'oven'}], u'step': u'Heat oven to 220C/200C fan/gas 7 andlightly flour a large baking sheet. Tip theflour into a large bowl and stir in thebaking powder, xanthan gum, sugarand \xbd tsp salt.Rub the butter into the flour mixturewith your fingertips until it is completelyincorporated, then add the sultanas. Stirthe buttermilk and milk together, thenpour into the flour mixture. Stir in withthe blade of a knife to make a soft dough.Tip onto a lightly floured work surfaceand pat out with your hands until about4cm thick. Don\u2019t knead the mixture asthis will make a heavy scone, and if themixture seems a little too wet, leave fora few mins, as gluten-free flour requiresmore liquid than wheat flour. Stamp outrounds using a 7cm floured cutter, thenplace the scones on the baking tray,spaced apart. You will need to lightlysquash the dough trimmings togetherto give you 8-9 scones in total.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 93626, u'name': u'xanthan gum'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/golden-raisins.jpg', u'id': 9297, u'name': u'golden raisins'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [], u'step': u'Brush thetops of the scones with milk, sprinklewith sugar and bake for 10-12 mins untilpale golden.', u'length': {u'number': 12, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [], u'step': u'Serve with butter and jam,or push the boat out with clotted creamand strawberries, too. Best eaten on theday they are made.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are my top picks for Scone. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'nutty , butterscotch , caramel', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 70, u'instructions': u'Heat oven to 220C/200C fan/gas 7 andlightly flour a large baking sheet. Tip theflour into a large bowl and stir in thebaking powder, xanthan gum, sugarand \xbd tsp salt.Rub the butter into the flour mixturewith your fingertips until it is completelyincorporated, then add the sultanas. Stirthe buttermilk and milk together, thenpour into the flour mixture. Stir in withthe blade of a knife to make a soft dough.Tip onto a lightly floured work surfaceand pat out with your hands until about4cm thick. Don\u2019t knead the mixture asthis will make a heavy scone, and if themixture seems a little too wet, leave fora few mins, as gluten-free flour requiresmore liquid than wheat flour. Stamp outrounds using a 7cm floured cutter, thenplace the scones on the baking tray,spaced apart. You will need to lightlysquash the dough trimmings togetherto give you 8-9 scones in total. Brush thetops of the scones with milk, sprinklewith sugar and bake for 10-12 mins untilpale golden. Serve with butter and jam,or push the boat out with clotted creamand strawberries, too. Best eaten on theday they are made.', u'spoonacularSourceUrl': u'https://spoonacular.com/buttermilk-sultana-scones-222267', u'dishTypes': [u'morning meal', u'brunch', u'breakfast'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 32, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}