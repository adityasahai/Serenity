{u'recipes': [{u'sourceName': u'Tori Avey', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/533534-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 20, u'id': 533534, u'veryPopular': False, u'lowFodmap': False, u'title': u'Mushroom Harissa & Goat Cheese Frittata', u'diets': [], u'preparationMinutes': 10, u'pricePerServing': 206.27, u'creditText': u'Tori Avey', u'extendedIngredients': [{u'originalString': u'10-inch seasoned cast iron skillet or nonstick skillet with oven-safe handle', u'aisle': u'Pasta and Rice', u'name': u'burger skillet', u'metaInformation': [u'with oven-safe handle'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/no.jpg', u'unitLong': u'inches', u'unitShort': u'inch', u'amount': 10.0, u'consistency': u'solid', u'id': 99037, u'unit': u'inch'}, {u'originalString': u'6 eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 6.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'1 tbsp extra virgin olive oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'extra virgin olive oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 1034053, u'unit': u'tbsp'}, {u'originalString': u'8 oz. fresh sliced mushrooms', u'aisle': u'Produce', u'name': u'fresh mushrooms', u'metaInformation': [u'fresh', u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mushrooms.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 8.0, u'consistency': u'solid', u'id': 11260, u'unit': u'oz'}, {u'originalString': u'1/3 cup chopped fresh parsley', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh parsley', u'metaInformation': [u'fresh', u'chopped'], u'image': u'https:/