{u'recipes': [{u'sourceName': u'Go Dairy Free', u'cuisines': [u'european', u'english', u'british', u'scottish'], u'image': u'https://spoonacular.com/recipeImages/676050-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 0, u'id': 676050, u'veryPopular': False, u'lowFodmap': False, u'title': u'Cake Batter Pudding Parfaits', u'diets': [u'lacto ovo vegetarian'], u'preparationMinutes': 15, u'pricePerServing': 285.73, u'creditText': u'Go Dairy Free', u'extendedIngredients': [{u'originalString': u'3 tablespoons dairy-free rainbow sprinkles, divided (Michele uses Wilton brand)', u'aisle': u'Cheese;Health Foods', u'name': u'Dairy Free Cheese', u'metaInformation': [u'divided', u'(Michele uses Wilton brand)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 93701, u'unit': u'tablespoons'}, {u'originalString': u'1 (9-ounce) container So Delicious Dairy Free CocoWhip, thawed', u'aisle': u'Cheese;Health Foods', u'name': u'Dairy Free Cheese', u'metaInformation': [u'thawed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 9.0, u'consistency': u'solid', u'id': 93701, u'unit': u'ounce'}, {u'originalString': u'1 (11-ounce) container So Delicious Dairy Free Original Culinary Coconut Milk', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'dairy milk', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 11.0, u'consistency': u'liquid', u'id': 1077, u'unit': u'ounce'}, {u'originalString': u'1 (3.4-ounce) box dairy-free instant vanilla pudding mix (Michele uses Jell-O brand)', u'aisle': u'Baking', u'name': u'fat-free sugar-free vanilla instant pudding mix', u'metaInformation': [u'(Michele uses Jell-O brand)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/instant-vanilla-pudding.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 3.4, u'consistency': u'solid', u'id': 99022, u'unit': u'ounce'}, {u'originalString': u'\xbc cup dairy-free yellow cake mix (Michele uses Duncan Hines)', u'aisle': u'Baking', u'name': u'gluten-free white cake mix', u'metaInformation': [u'yellow', u'(Michele uses Duncan Hines)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-cake-mix.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 99041, u'unit': u'cup'}], u'healthScore': 1.0, u'imageType': u'jpg', u'spoonacularScore': 20.0, u'sourceUrl': u'http://www.godairyfree.org/recipes/cake-batter-pudding-parfaits', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 14, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Whisk together the dry pudding mix and dry cake mix in a large mixing bowl.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/plastic-bag.jpg', u'id': 221671, u'name': u'ziploc bags'}], u'step': u'Add the coconut milk and beat with an electric mixer for 5 to 7 minutes, or until the pudding is creamy and lighter in color. Gently stir in 2 tablespoons sprinkles.Spoon the pudding into a zip-top plastic bag, and cut off a corner of the bag. Spoon the Coco Whip into another zip-top plastic bag, and cut off a corner.Layer the pudding and Coco Whip in parfait glasses, ending with a dollop of Coco Whip. Sprinkle the parfaits with the remaining sprinkles.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Serve immediately or refrigerate for later.', u'number': 3, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Parfait works really well with Cream Sherry, Moscato d'Asti, and Port. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 335, u'instructions': u'Whisk together the dry pudding mix and dry cake mix in a large mixing bowl. Add the coconut milk and beat with an electric mixer for 5 to 7 minutes, or until the pudding is creamy and lighter in color. Gently stir in 2 tablespoons sprinkles.Spoon the pudding into a zip-top plastic bag, and cut off a corner of the bag. Spoon the Coco Whip into another zip-top plastic bag, and cut off a corner.Layer the pudding and Coco Whip in parfait glasses, ending with a dollop of Coco Whip. Sprinkle the parfaits with the remaining sprinkles. Serve immediately or refrigerate for later.', u'spoonacularSourceUrl': u'https://spoonacular.com/cake-batter-pudding-parfaits-676050', u'dishTypes': [], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 15, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}