{u'recipes': [{u'sourceName': u'Pink When', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/715595-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 20, u'id': 715595, u'veryPopular': True, u'lowFodmap': False, u'title': u'Cheesiest Bowtie Mac and Cheese', u'diets': [u'lacto ovo vegetarian'], u'preparationMinutes': 15, u'pricePerServing': 71.81, u'creditText': u'Jen West', u'extendedIngredients': [{u'originalString': u'1 package of Bowtie pasta (cook as directed)', u'aisle': u'Pasta and Rice', u'name': u'bowtie pasta', u'metaInformation': [u'(cook as directed)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/farfalle.png', u'unitLong': u'package', u'unitShort': u'pkg', u'amount': 1.0, u'consistency': u'solid', u'id': 10120420, u'unit': u'package'}, {u'originalString': u'1 package Cracker Barrel Extra Sharp Cheddar', u'aisle': u'Cheese', u'name': u'extra sharp cheddar cheese', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'package', u'unitShort': u'pkg', u'amount': 1.0, u'consistency': u'solid', u'id': 1021009, u'unit': u'package'}, {u'originalString': u'1 package Cracker Barrel Extra Sharp White Cheddar', u'aisle': u'Cheese', u'name': u'extra sharp cheddar cheese', u'metaInformation': [u'white'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cheddar-cheese.jpg', u'unitLong': u'package', u'unitShort': u'pkg', u'amount': 1.0, u'consistency': u'solid', u'id': 1021009, u'unit': u'package'}, {u'originalString': u'2 tbsp Long Leaf Oregano', u'aisle': u'Produce;Spices and Seasonings', u'name': u'oregano', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oregano.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 2027, u'unit': u'tbsp'}, {u'originalString': u'1 tsp salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2047, u'unit': u'tsp'}, {u'originalString': u'\xbc cup sour cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'sour cream', u'metaInformation': [u'sour'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 1056, u'unit': u'cup'}, {u'originalString': u'\xbc cup unsalted butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 1145, u'unit': u'cup'}], u'healthScore': 8.0, u'imageType': u'jpg', u'spoonacularScore': 65.0, u'sourceUrl': u'http://www.pinkwhen.com/mac-and-cheese-recipe/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'dishTypes': [u'side dish', u'lunch', u'main course', u'main dish', u'dinner'], u'weightWatcherSmartPoints': 18, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Heat your oven to 350, and then cook your pasta as directed.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fusilli.jpg', u'id': 20420, u'name': u'pasta'}]}], u'name': u''}, {u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/casserole-dish.png', u'id': 404635, u'name': u'casserole dish'}], u'step': u'Bake in small casserole dish for 20 minutes on 350.', u'length': {u'number': 20, u'unit': u'minutes'}, u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Serve with a piping hot bowl of potato soup, sandwich, or your favorite dish.', u'number': 2, u'ingredients': []}], u'name': u'Drain.Step 2 With noodles still in sauce pan, stir in butter, salt, sour cream, and cheese (cut in small blocks to easily melt).Step 3 Stir until mixed well and cheese is melted, then stir in Oregano.Step 4'}], u'winePairing': {u'pairingText': u"Macaroni and Cheese works really well with Chenin Blanc, Lambrusco, and Riesling. With rich, cheesy mac & cheese you'll want to cleanse the palate with something crisp and acidic. We recommend Chenin Blanc, Riesling, or dry Lambrusco if you're in the mood for a red. The Essay Chenin Blanc with a 4.2 out of 5 star rating seems like a good match. It costs about 9 dollars per bottle.", u'pairedWines': [u'chenin blanc', u'lambrusco', u'riesling'], u'productMatches': [{u'description': u"The Essay Chenin Blanc is a medium-bodied white blend made from South Africa's white varietal, Chenin Blanc. The Chenin Blanc gives the wines its fruit salad, guava and melon aromas and a refreshing acidity. Some sur lie on the less for a few months adds to the complexity and body. A touch of aromatic Viognier complements the fruit structure with some floral hints. The wine is perfect on its own as an aperitif on the porch on a hot summer's day. Chenin Blanc pairs well with a wide range of foods, especially sushi, oysters, Asian curries, sweet-and-sour dishes, and summer salads.Blend: 87% Chenin Blanc, 13% Viognier", u'title': u'Essay Chenin Blanc', u'price': u'$8.99', u'id': 439943, u'link': u'https://www.wine.com/product/essay-chenin-blanc-2014/139942', u'imageUrl': u'https://spoonacular.com/productImages/439943-312x231.jpg'}]}, u'aggregateLikes': 401, u'instructions': u'Step 1: Heat your oven to 350, and then cook your pasta as directed. Drain.Step 2: With noodles still in sauce pan, stir in butter, salt, sour cream, and cheese (cut in small blocks to easily melt).Step 3: Stir until mixed well and cheese is melted, then stir in Oregano.Step 4: Bake in small casserole dish for 20 minutes on 350. Serve with a piping hot bowl of potato soup, sandwich, or your favorite dish.', u'spoonacularSourceUrl': u'https://spoonacular.com/cheesiest-bowtie-mac-and-cheese-715595', u'license': u'CC BY-SA 3.0', u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 35, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}