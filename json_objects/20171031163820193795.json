ion': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ghee.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 93632, u'unit': u'tbsp'}, {u'originalString': u'3 ginger julienne for garnishing', u'aisle': u'Produce', u'name': u'ginger', u'metaInformation': [u'for garnishing'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ginger.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11216, u'unit': u''}, {u'originalString': u'a pinch of asafoetida/hing', u'aisle': u'Spices and Seasonings', u'name': u'hing', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/curry-powder.jpg', u'unitLong': u'pinch', u'unitShort': u'pinch', u'amount': 1.0, u'consistency': u'solid', u'id': 1032035, u'unit': u'pinch'}, {u'originalString': u'1.5 tbsp besan/gram flour or makai ka atta/maize flour', u'aisle': u'Ethnic Foods;Baking', u'name': u'maize flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/corn-flour.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 1.5, u'consistency': u'solid', u'id': 20019, u'unit': u'tbsp'}, {u'originalString': u'1 medium sized onion, finely chopped', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'medium sized', u'finely chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u''}, {u'originalString': u'3 medium sized potatoes/aloo', u'aisle': u'Produce', u'name': u'potatoes', u'metaInformation': [u'medium sized'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/potatoes-yukon-gold.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11362, u'unit': u''}, {u'originalString': u'salt as required', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 3.0, u'consistency': u'solid', u'id': 2047, u'unit': u'servings'}, {u'originalString': u'1 bunch palak/spinach', u'aisle': u'Produce', u'name': u'spinach', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'unitLong': u'bunch', u'unitShort': u'bunch', u'amount': 1.0, u'consistency': u'solid', u'id': 10011457, u'unit': u'bunch'}, {u'originalString': u'1 medium sized tomato, chopped', u'aisle': u'Produce', u'name': u'tomato', u'metaInformation': [u'medium sized', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11529, u'unit': u''}, {u'originalString': u'\xbc tsp turmeric powder/haldi', u'aisle': u'Spices and Seasonings', u'name': u'turmeric powder', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/turmeric.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2043, u'unit': u'tsp'}, {u'originalString': u'1 cup water', u'aisle': u'Beverages', u'name': u'water', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'liquid', u'id': 14412, u'unit': u'cup'}], u'healthScore': 100.0, u'imageType': u'jpg', u'spoonacularScore': 100.0, u'sourceUrl': u'http://www.vegrecipesofindia.com/aloo-palak/', u'glutenFree': True, u'vegan': False, u'veryHealthy': True, u'weightWatcherSmartPoints': 9, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pressure-cooker.jpg', u'id': 404658, u'name': u'pressure cooker'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/wok.png', u'id': 404666, u'name': u'wok'}], u'step': u'boil the potatoes or aloos in a pressure cooker or steamer till the are completely cooked.peel and quarter or dice them. keep aside covered.boil 2 to 3 cups water with some salt.switch off the fire and immediately add the palak or spinach.blanch them in the water for 4-5 minutes.drain and immediately place the palak in cold water for 3-4 minutes.drain again and along with green chilies, make a smooth puree in a grinder or blender.heat oil or ghee or butter in a pan or kadai/wok.fry the bay leaf, cloves and cinnamon till they become aromatic.add the chopped onion and fry till light brown.now add the ginger-garlic paste and fry till the raw aroma goes away.add the tomatoes and fry till the tomatoes become soft and mushy.the oil should start releasing from the mixture.now add the turmeric powder and asafoetida and stir for 5-10 seconds.add the palak puree. stir well.add the besan or gram flour. stir with a wired whisk so that the besan dissolves and there are no lumps.alternatively, you can also dissolve the besan in 3-4 tbsp water and make a smooth paste.then add this paste to the palak mixture. stir and add 1 cup water. season with salt.simmer the palak gravy till the palak gets completely cooked.the gravy or sauce will slightly thicken.you can adjust the consistency by adding more or less water.add the aloos and simmer for 2-3 minutes more.sprinkle the garam masala powder & crushed kasuri methi leaves. stir and cook for a minute.serve aloo palak hot with some rotis, chapatis or parathas.', u'length': {u'number': 12, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/turmeric.jpg', u'id': 2043, u'name': u'turmeric'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garam-masala.jpg', u'id': 93663, u'name': u'garam masala'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/curry-powder.jpg', u'id': 1032035, u'name': u'asafoetida'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bay-leaves.jpg', u'id': 2004, u'name': u'bay leaves'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'id': 2010, u'name': u'cinnamon'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/potatoes-yukon-gold.jpg', u'id': 11362, u'name': u'potato'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato.jpg', u'id': 11529, u'name': u'tomato'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/water.jpg', u'id': 14412, u'name': u'water'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ghee.jpg', u'id': 93632, u'name': u'ghee'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'Asian works really well with Chenin Blanc, Gewurztraminer, and Riesling. The best wine for Asian food depends on the cuisine and dish - of course - but these acidic whites pair with a number of traditional meals, spicy or not. One wine you could try is Essay Chenin Blanc. It has 4.2 out of 5 stars and a bottle costs about 9 dollars.', u'pairedWines': [u'chenin blanc', u'gewurztraminer', u'riesling'], u'productMatches': [{u'description': u"The Essay Chenin Blanc is a medium-bodied white blend made from South Africa's white varietal, Chenin Blanc. The Chenin Blanc gives the wines its fruit salad, guava and melon aromas and a refreshing acidity. Some sur lie on the less for a few months adds to the complexity and body. A touch of aromatic Viognier complements the fruit structure with some floral hints. The wine is perfect on its own as an aperitif on the porch on a hot summer's day. Chenin Blanc pairs well with a wide range of foods, especially sushi, oysters, Asian curries, sweet-and-sour dishes, and summer salads.Blend: 87% Chenin Blanc, 13% Viognier", u'title': u'Essay Chenin Blanc', u'price': u'$8.99', u'id': 439943, u'link': u'https://www.wine.com/product/essay-chenin-blanc-2014/139942', u'imageUrl': u'https://spoonacular.com/productImages/439943-312x231.jpg'}]}, u'aggregateLikes': 536, u'instructions': u'boil the potatoes or aloos in a pressure cooker or steamer till the are completely cooked.peel and quarter or dice them. keep aside covered.boil 2 to 3 cups water with some salt.switch off the fire and immediately add the palak or spinach.blanch them in the water for 4-5 minutes.drain and immediately place the palak in cold water for 3-4 minutes.drain again and along with green chilies, make a smooth puree in a grinder or blender.heat oil or ghee or butter in a pan or kadai/wok.fry the bay leaf, cloves and cinnamon till they become aromatic.add the chopped onion and fry till light brown.now add the ginger-garlic paste and fry till the raw aroma goes away.add the tomatoes and fry till the tomatoes become soft and mushy.the oil should start releasing from the mixture.now add the turmeric powder and asafoetida and stir for 5-10 seconds.add the palak puree. stir well.add the besan or gram flour. stir with a wired whisk so that the besan dissolves and there are no lumps.alternatively, you can also dissolve the besan in 3-4 tbsp water and make a smooth paste.then add this paste to the palak mixture. stir and add 1 cup water. season with salt.simmer the palak gravy till the palak gets completely cooked.the gravy or sauce will slightly thicken.you can adjust the consistency by adding more or less water.add the aloos and simmer for 2-3 minutes more.sprinkle the garam masala powder & crushed kasuri methi leaves. stir and cook for a minute.serve aloo palak hot with some rotis, chapatis or parathas.', u'spoonacularSourceUrl': u'https://spoonacular.com/aloo-palak-how-to-make-aloo-palak-aloo-palak-curry-486890', u'dishTypes': [], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 3, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}