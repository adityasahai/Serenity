re.", u'spoonacularSourceUrl': u'https://spoonacular.com/healthy-delicious-yellow-tomato-salad-with-roasted-red-pepper-feta-and-mint-198758', u'dishTypes': [u'side dish', u'salad'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 2, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}