{u'recipes': [{u'sourceName': u'Gluten Free Recipe Box', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/615904-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 615904, u'veryPopular': False, u'lowFodmap': False, u'title': u'Gluten Free Beef Stew in Red Wine Sauce', u'diets': [u'gluten free'], u'pricePerServing': 260.58, u'creditText': u'Gluten Free Recipe Box', u'extendedIngredients': [{u'originalString': u'2 bay leaves', u'aisle': u'Produce;Spices and Seasonings', u'name': u'bay leaves', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bay-leaves.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 2004, u'unit': u''}, {u'originalString': u'4 cups gluten free beef broth (Better Than Bouillon)', u'aisle': u'Canned and Jarred', u'name': u'beef broth', u'metaInformation': [u'gluten free', u'(Better Than Bouillon)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/beef-broth.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 4.0, u'consistency': u'liquid', u'id': 6008, u'unit': u'cups'}, {u'originalString': u'5.75 lbs. beef stew meat, cut into 1-inch pieces', u'aisle': u'Meat', u'name': u'beef stew meat', u'metaInformation': [u'cut into 1-inch pieces'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/beef-cubes-raw.png', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 5.75, u'consistency': u'solid', u'id': 10013411, u'unit': u'lbs'}, {u'originalString': u'2 Tablespoons butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'Tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 1001, u'unit': u'Tablespoons'}, {u'originalString': u'1/2 lb. button mushrooms, sliced (optional)', u'aisle': u'Produce', u'name': u'button mushrooms', u'metaInformation': [u'sliced'], u'image': u'ht