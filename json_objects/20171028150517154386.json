eryHealthy': False, u'weightWatcherSmartPoints': 2, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-or-cupcake-forms.png', u'id': 404676, u'name': u'muffin liners'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 375.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 375 degrees F, and grease up a standard 12-cup muffin tin.In a large mixing bowl, whisk together the eggs, cream, and salsa. Stir in the rest of the ingredients and divide mixture into muffin cups.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salsa.jpg', u'id': 6164, u'name': u'salsa'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}], u'step': u'Bake for about 20 minutes or until cooked all the way through. (A knife inserted into the center should come out clean. Like when you check a quiche.)', u'length': {u'number': 20, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Serve with extra salsa and chopped avocado.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salsa.jpg', u'id': 6164, u'name': u'salsa'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Sparkling Wine are great choices for Omelet. Even if you aren't making mimosas, sparkling wine is great with eggs for two reasons. One, if you're eating eggs early in the day, sparkling wine has less alcohol. Secondly, it cleanses the palate, which is important since yolk is known to coat the palate. The Perrier-Jouet Nuit Blanche with a 4.9 out of 5 star rating seems like a good match. It costs about 68 dollars per bottle.", u'pairedWines': [u'sparkling wine'], u'productMatches': [{u'description': u'The sweeter taste, combined with the rounded and smooth mouth feel give the wine, a full body and richness that makes Perrier-Jou\xebt Nuit Blanche the perfect champagne to awaken the senses.Perrier-Jou\xebt Nuit Blanche is an exceptional Non-Vintage blend of 20% Chardonnay, 40% Pinot Noir and 40% Pinot Meunier, sweetened to taste with a 20g/l dosage of cane sugar to create a sensual style.   View More', u'title': u'Perrier-Jouet Nuit Blanche', u'price': u'$67.99', u'id': 461023, u'link': u'https://www.wine.com/product/perrier-jouet-nuit-blanche/137331', u'imageUrl': u'https://spoonacular.com/productImages/461023-312x231.jpg'}]}, u'aggregateLikes': 17, u'instructions': u'Preheat oven to 375 degrees F, and grease up a standard 12-cup muffin tin.In a large mixing bowl, whisk together the eggs, cream, and salsa. Stir in the rest of the ingredients and divide mixture into muffin cups. Bake for about 20 minutes or until cooked all the way through. (A knife inserted into the center should come out clean. Like when you check a quiche.)Serve with extra salsa and chopped avocado.', u'spoonacularSourceUrl': u'https://spoonacular.com/southwest-omelet-cups-556967', u'dishTypes': [u'morning meal', u'brunch', u'breakfast'], u'occasions': [], u'vegetarian': True, u'ketogenic': True, u'readyInMinutes': 30, u'servings': 12, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}