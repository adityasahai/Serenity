{u'recipes': [{u'sourceName': u'Tori Avey', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/533631-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 533631, u'veryPopular': False, u'lowFodmap': False, u'title': u'What the Colonial Virginians Ate \u2013 Apple Tansey', u'diets': [], u'pricePerServing': 93.89, u'creditText': u'Tori Avey', u'extendedIngredients': [{u'originalString': u'3 small apples (or 2 large)', u'aisle': u'Produce', u'name': u'apples', u'metaInformation': [u'small', u'large', u'(or 2 )'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 9003, u'unit': u''}, {u'originalString': u'a seasoned cast iron or nonstick skillet', u'aisle': u'Pasta and Rice', u'name': u'burger skillet', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/no.jpg', u'unitLong': u'stick', u'unitShort': u'stick', u'amount': 1.0, u'consistency': u'solid', u'id': 99037, u'unit': u'stick'}, {u'originalString': u'4 large eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 4.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'2 tbsp (6 tsp) heavy whipping cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'heavy whipping cream', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 6.0, u'consistency': u'liquid', u'id': 1053, u'unit': u'tsp'}, {u'originalString': u'Fresh lemon wedges for garnish', u'aisle': u'Produce', u'name': u'lemon wedges', u'metaInformation': [u'fresh', u'for garnish'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon-wedge.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 4.0, u'consistency': u'solid', u'id': 9150, u'unit': u'servings'}, {u'originalString': u'1/4 tsp nutmeg', u'aisle': u'Spices and Seasonings', u'name': u'nutmeg', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2025, u'unit': u'tsp'}, {u'originalString': u'Granulated or powdered sugar for garnish', u'aisle': u'Baking', u'name': u'powdered sugar', u'metaInformation': [u'for garnish'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 4.0, u'consistency': u'solid', u'id': 19336, u'unit': u'servings'}, {u'originalString': u'2 tsp rosewater', u'aisle': u'Gourmet', u'name': u'rosewater', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rose-water.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 2.0, u'consistency': u'solid', u'id': 10411111, u'unit': u'tsp'}, {u'originalString': u'2 tbsp sugar', u'aisle': u'Baking', u'name': u'sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 19335, u'unit': u'tbsp'}, {u'originalString': u'3 tbsp unsalted butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 1145, u'unit': u'tbsp'}], u'healthScore': 2.0, u'imageType': u'jpg', u'spoonacularScore': 22.0, u'sourceUrl': u'http://toriavey.com/history-kitchen/2011/03/what-the-colonial-virginians-ate-2/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 10, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 405914, u'name': u'broiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat your oven\u2019s broiler. Core the apples with a coring tool...then slice them into thin rounds. I left the skin on the apples because the recipe doesn\u2019t specify to \u201cpare\u201d them. If you prefer, you can peel the apples before slicing.Melt the butter over medium heat until hot, being careful not to let the butter brown or burn.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'id': 9003, u'name': u'apple'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Add apple slices to the skillet. Fry them for about 5 minutes, turning once, until they soften and begin browning at the edges.While apples fry, beat the eggs together with the cream, rosewater, nutmeg, and 2 tbsp sugar.When apples are ready, pour the egg mixture evenly over the top of the apples. Cook the tansey for about 3 minutes till the bottom solidifies.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rose-water.jpg', u'id': 10411111, u'name': u'rose water'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'id': 9003, u'name': u'apple'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'id': 2025, u'name': u'nutmeg'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 405914, u'name': u'broiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Place skillet in the oven under the broiler.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven-mitt.jpg', u'id': 404735, u'name': u'oven mitt'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Let it cook for 2-3 minutes longer till the egg mixture is cooked through. Use an oven mitt to remove the skillet.Turn the apple tansey onto a large flat plate. Sprinkle it with sugar and splash it with fresh lemon juice.', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'id': 9003, u'name': u'apple'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [], u'step': u'Serve garnished with lemon wedges or sliced lemon rounds, if desired.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon-wedge.jpg', u'id': 9150, u'name': u'lemon wedge'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lemon.jpg', u'id': 9150, u'name': u'lemon'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 10, u'instructions': u'Preheat your oven\u2019s broiler. Core the apples with a coring tool...then slice them into thin rounds. I left the skin on the apples because the recipe doesn\u2019t specify to \u201cpare\u201d them. If you prefer, you can peel the apples before slicing.Melt the butter over medium heat until hot, being careful not to let the butter brown or burn. Add apple slices to the skillet. Fry them for about 5 minutes, turning once, until they soften and begin browning at the edges.While apples fry, beat the eggs together with the cream, rosewater, nutmeg, and 2 tbsp sugar.When apples are ready, pour the egg mixture evenly over the top of the apples. Cook the tansey for about 3 minutes till the bottom solidifies.Place skillet in the oven under the broiler. Let it cook for 2-3 minutes longer till the egg mixture is cooked through. Use an oven mitt to remove the skillet.Turn the apple tansey onto a large flat plate. Sprinkle it with sugar and splash it with fresh lemon juice.Serve garnished with lemon wedges or sliced lemon rounds, if desired.', u'spoonacularSourceUrl': u'https://spoonacular.com/what-the-colonial-virginians-ate-apple-tansey-533631', u'dishTypes': [u'side dish'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}