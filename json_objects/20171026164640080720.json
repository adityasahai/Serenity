ation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/honey.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 19296, u'unit': u'tbsp'}, {u'originalString': u'Hexagon Silicone Ice cube tray', u'aisle': u'Frozen', u'name': u'ice cube', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ice-cubes.jpg', u'unitLong': u'cube', u'unitShort': u'cube', u'amount': 1.0, u'consistency': u'solid', u'id': 10014412, u'unit': u'cube'}, {u'originalString': u'\xbc tsp of sea salt', u'aisle': u'Spices and Seasonings', u'name': u'sea salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 1012047, u'unit': u'tsp'}, {u'originalString': u'1 tbsp of tapioca flour', u'aisle': u'Ethnic Foods;Health Foods;Baking', u'name': u'tapioca flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 93696, u'unit': u'tbsp'}, {u'originalString': u'\xbd tsp of vanilla', u'aisle': u'Baking', u'name': u'vanilla', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 2050, u'unit': u'tsp'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 2.0, u'sourceUrl': u'http://www.agirlworthsaving.net/2013/10/chocolate-covered-cherries.html', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 10, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/ice-cube-tray.jpg', u'id': 406911, u'name': u'ice cube tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/potato-masher.jpg', u'id': 404763, u'name': u'potato masher'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}], u'step': u'First, place 14 cherries in the silicone ice cube tray. Set aside.Next, place 20 cherries, the coconut oil, honey, sea salt, vanilla and honey in a medium sauce pan and bring to a boil.Mash the cherries with a potato masher to help break them up.Cook for 3 minutes and then add in the tapioca four. Cook for one minute. Break out your stick blender or pour the mixture unto your blender and pulse to for 1 minute or until the filling is completely smooth and blended.', u'length': {u'number': 4, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oil-coconut.jpg', u'id': 4047, u'name': u'coconut oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cherries.jpg', u'id': 9070, u'name': u'cherries'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ice-cubes.jpg', u'id': 10014412, u'name': u'ice cubes'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 1012047, u'name': u'sea salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/honey.jpg', u'id': 19296, u'name': u'honey'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/ice-cube-tray.jpg', u'id': 406911, u'name': u'ice cube tray'}], u'step': u'Pour the filling over the cherries in the ice cube tray.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cherries.jpg', u'id': 9070, u'name': u'cherries'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ice-cubes.jpg', u'id': 10014412, u'name': u'ice cubes'}]}, {u'equipment': [], u'step': u'Place in your freezer for 8 hours.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}], u'step': u'Remove the frozen cherry filling from the tray and set back in the freezer until you are ready to coat them.In a saucepan melt 1 cup of the dark chocolate chips.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cherries.jpg', u'id': 9070, u'name': u'cherries'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404794, u'name': u'stove'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Remove from the heat and then drop the cherry bites in, one at a time, and coat with the chocolate on all sides. TIP: The chocolate will start to firm up after a few cherries have been coated. Set the pan back on the stove top and add more chocolate chips until it is a smooth, liquidy consistency and then let cool for 30 seconds before you continue the process. You will go through the whole bag of chips.Set on a piece of parchment paper and when complete, move them to the fridge.Enjoy!', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chocolate-chips.jpg', u'id': 10019146, u'name': u'chocolate chips'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cherries.jpg', u'id': 9070, u'name': u'cherries'}]}], u'name': u''}], u'aggregateLikes': 159, u'instructions': u'First, place 14 cherries in the silicone ice cube tray. Set aside.Next, place 20 cherries, the coconut oil, honey, sea salt, vanilla and honey in a medium sauce pan and bring to a boil.Mash the cherries with a potato masher to help break them up.Cook for 3 minutes and then add in the tapioca four. Cook for one minute. Break out your stick blender or pour the mixture unto your blender and pulse to for 1 minute or until the filling is completely smooth and blended.Pour the filling over the cherries in the ice cube tray.Place in your freezer for 8 hours.Remove the frozen cherry filling from the tray and set back in the freezer until you are ready to coat them.In a saucepan melt 1 cup of the dark chocolate chips. Remove from the heat and then drop the cherry bites in, one at a time, and coat with the chocolate on all sides. TIP: The chocolate will start to firm up after a few cherries have been coated. Set the pan back on the stove top and add more chocolate chips until it is a smooth, liquidy consistency and then let cool for 30 seconds before you continue the process. You will go through the whole bag of chips.Set on a piece of parchment paper and when complete, move them to the fridge.Enjoy!', u'spoonacularSourceUrl': u'https://spoonacular.com/chocolate-covered-cherries-541919', u'dishTypes': [u'side dish'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 570, u'servings': 14, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}