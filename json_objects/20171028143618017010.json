edInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Combine the tomatoes, red onion, jalapeno, lime juice, and salt. Gently stir in avocado chunks and cilantro. Set aside while you prepare the burgers.Prepare the Southwest Burgers: In a large bowl, combine the ground beef, jalapeno hot sauce, cumin, oregano, garlic powder, coriander, chili powder, and sweet paprika.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/green-hot-sauce.png', u'id': 1006168, u'name': u'green hot sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic-powder.jpg', u'id': 1022020, u'name': u'garlic powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/paprika.jpg', u'id': 1002028, u'name': u'sweet paprika'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chili-powder.jpg', u'id': 2009, u'name': u'chili powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'id': 10023572, u'name': u'ground beef'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lime-juice.jpg', u'id': 9160, u'name': u'lime juice'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-coriander.jpg', u'id': 2012, u'name': u'coriander'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-onion.jpg', u'id': 10011282, u'name': u'red onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cilantro.png', u'id': 11165, u'name': u'cilantro'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/jalapeno-pepper.png', u'id': 11979, u'name': u'jalapeno pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato.jpg', u'id': 11529, u'name': u'tomato'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/avocado.jpg', u'id': 9037, u'name': u'avocado'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oregano.jpg', u'id': 2027, u'name': u'oregano'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-cumin.jpg', u'id': 1002014, u'name': u'cumin'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [], u'step': u'Mix gently with your handsbut do not over mix. Form the ground beef mixture into 6-ounce burger patties (roughly -inch thick).', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'id': 10023572, u'name': u'ground beef'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill-pan.jpg', u'id': 404648, u'name': u'grill pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Heat a large cast iron grill pan (over medium-high heat, and grease lightly with cooking spray. Cook the burgers on each side until charred, roughly 4-5 minutes per side (or until the internal temperature reaches roughly 150 degrees Fahrenheit). Top each burger with 1 ounce of pepper jack cheese, tent pan with foil (or close grill top), and cook until the cheese has just melted.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper-jack-cheese.jpg', u'id': 1025, u'name': u'pepperjack cheese'}]}], u'name': u'Allow ground beef to rest at room temperature for 45 minutes to an hour.Prepare the Avocado Salsa'}, {u'steps': [{u'equipment': [], u'step': u'Place burgers on toasted buns, and add a large spoonful of avocado salsa.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/avocado.jpg', u'id': 9037, u'name': u'avocado'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salsa.jpg', u'id': 6164, u'name': u'salsa'}]}, {u'equipment': [], u'step': u'Serve immediately.', u'number': 2, u'ingredients': []}], u'name': u'Remove and allow the burgers for rest for several minutes. Meanwhile, grill or toast buns lightly, if desired (spray grill pan lightly with oil, if grilling).To Assemble'}], u'winePairing': {u'pairingText': u'Burger works really well with Malbec, Merlot, and Zinfandel. Merlot will be perfectly adequate for a classic burger with standard toppings. Bolder toppings call for bolder wines, such as a malbec or peppery zinfandel. You could try Gascon Malbec Riserva. Reviewers quite like it with a 4.4 out of 5 star rating and a price of about 21 dollars per bottle.', u'pairedWines': [u'malbec', u'merlot', u'zinfandel'], u'productMatches': [{u'description': u'Dark, rich and complex, Don Miguel Gascon Reserva Malbec is a powerful, yet fresh, wine. An elegant nose features hints of violet, blackberry and blackcurrants, with subtle aromas of chocolate mint. The wine is robust and strong on the palate, with smooth tannins and distinctive layers of dried plum, dates and cherry flavors that integrate seamlessly with notes of almond and hazelnut, backed up by a long and velvety-smooth finish. View More', u'title': u'Gascon Malbec Riserva', u'price': u'$20.99', u'id': 454160, u'link': u'https://www.wine.com/product/gascon-malbec-riserva-2014/162603', u'imageUrl': u'https://spoonacular.com/productImages/454160-312x231.jpg'}]}, u'aggregateLikes': 14, u'instructions': u'Allow ground beef to rest at room temperature for 45 minutes to an hour.Prepare the Avocado Salsa: Combine the tomatoes, red onion, jalapeno, lime juice, and salt. Gently stir in avocado chunks and cilantro. Set aside while you prepare the burgers.Prepare the Southwest Burgers: In a large bowl, combine the ground beef, jalapeno hot sauce, cumin, oregano, garlic powder, coriander, chili powder, and sweet paprika. Mix gently with your handsbut do not over mix. Form the ground beef mixture into 6-ounce burger patties (roughly -inch thick). Heat a large cast iron grill pan (over medium-high heat, and grease lightly with cooking spray. Cook the burgers on each side until charred, roughly 4-5 minutes per side (or until the internal temperature reaches roughly 150 degrees Fahrenheit). Top each burger with 1 ounce of pepper jack cheese, tent pan with foil (or close grill top), and cook until the cheese has just melted. Remove and allow the burgers for rest for several minutes. Meanwhile, grill or toast buns lightly, if desired (spray grill pan lightly with oil, if grilling).To Assemble: Place burgers on toasted buns, and add a large spoonful of avocado salsa. Serve immediately.', u'spoonacularSourceUrl': u'https://spoonacular.com/southwest-burgers-with-pepper-jack-cheese-and-avocado-salsa-626046', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': True, u'readyInMinutes': 30, u'servings': 6, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}