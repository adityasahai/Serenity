formation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lime-wedge.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'liquid', u'id': 9159, u'unit': u''}, {u'originalString': u'3/4 cup grated Manchego cheese', u'aisle': u'Cheese', u'name': u'manchego cheese', u'metaInformation': [u'grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/manchego.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.75, u'consistency': u'solid', u'id': 93821, u'unit': u'cup'}, {u'originalString': u'2 tbsp deshelled roasted pepitas', u'aisle': u'Savory Snacks', u'name': u'pepitas', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pumpkin-seeds.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 10012014, u'unit': u'tbsp'}, {u'originalString': u'pepper to taste', u'aisle': u'Spices and Seasonings', u'name': u'pepper', u'metaInformation': [u'to taste'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 2.0, u'consistency': u'solid', u'id': 1002030, u'unit': u'servings'}, {u'originalString': u'1/4 tsp salt (or less)', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [u'(or less)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2047, u'unit': u'tsp'}, {u'originalString': u'salt and pepper, to taste', u'aisle': u'Spices and Seasonings', u'name': u'salt and pepper', u'metaInformation': [u'to taste'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 2.0, u'consistency': u'solid', u'id': 1102047, u'unit': u'servings'}, {u'originalString': u'2 tilapia filets', u'aisle': u'Seafood', u'name': u'tilapia', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raw-tilapia.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 15261, u'unit': u''}, {u'originalString': u'3 medium zucchinis, Blade A', u'aisle': u'Produce', u'name': u'zucchinis', u'metaInformation': [u'medium'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/zucchini.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11477, u'unit': u''}], u'healthScore': 83.0, u'imageType': u'jpg', u'spoonacularScore': 99.0, u'sourceUrl': u'http://www.inspiralized.com/2014/02/04/roasted-jalapeno-lime-tilapia-with-cilantro-manchego-pepita-pesto-zucchini-pasta/', u'glutenFree': True, u'vegan': False, u'veryHealthy': True, u'weightWatcherSmartPoints': 17, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat the oven to 400 degrees.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}], u'step': u'Place the tilapia filets on a baking tray lightly coated with cooking spray. Slice the lime in half and squeeze one half over one filet and the other half over the other filet. Season lightly with salt and pepper.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raw-tilapia.jpg', u'id': 15261, u'name': u'tilapia'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lime.jpg', u'id': 9159, u'name': u'lime'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Bake in the oven for 7 minutes. Then, take out, place 3 jalapeno slices on top of each filet and bake for another 3-5 minutes, or until tilapia is opaque and cooked through.While the tilapia is roasting, place all of the ingredients for your pesto into a food processor. Pulse until creamy. Taste and adjust, if necessary. In a large bowl, pour the pesto over the zucchini noodles and toss to combine thoroughly. Divide the noodles evenly onto two plates.When the tilapia is done, top each bed of zucchini pasta with one filet. Enjoy!', u'length': {u'number': 12, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/zoodles.jpg', u'id': 10111477, u'name': u'zucchini noodles'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raw-tilapia.jpg', u'id': 15261, u'name': u'tilapia'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Pinot Grigio, Gruener Veltliner, and Pinot Noir are my top picks for Fish. Fish is as diverse as wine, so it's hard to pick wines that go with every fish. A crisp white wine, such as a pinot grigio or Gr\xfcner Veltliner, will suit any delicately flavored white fish. Meaty, strongly flavored fish such as salmon and tuna can even handle a light red wine, such as a pinot noir. You could try Kris Pinot Grigio. Reviewers quite like it with a 4.2 out of 5 star rating and a price of about 15 dollars per bottle.", u'pairedWines': [u'pinot grigio', u'gruener veltliner', u'pinot noir'], u'productMatches': [{u'description': u'Brilliant lemon-green in color, KRIS offers enticing aromas of acacia flowers, citrus, tangerine, and hints of almonds.Try pairing with risotto, tossed salads, homemade pasta and grilled white meats or salmon.Blend: 100% Pinot Grigio View More', u'title': u'Kris Pinot Grigio', u'price': u'$14.99', u'id': 433207, u'link': u'https://www.wine.com/product/kris-pinot-grigio-2016/206138', u'imageUrl': u'https://spoonacular.com/productImages/433207-312x231.jpg'}]}, u'aggregateLikes': 310, u'instructions': u'Preheat the oven to 400 degrees. Place the tilapia filets on a baking tray lightly coated with cooking spray. Slice the lime in half and squeeze one half over one filet and the other half over the other filet. Season lightly with salt and pepper. Bake in the oven for 7 minutes. Then, take out, place 3 jalapeno slices on top of each filet and bake for another 3-5 minutes, or until tilapia is opaque and cooked through.While the tilapia is roasting, place all of the ingredients for your pesto into a food processor. Pulse until creamy. Taste and adjust, if necessary. In a large bowl, pour the pesto over the zucchini noodles and toss to combine thoroughly. Divide the noodles evenly onto two plates.When the tilapia is done, top each bed of zucchini pasta with one filet. Enjoy!', u'spoonacularSourceUrl': u'https://spoonacular.com/roasted-jalapeno-lime-tilapia-with-cilantro-manchego-pepita-pesto-zucchini-pasta-563815', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 22, u'servings': 2, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}