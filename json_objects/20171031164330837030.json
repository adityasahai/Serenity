{u'recipes': [{u'sourceName': u'Crazy for Crust', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/540499-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 540499, u'veryPopular': True, u'lowFodmap': False, u'title': u'chocolate stuffed Peanut Butter Monkey Bread Loaf', u'diets': [], u'pricePerServing': 59.97, u'creditText': u'Crazy for Crust', u'extendedIngredients': [{u'originalString': u'1 8-count package Grands! Biscuits (I used Homestyle Buttermilk)', u'aisle': u'Refrigerated', u'name': u'biscuits', u'metaInformation': [u'(I used Homestyle Buttermilk)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/buttermilk-biscuits.jpg', u'unitLong': u'package', u'unitShort': u'pkg', u'amount': 1.0, u'consistency': u'solid', u'id': 18009, u'unit': u'package'}, {u'originalString': u'1/4 cup brown sugar', u'aisle': u'Baking', u'name': u'brown sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 19334, u'unit': u'cup'}, {u'originalString': u'3 tablespoons butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 1001, u'unit': u'tablespoons'}, {u'originalString': u'1/4 cup + 32 chocolate chips, divided', u'aisle': u'Baking', u'name': u'chocolate chips', u'metaInformation': [u'divided'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chocolate-chips.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 10019146, u'unit': u'cup'}, {u'originalString': u'1 teaspoon cinnamon, divided', u'aisle': u'Spices and Seasonings', u'name': u'cinnamon', u'metaInformation': [u'divided'], u'image': u'https://spoonacular.com/cdn/ingredient