u'metaInformation': [u'unsweetened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/baking-chocolate.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 2.0, u'consistency': u'solid', u'id': 19078, u'unit': u'ounces'}, {u'originalString': u'1 teaspoon pure vanilla extract', u'aisle': u'Baking', u'name': u'vanilla extract', u'metaInformation': [u'pure'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'teaspoon'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 5.0, u'sourceUrl': u'http://www.foodfanatic.com/2014/10/salted-caramel-fudge/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 9, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stand-mixer.jpg', u'id': 404665, u'name': u'stand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'In a large saucepan, combine sugar, milk, vanilla, butter and espresso powder.Bring to a boil over medium high heat, stirring constantly.Once boiling, keep at a rolling boil for two full minutes (still stirring).For this next step, I use a stand mixer with whisk attachment, but a hand mixer would work too.In a large bowl, combine marshmallows and chocolate.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/instant-coffee-or-instant-espresso.png', u'id': 10014214, u'name': u'instant espresso'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/normal-marshmallows.jpg', u'id': 19116, u'name': u'marshmallows'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Pour hot mixture over these ingredients and blend until smooth. I turn my mixer on medium speed and allow it to blend for about 2-3 minutes, scraping down the sides of the bowl several times.While this is mixing, place your caramel and heavy cream in a microwave safe bowl and heat for 1-2 minutes, stirring every 30 seconds until melted and smooth. Set aside.', u'length': {u'number': 5, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/soft-caramels.jpg', u'id': 19074, u'name': u'caramel'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Pour the creamy fudge mixture into a parchment paper lined 15inch x 10inch x 1inch baking sheet.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}], u'step': u'Drizzle with hot caramel, using a knife to swirl it into the fudge.Sprinkle immediately with kosher salt (or coarse sea salt).Refrigerate for 4 hours, or overnight.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 1012047, u'name': u'coarse sea salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 1082047, u'name': u'kosher salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/soft-caramels.jpg', u'id': 19074, u'name': u'caramel'}]}, {u'equipment': [], u'step': u'Cut into small bites and store in the refrigerator for up to two weeks. ENJOY!', u'number': 5, u'ingredients': []}], u'name': u''}], u'aggregateLikes': 34, u'instructions': u'In a large saucepan, combine sugar, milk, vanilla, butter and espresso powder.Bring to a boil over medium high heat, stirring constantly.Once boiling, keep at a rolling boil for two full minutes (still stirring).For this next step, I use a stand mixer with whisk attachment, but a hand mixer would work too.In a large bowl, combine marshmallows and chocolate.Pour hot mixture over these ingredients and blend until smooth. I turn my mixer on medium speed and allow it to blend for about 2-3 minutes, scraping down the sides of the bowl several times.While this is mixing, place your caramel and heavy cream in a microwave safe bowl and heat for 1-2 minutes, stirring every 30 seconds until melted and smooth. Set aside.Pour the creamy fudge mixture into a parchment paper lined 15inch x 10inch x 1inch baking sheet. Drizzle with hot caramel, using a knife to swirl it into the fudge.Sprinkle immediately with kosher salt (or coarse sea salt).Refrigerate for 4 hours, or overnight.Cut into small bites and store in the refrigerator for up to two weeks. ENJOY!', u'spoonacularSourceUrl': u'https://spoonacular.com/salted-caramel-fudge-617542', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 274, u'servings': 60, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}