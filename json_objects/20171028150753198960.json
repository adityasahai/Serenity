u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 1017, u'unit': u'cup'}, {u'originalString': u'1 teaspoon garlic powder', u'aisle': u'Spices and Seasonings', u'name': u'garlic powder', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic-powder.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1022020, u'unit': u'teaspoon'}, {u'originalString': u'1/4 cup thinly sliced green onions, for garnish', u'aisle': u'Produce', u'name': u'green onions', u'metaInformation': [u'thinly sliced', u'for garnish'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spring-onions.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 11291, u'unit': u'cup'}, {u'originalString': u'1/2 cup prepared ranch dressing, plus additional if necessary', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'ranch dressing', u'metaInformation': [u'prepared'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ranch-dressing.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'liquid', u'id': 4639, u'unit': u'cup'}, {u'originalString': u'1 packet (1 ounce) ranch dressing seasoning mix', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'ranch dressing mix', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oregano-dried.png', u'unitLong': u'ounce', u'unitShort': u'oz', u'amount': 1.0, u'consistency': u'solid', u'id': 93733, u'unit': u'ounce'}, {u'originalString': u'1-1/2 cups shredded cooked chicken', u'aisle': u'Meat', u'name': u'shredded chicken', u'metaInformation': [u'shredded', u'cooked'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rotisserie-chicken.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'liquid', u'id': 5348, u'unit': u'cups'}, {u'originalString': u'1 cup sour cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'sour cream', u'metaInformation': [u'sour'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 1056, u'unit': u'cup'}, {u'originalString': u'24 wonton wrappers (the ones I used were 4-1/2-inch squares)', u'aisle': u'Ethnic Foods', u'name': u'wonton wrappers', u'metaInformation': [u'(the ones I used were 4-1/ squares)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/puff-pastry-squares.jpg', u'unitLong': u'2-inchs', u'unitShort': u'2-inch', u'amount': 24.0, u'consistency': u'solid', u'id': 10018368, u'unit': u'2-inch'}], u'healthScore': 1.0, u'imageType': u'jpg', u'spoonacularScore': 16.0, u'sourceUrl': u'http://foxeslovelemons.com/veggie-pizza-and-buffalo-chicken-salad-wonton-cup-appetizers-pinterest-challenge/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 3, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350 degrees F. Spray 2 standard muffin tins with nonstick spray. Press 1 wonton wrapper into each cup, taking care to press again bottom and sides of cups. Lightly spray wrappers with additional nonstick spray.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Bake 8 minutes or until golden brown and crisp.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/wire-rack.jpg', u'id': 405900, u'name': u'wire rack'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Remove wonton cups from muffin pan and transfer to a cooling rack to cool completely before filling.In a small bowl, stir together seasoning mix, cream cheese, sour cream and garlic powder. Dollop a heaping tablespoon of cream cheese mixture into each baked wonton cup. Top with veggies.In a medium bowl, stir together chicken, ranch dressing, blue cheese, celery and buffalo sauce. You may need to add a bit more ranch dressing, depending on the texture of chicken you started with. You want it to be thoroughly coated in sauce, but not too wet or anything. Refrigerate at least 30 minutes or up to overnight to allow flavors to come together. Divide chicken mixture between baked wonton cups.', u'length': {u'number': 30, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ranch-dressing.jpg', u'id': 4639, u'name': u'ranch dressing'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/hot-sauce-or-tabasco.png', u'id': 98878, u'name': u'buffalo sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic-powder.jpg', u'id': 1022020, u'name': u'garlic powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'id': 1017, u'name': u'cream cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/blue-cheese.png', u'id': 1004, u'name': u'blue cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'id': 1056, u'name': u'sour cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/celery.jpg', u'id': 11143, u'name': u'celery'}]}, {u'equipment': [], u'step': u'Garnish with green onions and additional hot sauce, if desired.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spring-onions.jpg', u'id': 11291, u'name': u'green onions'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Sparkling ros\xe9 and Sparkling Wine are great choices for Wontons. If you're serving a selection of appetizers, you can't go wrong with these. Both are very food friendly and complement a variety of flavors. One wine you could try is Bellissima Sparkling ros\xe9. It has 4.5 out of 5 stars and a bottle costs about 24 dollars.", u'pairedWines': [u'sparkling rose', u'sparkling wine'], u'productMatches': [{u'description': u'This is an elegant, bright ros\xe9, with hints of strawberry and grapefruit, and a soft, persistent foam. Uncorked at the very last minute, Bellissima Sparkling Ros\xe9 pairs perfectly with smoked salmon, shellfish, and seafood hor d\u2019oeuvres such as tuna tartare, sushi, and sashimi. View More', u'title': u'Bellissima Sparkling Rose', u'price': u'$23.99', u'id': 458665, u'link': u'https://www.wine.com/product/bellissima-sparkling-rose/293794', u'imageUrl': u'https://spoonacular.com/productImages/458665-312x231.jpg'}]}, u'aggregateLikes': 15, u'instructions': u'Preheat oven to 350 degrees F. Spray 2 standard muffin tins with nonstick spray. Press 1 wonton wrapper into each cup, taking care to press again bottom and sides of cups. Lightly spray wrappers with additional nonstick spray. Bake 8 minutes or until golden brown and crisp. Remove wonton cups from muffin pan and transfer to a cooling rack to cool completely before filling.In a small bowl, stir together seasoning mix, cream cheese, sour cream and garlic powder. Dollop a heaping tablespoon of cream cheese mixture into each baked wonton cup. Top with veggies.In a medium bowl, stir together chicken, ranch dressing, blue cheese, celery and buffalo sauce. You may need to add a bit more ranch dressing, depending on the texture of chicken you started with. You want it to be thoroughly coated in sauce, but not too wet or anything. Refrigerate at least 30 minutes or up to overnight to allow flavors to come together. Divide chicken mixture between baked wonton cups. Garnish with green onions and additional hot sauce, if desired.', u'spoonacularSourceUrl': u'https://spoonacular.com/veggie-pizza-and-buffalo-chicken-salad-wonton-cup-appetizers-511906', u'dishTypes': [u'antipasti', u'starter', u'snack', u'appetizer', u'antipasto', u"hor d'oeuvre"], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 24, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}