{u'recipes': [{u'sourceName': u'Taste of Home', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/468010-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 0, u'id': 468010, u'veryPopular': False, u'lowFodmap': False, u'title': u'Wild Rice Salad', u'diets': [u'gluten free', u'dairy free'], u'preparationMinutes': 25, u'pricePerServing': 139.5, u'creditText': u'Taste of Home', u'extendedIngredients': [{u'originalString': u'1 cup cashews, optional', u'aisle': u'Nuts;Savory Snacks', u'name': u'cashews', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cashews.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 12087, u'unit': u'cup'}, {u'originalString': u'2 cups diced cooked chicken', u'aisle': u'Meat', u'name': u'chicken', u'metaInformation': [u'diced', u'cooked'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/whole-chicken.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 2.0, u'consistency': u'solid', u'id': 1025006, u'unit': u'cups'}, {u'originalString': u'1-1/2 cups halved green grapes', u'aisle': u'Produce', u'name': u'green grapes', u'metaInformation': [u'green', u'halved'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-grapes.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 9132, u'unit': u'cups'}, {u'originalString': u'Lettuce leaves', u'aisle': u'Produce', u'name': u'lettuce leaves', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/iceberg-lettuce.jpg', u'unitLong': u'leave', u'unitShort': u'leaves', u'amount': 1.0, u'consistency': u'solid', u'id': 93623, u'unit': u'leaves'}, {u'originalString': u'3/4 cup reduced-fat mayonnaise', u'aisle': u'Condiments', u'name': u'low fat mayonnaise', u'metaInformation': [u'reduced-fat'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mayonnaise.jpg', u'unitLong': u'cups', u'un