 u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 4053, u'unit': u'tablespoon'}, {u'originalString': u'3 large parsnips (or 4-5 small ones)', u'aisle': u'Produce', u'name': u'parsnips', u'metaInformation': [u'small', u'large', u'(or 4-5 ones)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsnip.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11298, u'unit': u''}, {u'originalString': u'pinch of red pepper flakes (less than \xbc teaspoon)', u'aisle': u'Spices and Seasonings', u'name': u'red pepper flakes', u'metaInformation': [u'red', u'(less than \xbc teaspoon)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-pepper-flakes.jpg', u'unitLong': u'pinch', u'unitShort': u'pinch', u'amount': 1.0, u'consistency': u'solid', u'id': 1032009, u'unit': u'pinch'}, {u'originalString': u'6 sage leaves', u'aisle': u'Spices and Seasonings', u'name': u'sage leaves', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sage.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 6.0, u'consistency': u'solid', u'id': 2038, u'unit': u''}, {u'originalString': u'freshly ground sea salt and pepper, to taste', u'aisle': u'Spices and Seasonings', u'name': u'sea-salt', u'metaInformation': [u'freshly ground', u'to taste'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 3.0, u'consistency': u'solid', u'id': 1012047, u'unit': u'servings'}, {u'originalString': u'1 large shallot, minced', u'aisle': u'Produce', u'name': u'shallot', u'metaInformation': [u'minced', u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11677, u'unit': u''}], u'healthScore': 51.0, u'imageType': u'jpg', u'spoonacularScore': 97.0, u'sourceUrl': u'http://www.inspiralized.com/2014/12/22/parsnip-noodles-with-leftover-christmas-ham-and-butternut-squash-sage-sauce/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 3, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Heat the oil in a large pot over medium heat. Once the oil heats, add the sage and cook until crispy. Set the sage aside on a paper-towel lined plate. Immediately add in the butternut squash, onion, garlic and red pepper flakes to the pot and season with salt and pepper. Cook for 5-7 minutes or until onion is translucent.', u'length': {u'number': 7, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-pepper-flakes.jpg', u'id': 1032009, u'name': u'red pepper flakes'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butternut-squash.jpg', u'id': 11485, u'name': u'butternut squash'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sage.jpg', u'id': 2038, u'name': u'sage'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Pour in the broth and raise the heat to bring the mixture to a boil. Once boiling, reduce the heat to a simmer and cook until the squash is softened, 15-20 minutes. While the squash cooks, peel and spiralize the parsnips. Set aside. Once the squash mixture is softened and easily pierced with a fork, transfer to a high-speed blender. Wipe down the pot and set aside. Blend the squash mixture until smooth, about 1 minute. Taste and season with salt and pepper to your preference. If the mixture is too thick, add in vegetable broth.If not using leftover Christmas ham: Set the pot down over medium heat and once heated, add in the ham or pancetta and cook for 5 minutes or until cooked through and starting to brown on edges. Then, add in the parsnip noodles, cover  and toss to cook for 5 more minutes or until al dente. Then, add in the squash sauce and toss altogether to warm sauce. Crumble the sage on top and serve immediately.If using leftover Christmas ham: Set the pot down over medium heat and once heated, add in the ham, squash sauce and parsnip noodles, toss and cook for 5-7 minutes or until noodles al dente. Crumble the fried sage on top and serve immediately.', u'length': {u'number': 38, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsnip.jpg', u'id': 11298, u'name': u'parsnip'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butternut-squash.jpg', u'id': 10011485, u'name': u'squash'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sage.jpg', u'id': 2038, u'name': u'sage'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ham-whole.jpg', u'id': 10151, u'name': u'ham'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 200, u'instructions': u'Heat the oil in a large pot over medium heat. Once the oil heats, add the sage and cook until crispy. Set the sage aside on a paper-towel lined plate. Immediately add in the butternut squash, onion, garlic and red pepper flakes to the pot and season with salt and pepper. Cook for 5-7 minutes or until onion is translucent. Pour in the broth and raise the heat to bring the mixture to a boil. Once boiling, reduce the heat to a simmer and cook until the squash is softened, 15-20 minutes. While the squash cooks, peel and spiralize the parsnips. Set aside. Once the squash mixture is softened and easily pierced with a fork, transfer to a high-speed blender. Wipe down the pot and set aside. Blend the squash mixture until smooth, about 1 minute. Taste and season with salt and pepper to your preference. If the mixture is too thick, add in vegetable broth.If not using leftover Christmas ham: Set the pot down over medium heat and once heated, add in the ham or pancetta and cook for 5 minutes or until cooked through and starting to brown on edges. Then, add in the parsnip noodles, cover  and toss to cook for 5 more minutes or until al dente. Then, add in the squash sauce and toss altogether to warm sauce. Crumble the sage on top and serve immediately.If using leftover Christmas ham: Set the pot down over medium heat and once heated, add in the ham, squash sauce and parsnip noodles, toss and cook for 5-7 minutes or until noodles al dente. Crumble the fried sage on top and serve immediately.', u'spoonacularSourceUrl': u'https://spoonacular.com/parsnip-noodles-with-leftover-christmas-ham-and-butternut-squash-sage-sauce-624403', u'dishTypes': [u'side dish'], u'occasions': [u'christmas'], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 55, u'servings': 3, u'sustainable': False, u'dairyFree': True, u'whole30': True}]}