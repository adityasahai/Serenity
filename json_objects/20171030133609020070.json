 u'metaInformation': [u'pure'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 2050, u'unit': u'teaspoon'}], u'healthScore': 4.0, u'imageType': u'jpg', u'spoonacularScore': 48.0, u'sourceUrl': u'http://www.theroastedroot.net/no-bake-vegan-key-lime-tarts/', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 21, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/cake-pan.png', u'id': 404650, u'name': u'springform pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}], u'step': u'Add all of the ingredients for the tart crust to a mixing bowl and stir until a sticky ball forms. Distribute crust mixture among a 12-hole muffin pan and press firmly into the bottoms and sides of the holes, creating thick tart crusts (Note: You can also use ten 2-1/2 inch tart pans or one 10-inch springform pan).', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}], u'step': u'Place muffin tray in the refrigerator while youre preparing the pie filling.', u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}], u'step': u'Add all of the ingredients for the pie filling to a blender or food processor. Blend until completely smooth.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-tray.jpg', u'id': 404671, u'name': u'muffin tray'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/chefs-knife.jpg', u'id': 404745, u'name': u'knife'}], u'step': u'Pour the key lime pie filling into the prepared muffin pan and use a knife to smooth out the mixture. Freeze for 30 minutes or refrigerate for at least 2 hours. If using a muffin tray, run a paring knife along the edge of the crust and the muffin pan in order to release the tarts.', u'length': {u'number': 30, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Serve with unsweetened shredded coconut and lime zest.', u'number': 5, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are great choices for Tart. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 678, u'instructions': u'Add all of the ingredients for the tart crust to a mixing bowl and stir until a sticky ball forms. Distribute crust mixture among a 12-hole muffin pan and press firmly into the bottoms and sides of the holes, creating thick tart crusts (Note: You can also use ten 2-1/2 inch tart pans or one 10-inch springform pan). Place muffin tray in the refrigerator while youre preparing the pie filling.Add all of the ingredients for the pie filling to a blender or food processor. Blend until completely smooth. Pour the key lime pie filling into the prepared muffin pan and use a knife to smooth out the mixture. Freeze for 30 minutes or refrigerate for at least 2 hours. If using a muffin tray, run a paring knife along the edge of the crust and the muffin pan in order to release the tarts. Serve with unsweetened shredded coconut and lime zest.', u'spoonacularSourceUrl': u'https://spoonacular.com/no-bake-vegan-key-lime-tarts-814207', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 30, u'servings': 10, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}