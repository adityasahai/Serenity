,{"number":2,"step":"Add sugar and water to a small saucepan or skillet over medium heat. Cook to 280 degrees, or until dark and clear amber in color, about 5 to 6 minutes.","ingredients":[{"id":19335,"name":"sugar","image":"https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg"},{"id":14412,"name":"water","image":"https://spoonacular.com/cdn/ingredients_100x100/water.jpg"}],"equipment":[{"id":404669,"name":"sauce pan","image":"https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg"},{"id":404645,"name":"frying pan","image":"https://spoonacular.com/cdn/equipment_100x100/pan.png"}],"length":{"number":5,"unit":"minutes"}}]}]}]}