{u'recipes': [{u'sourceName': u'Fuss Free Cooking', u'cuisines': [u'asian', u'indian'], u'image': u'https://spoonacular.com/recipeImages/494193-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 494193, u'veryPopular': False, u'lowFodmap': False, u'title': u'[] Grilled Thai Red Curry Steak & Noodles Stir Fry', u'diets': [u'dairy free'], u'pricePerServing': 431.46, u'creditText': u'Fuss Free Cooking', u'extendedIngredients': [{u'originalString': u'1 cup beef stock', u'aisle': u'Canned and Jarred', u'name': u'beef stock', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'liquid', u'id': 6170, u'unit': u'cup'}, {u'originalString': u'\xbd brown onion, sliced', u'aisle': u'Produce', u'name': u'brown onion', u'metaInformation': [u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 0.5, u'consistency': u'solid', u'id': 10511282, u'unit': u''}, {u'originalString': u'1 tbsp brown sugar', u'aisle': u'Baking', u'name': u'brown sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 19334, u'unit': u'tbsp'}, {u'originalString': u'1 large carrot', u'aisle': u'Produce', u'name': u'carrot', u'metaInformation': [u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11124, u'unit': u''}, {u'originalString': u'Fresh coriander & chilli for garnish optional', u'aisle': u'Produce', u'name': u'chilli', u'metaInformation': [u'fresh', u'for garnish optional'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-chili.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 4.0, u'consis