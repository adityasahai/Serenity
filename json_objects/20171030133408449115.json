{u'recipes': [{u'sourceName': u'Beyond Frosting', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/813821-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 813821, u'veryPopular': True, u'lowFodmap': False, u'title': u'Lemon Raspberry Shortbread Magic Bars', u'diets': [u'gluten free'], u'pricePerServing': 4.05, u'creditText': u'Beyond Frosting', u'extendedIngredients': [{u'originalString': u'While the chocolate is cooling, use a knife to gently spread raspberry jam on top of your bars. Sprinkle additional coconut on top if desired.', u'aisle': u'Sweet Snacks', u'name': u'chocolate', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk-chocolate.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 20.0, u'consistency': u'solid', u'id': 19081, u'unit': u'servings'}, {u'originalString': u'Return cream to heat, and add 3 ounces of white chocolate chips. Heat cream and chocolate chips until melted, using a spatula to stir. Allow to cool for 15 minutes.', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'cream', u'metaInformation': [u'white', u'melted', u'for 15 minutes.'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 3.0, u'consistency': u'liquid', u'id': 1053, u'unit': u'ounces'}, {u'originalString': u'Use a spoon or piping bag to drizzle lemon infused chocolate ganache on top of the bars.', u'aisle': u'Sweet Snacks', u'name': u'dark chocolate candy bars', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dark-chocolate-pieces.jpg', u'unitLong': u'bag', u'unitShort': u'bag', u'amount': 1.0, u'consistency': u'liquid', u'id': 10019904, u'unit': u'bag'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 4.0, u'sourceUrl': u'http://beyondfrosting.com/2013/05/12/lemon-raspberry-magic-bars/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 0, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat oven to 350Use a food processor to grind cookies into a fine crumb.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Pour cookie crumbs into a 13*9 inch baking sheet.', u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}], u'step': u'Mix crumbs together to combine.Melt butter in the microwave 30-45 seconds until completely melted.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Pour over cookie crumbs and use a spoon to mix butter into crumbs. Using your hands, gently pat cookie crumbs to spread and cover the bottom of the baking sheet.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/spatula-or-turner.jpg', u'id': 404642, u'name': u'spatula'}], u'step': u'Pour one can of sweetened condensed milk over cookie crumbs; use the back of a spatula to spread if necessary.Next, pour macadamia nuts, shredded coconut and 5 ounces of white chocolate chips over top of the milk to cover evenly.', u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'Bake at 350 for 22-25 minutes until coconut starts to turn golden brown.', u'length': {u'number': 25, u'unit': u'minutes'}, u'number': 6, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Remove from oven and allow to cool for at least 20 minutes.', u'length': {u'number': 20, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies works really well with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. The NV Johnson Estate Cream Sherry with a 5 out of 5 star rating seems like a good match. It costs about 19 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'Very aromatic with notes of hazelnut, vanilla, and a touch of oak followed by sweet raisins and a touch of yeast. Clean lasting finish. Good now but will reward those allow it to age"". A favorite pre-prandial beverage. Consider it with nuts before dinner as an aperitif, or after dinner with dessert, especially chocolates and fruit-based desserts. Also wonderful on cold afternoons, served with biscotti to dip in ""Italian-style"". "', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 2455, u'instructions': u'Preheat oven to 350Use a food processor to grind cookies into a fine crumb. Pour cookie crumbs into a 13*9 inch baking sheet. Mix crumbs together to combine.Melt butter in the microwave 30-45 seconds until completely melted. Pour over cookie crumbs and use a spoon to mix butter into crumbs. Using your hands, gently pat cookie crumbs to spread and cover the bottom of the baking sheet.Pour one can of sweetened condensed milk over cookie crumbs; use the back of a spatula to spread if necessary.Next, pour macadamia nuts, shredded coconut and 5 ounces of white chocolate chips over top of the milk to cover evenly.Bake at 350 for 22-25 minutes until coconut starts to turn golden brown. Remove from oven and allow to cool for at least 20 minutes.', u'spoonacularSourceUrl': u'https://spoonacular.com/lemon-raspberry-shortbread-magic-bars-813821', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 20, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}