'metaInformation': [u'white', u'chopped', u'for dipping*'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-chocolate.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 8.0, u'consistency': u'liquid', u'id': 19087, u'unit': u'ounces'}], u'healthScore': 0.0, u'imageType': u'jpg', u'spoonacularScore': 12.0, u'sourceUrl': u'http://flavorthemoments.com/cranberry-orange-almond-biscotti/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 4, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat the oven to 350 degrees, and position the oven racks in the upper and lower thirds of the oven. Line (2) large rimmed baking sheets with parchment paper and set aside.In a small bowl, whisk the flour, baking powder, and salt until combined. Set aside.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 18371, u'name': u'baking powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place the butter, sugar, and orange zest in a large bowl. Beat on medium speed for 1 minute or until light and fluffy.', u'length': {u'number': 1, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/zest-orange.jpg', u'id': 9216, u'name': u'orange zest'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u'Add the eggs one at a time on low speed just until incorporated. Beat in the vanilla.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/serrated-knife.jpg', u'id': 404740, u'name': u'serrated knife'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/cutting-board.jpg', u'id': 404716, u'name': u'cutting board'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Add the flour mixture and beat on low speed until just combined. Fold in the almonds and cranberries.Knead the dough together with your hands while still in the bowl until it\'s cohesive (it\'s not sticky, so this doesn\'t take long). Divide in two equal pieces and place on one of the prepared baking sheets. Form into (2) 12" x 2" logs and bake on the middle rack for 30-35 minutes, or until golden brown. Reduce the oven temperature to 275 degrees.Allow the dough to cool for 10 minutes, then place on a cutting board. Using a serrated knife, cut slightly on the bias into " thick slices and place cut side down on the prepared baking sheets.', u'length': {u'number': 45, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cranberries.jpg', u'id': 9078, u'name': u'cranberries'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/almonds.jpg', u'id': 12061, u'name': u'almonds'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/double-boiler.jpg', u'id': 404699, u'name': u'double boiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}], u'step': u'Bake at 275 degrees for 30-40 minutes, or until golden brown and crisp. Cool completely on wire racks.Melt the chocolate in a double boiler or microwave according to package instructions. Dip the biscotti in the chocolate and place on the parchment lined baking sheets until set. Store biscotti in an airtight container for up to 3 weeks. Enjoy!', u'length': {u'number': 40, u'unit': u'minutes'}, u'number': 5, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies works really well with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. You could try NV Solera Cream Sherry. Reviewers quite like it with a 4.5 out of 5 star rating and a price of about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'nutty , butterscotch , caramel', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 459, u'instructions': u'Preheat the oven to 350 degrees, and position the oven racks in the upper and lower thirds of the oven. Line (2) large rimmed baking sheets with parchment paper and set aside.In a small bowl, whisk the flour, baking powder, and salt until combined. Set aside.Place the butter, sugar, and orange zest in a large bowl. Beat on medium speed for 1 minute or until light and fluffy. Add the eggs one at a time on low speed just until incorporated. Beat in the vanilla.Add the flour mixture and beat on low speed until just combined. Fold in the almonds and cranberries.Knead the dough together with your hands while still in the bowl until it\'s cohesive (it\'s not sticky, so this doesn\'t take long). Divide in two equal pieces and place on one of the prepared baking sheets. Form into (2) 12" x 2" logs and bake on the middle rack for 30-35 minutes, or until golden brown. Reduce the oven temperature to 275 degrees.Allow the dough to cool for 10 minutes, then place on a cutting board. Using a serrated knife, cut slightly on the bias into " thick slices and place cut side down on the prepared baking sheets. Bake at 275 degrees for 30-40 minutes, or until golden brown and crisp. Cool completely on wire racks.Melt the chocolate in a double boiler or microwave according to package instructions. Dip the biscotti in the chocolate and place on the parchment lined baking sheets until set. Store biscotti in an airtight container for up to 3 weeks. Enjoy!', u'spoonacularSourceUrl': u'https://spoonacular.com/cranberry-orange-almond-biscotti-845618', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 80, u'servings': 38, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}