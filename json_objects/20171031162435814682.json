ormation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 4.0, u'consistency': u'liquid', u'id': 1053, u'unit': u'tbsp'}, {u'originalString': u'3 tbsp Heavy whipping cream', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'heavy whipping cream', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'liquid', u'id': 1053, u'unit': u'tbsp'}, {u'originalString': u'1 C Peanut butter chips', u'aisle': u'Baking', u'name': u'peanut butter chips', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/peanut-butter-or-butterscotch-chips.jpg', u'unitLong': u'C', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 93762, u'unit': u'C'}, {u'originalString': u'6 C Powdered sugar', u'aisle': u'Baking', u'name': u'powdered sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'unitLong': u'Cs', u'unitShort': u'cup', u'amount': 6.0, u'consistency': u'solid', u'id': 19336, u'unit': u'C'}, {u'originalString': u'1\xbd C (3 sticks) Unsalted butter (room temperature)', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted', u'room temperature', u'(3 sticks)', u'()'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'Cs', u'unitShort': u'cup', u'amount': 1.5, u'consistency': u'solid', u'id': 1145, u'unit': u'C'}], u'healthScore': 3.0, u'imageType': u'jpg', u'spoonacularScore': 40.0, u'sourceUrl': u'http://beyondfrosting.com/2014/02/23/take-5-cupcakes/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 85, u'analyzedInstructions': [{u'steps': [{u'equipment': [], u'step': u'Bake a batch of your favorite chocolate cupcakes and allow to cool completely. Core out the center of your cupcakes about halfway deep.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/plain-cupcake.jpg', u'id': 18139, u'name': u'cupcakes'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Combine peanut butter chips and heavy cream in a microwave safe bowl. Microwave on high for 60 seconds.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/peanut-butter-or-butterscotch-chips.jpg', u'id': 93762, u'name': u'peanut butter chips'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Remove from microwave and whisk until smooth. If there are still chunks left after whisking, microwave for another 15-20 seconds.Take peanuts and gently plush in a food processor to break up into smaller pieces. Empty crushed peanuts into ganache and stir to combine.Spoon peanut butter ganache into the hollow center of the cupcake.Allow butter to come to room temperature.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/plain-cupcake.jpg', u'id': 18139, u'name': u'cupcakes'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Whisk butter on medium high speed until light and fluffy (3-4 minutes).', u'length': {u'number': 4, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Add Herseys chocolate spread and beat until combined.Turn mixer speed down to slow. Slowly add powdered sugar 1 cup at a time. After adding 3 cups of powdered sugar, the frosting will start to become very thick.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'id': 19336, u'name': u'powdered sugar'}]}, {u'equipment': [], u'step': u'Add 1 tablespoon of heavy cream.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}]}, {u'equipment': [], u'step': u'Add remaining powdered sugar, 1 cup at a time. After the last cup, add remaining 2 tablespoons of heavy cream.Once all ingredients are combined, increase mixing speed to medium high and beat frosting for 1-2 minutes to whip some air into it.Once you reach the desired consistency, frost peanut-butter filled cupcakes.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'id': 19336, u'name': u'powdered sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fluid-cream.jpg', u'id': 1053, u'name': u'heavy cream'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/plain-cupcake.jpg', u'id': 18139, u'name': u'cupcakes'}]}, {u'equipment': [], u'step': u'Drizzle with caramel before serving.', u'number': 8, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/soft-caramels.jpg', u'id': 19074, u'name': u'caramel'}]}, {u'equipment': [], u'step': u'Add chocolate covered pretzels as a garnish.', u'number': 9, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are my top picks for Cupcakes. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. One wine you could try is NV Solera Cream Sherry. It has 4.5 out of 5 stars and a bottle costs about 17 dollars.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 612, u'instructions': u'Bake a batch of your favorite chocolate cupcakes and allow to cool completely. Core out the center of your cupcakes about halfway deep.Combine peanut butter chips and heavy cream in a microwave safe bowl. Microwave on high for 60 seconds. Remove from microwave and whisk until smooth. If there are still chunks left after whisking, microwave for another 15-20 seconds.Take peanuts and gently plush in a food processor to break up into smaller pieces. Empty crushed peanuts into ganache and stir to combine.Spoon peanut butter ganache into the hollow center of the cupcake.Allow butter to come to room temperature. Whisk butter on medium high speed until light and fluffy (3-4 minutes).Add Herseys chocolate spread and beat until combined.Turn mixer speed down to slow. Slowly add powdered sugar 1 cup at a time. After adding 3 cups of powdered sugar, the frosting will start to become very thick. Add 1 tablespoon of heavy cream.Add remaining powdered sugar, 1 cup at a time. After the last cup, add remaining 2 tablespoons of heavy cream.Once all ingredients are combined, increase mixing speed to medium high and beat frosting for 1-2 minutes to whip some air into it.Once you reach the desired consistency, frost peanut-butter filled cupcakes. Drizzle with caramel before serving. Add chocolate covered pretzels as a garnish.', u'spoonacularSourceUrl': u'https://spoonacular.com/take-5-cupcakes-590864', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 30, u'servings': 6, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}