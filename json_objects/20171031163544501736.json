[u'all-purpose'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 20081, u'unit': u'cup'}, {u'originalString': u'2 handfuls fresh basil, leaves only, torn', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh basil', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/basil.jpg', u'unitLong': u'handfuls', u'unitShort': u'handfuls', u'amount': 2.0, u'consistency': u'solid', u'id': 2044, u'unit': u'handfuls'}, {u'originalString': u'1 tablespoon fresh thyme leaves', u'aisle': u'Produce', u'name': u'fresh thyme leaves', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/thyme.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2049, u'unit': u'tablespoon'}, {u'originalString': u'6 cloves garlic, peeled and grated', u'aisle': u'Produce', u'name': u'garlic', u'metaInformation': [u'grated', u'peeled'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'cloves', u'unitShort': u'cloves', u'amount': 6.0, u'consistency': u'solid', u'id': 11215, u'unit': u'cloves'}, {u'originalString': u'1 tablespoon granulated sugar', u'aisle': u'Baking', u'name': u'granulated sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 19335, u'unit': u'tablespoon'}, {u'originalString': u'Kosher salt', u'aisle': u'Spices and Seasonings', u'name': u'kosher salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 6.0, u'consistency': u'solid', u'id': 1082047, u'unit': u'servings'}, {u'originalString': u'1 1/2 pounds mozzarella cheese, cut into thin slices', u'aisle': u'Cheese', u'name': u'mozzarella cheese', u'metaInformation': [u'cut into thin slices'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mozzarella-fresh.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 1.5, u'consistency': u'solid', u'id': 1026, u'unit': u'pounds'}, {u'originalString': u'1/4 cup extra-virgin olive oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'olive oil', u'metaInformation': [u'extra-virgin'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'liquid', u'id': 4053, u'unit': u'cup'}, {u'originalString': u'1 tablespoon dried oregano', u'aisle': u'Produce;Spices and Seasonings', u'name': u'oregano', u'metaInformation': [u'dried'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oregano.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2027, u'unit': u'tablespoon'}, {u'originalString': u'1/2 cup grated Parmesan', u'aisle': u'Cheese', u'name': u'parmesan', u'metaInformation': [u'grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 1033, u'unit': u'cup'}, {u'originalString': u'3 (28-ounce) cans San Marzano whole plum tomatoes', u'aisle': u'Produce', u'name': u'plum tomatoes', u'metaInformation': [u'whole', u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/plum-tomatoes.png', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 84.0, u'consistency': u'solid', u'id': 10411529, u'unit': u'ounce'}, {u'originalString': u'1 pound provolone cheese, grated', u'aisle': u'Cheese', u'name': u'provolone cheese', u'metaInformation': [u'grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/provolone-cheese.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 1035, u'unit': u'pound'}, {u'originalString': u'1 tablespoon crushed red pepper flakes', u'aisle': u'Spices and Seasonings', u'name': u'red pepper flakes', u'metaInformation': [u'red', u'crushed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-pepper-flakes.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1032009, u'unit': u'tablespoon'}, {u'originalString': u'Vegetable oil, for frying, as needed, about 1 1/2 to 2 cups', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'vegetable oil', u'metaInformation': [u'for frying', u'as needed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vegetable-oil.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.5, u'consistency': u'liquid', u'id': 4513, u'unit': u'cups'}, {u'originalString': u'3 tablespoons whole milk', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'whole milk', u'metaInformation': [u'whole'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'liquid', u'id': 1077, u'unit': u'tablespoons'}, {u'originalString': u'3 medium yellow onions, peeled, halved, and cut into thin slices', u'aisle': u'Produce', u'name': u'yellow onions', u'metaInformation': [u'yellow', u'medium', u'peeled', u'halved', u'cut into thin slices'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 10511282, u'unit': u''}], u'healthScore': 59.0, u'imageType': u'jpeg', u'spoonacularScore': 94.0, u'sourceUrl': u'http://www.foodnetwork.com/recipes/alexandra-guarnaschelli/eggplant-parmigiana-recipe.html', u'glutenFree': False, u'vegan': False, u'veryHealthy': True, u'weightWatcherSmartPoints': 41, u'analyzedInstructions': [{u'steps': [{u'equipment': [], u'step': u'Watch how to make this recipe.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u"I really think this is what I would eat for my last meal on Earth. It's so simple and, when done right, sublime. I cannot stop eating this!;", u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'For the tomato sauce: In a large skillet, heat the olive oil over medium-high heat.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [], u'step': u'Add the onion and garlic and season with salt and red pepper flakes. Cook until the onions become translucent, 3 to 5 minutes.', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-pepper-flakes.jpg', u'id': 1032009, u'name': u'red pepper flakes'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/wooden-spoon.jpg', u'id': 404732, u'name': u'wooden spoon'}], u'step': u'Add the sugar and the canned tomatoes. Use a wooden spoon to break up some of the whole tomatoes and cook 10 to 15 minutes over medium heat, stirring from time to time. Taste for seasoning, the tomatoes should be fairly broken down and the flavors coming together. Cook for another few minutes if the tomatoes still taste like they need a little more time to break down. Set aside to cool.', u'length': {u'number': 10, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spices.png', u'id': 1042027, u'name': u'seasoning'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomato.jpg', u'id': 11529, u'name': u'tomato'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/dish-towel.jpg', u'id': 221439, u'name': u'kitchen towels'}], u'step': u'For the eggplant (this step is optional, leave it out if you have limited time): Arrange the eggplant rounds in a single layer on 2 baking sheets. Sprinkle with salt on both sides of each slice and allow it to sit for about 1 hour. Salting it draws out the liquid and bitter flavor. After an hour, rinse with cold water and dry them thoroughly with a kitchen towel.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/eggplant.jpg', u'id': 11209, u'name': u'eggplant'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Put the flour in a medium bowl and season with salt and pepper. In another bowl, whisk together the eggs and milk and season with salt and pepper. In a third bowl, combine the breadcrumbs with the oregano and fresh thyme leaves and season with salt and pepper. Dip each eggplant slice in the flour and shake off any excess. Then, dip in the egg mixture, and finally in the breadcrumbs. Make sure to coat both sides of each slice of eggplant. Arrange them in single layers on the baking sheets.', u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/thyme.jpg', u'id': 2049, u'name': u'fresh thyme'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/breadcrumbs.jpg', u'id': 18079, u'name': u'breadcrumbs'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/eggplant.jpg', u'id': 11209, u'name': u'eggplant'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/oregano.jpg', u'id': 2027, u'name': u'oregano'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'In a large skillet, pour enough oil to accumulate about 1/2-inch in the bottom.', u'number': 8, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/tongs.jpg', u'id': 404641, u'name': u'tongs'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-thermometer.jpg', u'id': 404789, u'name': u'kitchen thermometer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Heat the oil until it begins to smoke lightly (alternatively, test with a thermometer and wait until the oil registers between 380 degrees F and 400 degrees F). Use a pair of kitchen tongs to add a single layer of the eggplant to the pan. Cook them until they are golden brown, about 2 minutes on each side.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 9, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/eggplant.jpg', u'id': 11209, u'name': u'eggplant'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/dish-towel.jpg', u'id': 221439, u'name': u'kitchen towels'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Remove from the oil and transfer to a baking sheet fitted with a kitchen towel so the eggplant can drain as the others cook. Season lightly with salt. Take care to reheat the oil back up to temperature before adding another batch of slices to the pan.', u'number': 10, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/eggplant.jpg', u'id': 11209, u'name': u'eggplant'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350 degrees F.', u'number': 11, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}], u'step': u'To assemble: In a 9 by 13-inch baking dish, spoon about 1/4 of the tomato sauce on the bottom. Top with a layer of the fried eggplant; the eggplant slices can overlap slightly. Top with about 1/3 of the mozzarella slices. Sprinkle with about 1/4 of the Parmesan and provolone cheeses. Top with a layer of torn basil leaves. Spoon sauce and repeat the layering 2 more times to make 3 layers. End with the remaining mozzarella. Carefully press the layers down firmly into the dish once assembled.', u'number': 12, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-basil.jpg', u'id': 2044, u'name': u'fresh basil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mozzarella-fresh.jpg', u'id': 1026, u'name': u'mozzarella'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/provolone-slices.jpg', u'id': 1035, u'name': u'provolone'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/eggplant.jpg', u'id': 11209, u'name': u'eggplant'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 405914, u'name': u'broiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Place the dish in the top part of the oven and cook until the cheese is melted and bubbly, 35 to 40 minutes. For extra browning, put the dish under the broiler for a minute or two just before serving to get an extra brown cheesy top. I always nibble on a bit of that before sharing it with my friends!', u'length': {u'number': 35, u'unit': u'minutes'}, u'number': 13, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Verdicchio, Trebbiano, and Chianti are my top picks for Eggplant Parmigiana. Italians know food and they know wine. Trebbiano and Verdicchio are Italian white wines that pair well with fish and white meat, while Chianti is a great Italian red for heavier, bolder dishes. One wine you could try is Sartarelli Verdicchio Classico. It has 3.2 out of 5 stars and a bottle costs about 13 dollars.', u'pairedWines': [u'verdicchio', u'trebbiano', u'chianti'], u'productMatches': [{u'description': u'Vinification: sourced from 3 vineyards, soft pressing and temperature controlled fermentation in stainless steel lasting 3-4 weeks, partial malolactic.Aging: 6 months in stainless steel before bottling.TASTING NOTES: Nutty, discrete aromas of pit fruits, smoke and minerals, rich and dry, intensely flavored; finishes with floral and herbal notes.', u'title': u'Sartarelli Verdicchio Classico', u'price': u'$12.99', u'id': 434907, u'link': u'https://www.wine.com/product/sartarelli-verdicchio-classico-2000/52251', u'imageUrl': u'https://spoonacular.com/productImages/434907-312x231.jpg'}]}, u'aggregateLikes': 50, u'instructions': u"Watch how to make this recipe.                    I really think this is what I would eat for my last meal on Earth. It's so simple and, when done right, sublime. I cannot stop eating this!;                          For the tomato sauce: In a large skillet, heat the olive oil over medium-high heat. Add the onion and garlic and season with salt and red pepper flakes. Cook until the onions become translucent, 3 to 5 minutes. Add the sugar and the canned tomatoes. Use a wooden spoon to break up some of the whole tomatoes and cook 10 to 15 minutes over medium heat, stirring from time to time. Taste for seasoning, the tomatoes should be fairly broken down and the flavors coming together. Cook for another few minutes if the tomatoes still taste like they need a little more time to break down. Set aside to cool.                          For the eggplant (this step is optional, leave it out if you have limited time): Arrange the eggplant rounds in a single layer on 2 baking sheets. Sprinkle with salt on both sides of each slice and allow it to sit for about 1 hour. Salting it draws out the liquid and bitter flavor. After an hour, rinse with cold water and dry them thoroughly with a kitchen towel.                          Put the flour in a medium bowl and season with salt and pepper. In another bowl, whisk together the eggs and milk and season with salt and pepper. In a third bowl, combine the breadcrumbs with the oregano and fresh thyme leaves and season with salt and pepper. Dip each eggplant slice in the flour and shake off any excess. Then, dip in the egg mixture, and finally in the breadcrumbs. Make sure to coat both sides of each slice of eggplant. Arrange them in single layers on the baking sheets.                          In a large skillet, pour enough oil to accumulate about 1/2-inch in the bottom. Heat the oil until it begins to smoke lightly (alternatively, test with a thermometer and wait until the oil registers between 380 degrees F and 400 degrees F). Use a pair of kitchen tongs to add a single layer of the eggplant to the pan. Cook them until they are golden brown, about 2 minutes on each side. Remove from the oil and transfer to a baking sheet fitted with a kitchen towel so the eggplant can drain as the others cook. Season lightly with salt. Take care to reheat the oil back up to temperature before adding another batch of slices to the pan.                          Preheat oven to 350 degrees F.                          To assemble: In a 9 by 13-inch baking dish, spoon about 1/4 of the tomato sauce on the bottom. Top with a layer of the fried eggplant; the eggplant slices can overlap slightly. Top with about 1/3 of the mozzarella slices. Sprinkle with about 1/4 of the Parmesan and provolone cheeses. Top with a layer of torn basil leaves. Spoon sauce and repeat the layering 2 more times to make 3 layers. End with the remaining mozzarella. Carefully press the layers down firmly into the dish once assembled. Place the dish in the top part of the oven and cook until the cheese is melted and bubbly, 35 to 40 minutes. For extra browning, put the dish under the broiler for a minute or two just before serving to get an extra brown cheesy top. I always nibble on a bit of that before sharing it with my friends!", u'spoonacularSourceUrl': u'https://spoonacular.com/eggplant-parmigiana-311324', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 165, u'servings': 6, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}