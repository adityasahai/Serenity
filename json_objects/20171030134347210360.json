{u'recipes': [{u'sourceName': u'Food Fanatic', u'cuisines': [u'mediterranean', u'european', u'italian'], u'image': u'https://spoonacular.com/recipeImages/578577-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 12, u'id': 578577, u'veryPopular': False, u'lowFodmap': False, u'title': u'Bruschetta Burger', u'diets': [], u'preparationMinutes': 15, u'pricePerServing': 235.21, u'creditText': u'Food Fanatic', u'extendedIngredients': [{u'originalString': u'1 teaspoon black pepper', u'aisle': u'Spices and Seasonings', u'name': u'black pepper', u'metaInformation': [u'black'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1002030, u'unit': u'teaspoon'}, {u'originalString': u'1 1/2 teaspoons fresh basil', u'aisle': u'Produce;Spices and Seasonings', u'name': u'fresh basil', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/basil.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 1.5, u'consistency': u'solid', u'id': 2044, u'unit': u'teaspoons'}, {u'originalString': u'3 cloves garlic, minced', u'aisle': u'Produce', u'name': u'garlic', u'metaInformation': [u'minced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'cloves', u'unitShort': u'cloves', u'amount': 3.0, u'consistency': u'solid', u'id': 11215, u'unit': u'cloves'}, {u'originalString': u'2 pounds ground beef', u'aisle': u'Meat', u'name': u'ground beef', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'liquid', u'id': 10023572, u'unit': u'pounds'}, {u'originalString': u'1 loaf italian bread', u'aisle': u'Bakery/Bread', u'name': u'italian bread', u'metaInformation': [u'italian'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/italian-bread.jpg', u'unitLong': u'loaf', u'unitShort': u'loaf', u'amount': 1.0, u'consistency': u'solid', u'id': 10028033, u'unit': u'loaf'}, {u'originalString': u'1/2 pound mozzarella cheese, cut into 6 slices', u'aisle': u'Cheese', u'name': u'mozzarella cheese', u'metaInformation': [u'cut into 6 slices'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mozzarella-fresh.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 0.5, u'consistency': u'solid', u'id': 1026, u'unit': u'pound'}, {u'originalString': u'tablespoon olive oil, to drizzle', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'olive oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 4053, u'unit': u'tablespoon'}, {u'originalString': u'1/4 cup grated parmesan cheese', u'aisle': u'Cheese', u'name': u'parmesan cheese', u'metaInformation': [u'grated'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 1033, u'unit': u'cup'}, {u'originalString': u'1 teaspoon salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2047, u'unit': u'teaspoon'}, {u'originalString': u'1 8-ounce jar bruschetta mix', u'aisle': u'?', u'name': u'bruschetta mix', u'metaInformation': [], u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 8.0, u'unit': u'ounce'}], u'healthScore': 12.0, u'imageType': u'jpg', u'spoonacularScore': 68.0, u'sourceUrl': u'http://www.foodfanatic.com/recipes/bruschetta-burger-recipe/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 41, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'In large bowl combine ground beef, parmesan cheese, basil, minced garlic, salt and pepper.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parmesan-or-romano.jpg', u'id': 1033, u'name': u'parmesan'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fresh-ground-beef.jpg', u'id': 10023572, u'name': u'ground beef'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/basil.jpg', u'id': 2044, u'name': u'basil'}]}, {u'equipment': [], u'step': u"Combine all ingredients, using your hands.\xa0Divide the meat into 6 equal portions. With each portion divide it again in half when forming the burgers.\xa0Flatten out one half, place a slice of fresh mozzarella on top and top with remaining half of meat, forming into a round flat patty, pinching the sides closed, so the cheese isn't exposed.Repeat with all the meat, making 6 burgers.", u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mozzarella-fresh.jpg', u'id': 1026, u'name': u'fresh mozzarella'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}], u'step': u'Heat grill to medium high heat and cook burgers for 5-6 minutes on each side.While burgers are cooking slice the loaf of Italian bread into 3/4 inch slices.', u'length': {u'number': 6, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/italian-bread.jpg', u'id': 10028033, u'name': u'italian bread'}]}, {u'equipment': [], u'step': u'Drizzle with olive oil.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 405914, u'name': u'broiler'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}], u'step': u"Place bread on a baking sheet and place under the broiler, or on the grill for 1-2 minutes until toasted. (If you use the broiler watch closely so bread doesn't burn!)As soon as the bread is toasted, remove it from the broiler or grill and rub with the inside of remaining clove of garlic that has been halved.When burgers are done, place on non-toasted side of bread, top with a generous portion of Bruschetta and the other slice of toast.", u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'garlic'}]}, {u'equipment': [], u'step': u'Serve immediately.', u'number': 6, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Malbec, Merlot, and Zinfandel are my top picks for Burger. Merlot will be perfectly adequate for a classic burger with standard toppings. Bolder toppings call for bolder wines, such as a malbec or peppery zinfandel. You could try Don Rodolfo Malbec. Reviewers quite like it with a 4.1 out of 5 star rating and a price of about 12 dollars per bottle.', u'pairedWines': [u'malbec', u'merlot', u'zinfandel'], u'productMatches': [{u'description': u'An intense ruby red color with violet flashes. Aromas of berries, cassis and plums. This Malbec is a burst of dark cherry and blackberry with a rich, plush finish.', u'title': u'Don Rodolfo Malbec', u'price': u'$11.99', u'id': 438361, u'link': u'https://www.wine.com/product/don-rodolfo-malbec-2015/153771', u'imageUrl': u'https://spoonacular.com/productImages/438361-312x231.jpg'}]}, u'aggregateLikes': 78, u'instructions': u"In large bowl combine ground beef, parmesan cheese, basil, minced garlic, salt and pepper. Combine all ingredients, using your hands.\xa0Divide the meat into 6 equal portions. With each portion divide it again in half when forming the burgers.\xa0Flatten out one half, place a slice of fresh mozzarella on top and top with remaining half of meat, forming into a round flat patty, pinching the sides closed, so the cheese isn't exposed.Repeat with all the meat, making 6 burgers. Heat grill to medium high heat and cook burgers for 5-6 minutes on each side.While burgers are cooking slice the loaf of Italian bread into 3/4 inch slices. Drizzle with olive oil. Place bread on a baking sheet and place under the broiler, or on the grill for 1-2 minutes until toasted. (If you use the broiler watch closely so bread doesn't burn!)As soon as the bread is toasted, remove it from the broiler or grill and rub with the inside of remaining clove of garlic that has been halved.When burgers are done, place on non-toasted side of bread, top with a generous portion of Bruschetta and the other slice of toast. Serve immediately.", u'spoonacularSourceUrl': u'https://spoonacular.com/bruschetta-burger-578577', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 27, u'servings': 6, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}