{u'recipes': [{u'sourceName': u'A Cedar Spoon', u'cuisines': [u'spanish', u'european'], u'image': u'https://spoonacular.com/recipeImages/536416-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 8, u'id': 536416, u'veryPopular': False, u'lowFodmap': False, u'title': u'Mussels with Smoked Spanish Paprika', u'diets': [u'dairy free', u'pescatarian'], u'preparationMinutes': 30, u'pricePerServing': 202.9, u'creditText': u'A Cedar Spoon', u'extendedIngredients': [{u'originalString': u'1 28 oz. can of diced tomatoes with liquid', u'aisle': u'Canned and Jarred', u'name': u'canned tomatoes', u'metaInformation': [u'diced', u'with liquid', u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/tomatoes-canned.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 28.0, u'consistency': u'solid', u'id': 10011693, u'unit': u'oz'}, {u'originalString': u'Crusty bread to soak it all up', u'aisle': u'Bakery/Bread', u'name': u'crusty bread', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/crusty-bread.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 4.0, u'consistency': u'solid', u'id': 10018029, u'unit': u'servings'}, {u'originalString': u'3 garlic cloves, chopped', u'aisle': u'Produce', u'name': u'garlic cloves', u'metaInformation': [u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 11215, u'unit': u''}, {u'originalString': u'2 lbs. fresh mussels', u'aisle': u'Seafood', u'name': u'mussels', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mussels.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'solid', u'id': 15164, u'unit': u'lbs'}, {u'originalString': u'1 small onion, chopped', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'small', u'chopped'], u'image': u'https://spoonacu