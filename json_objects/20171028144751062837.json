'veryHealthy': False, u'weightWatcherSmartPoints': 4, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350\xb0F with rack in middle. Line 2 large baking sheets with parchment paper.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Whisk together flour, cornstarch, and salt.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 20027, u'name': u'corn starch'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}], u'step': u"Beat together butter and confectioners' sugar with an electric mixer until pale and fluffy, then beat in zest and vanilla. At low speed, mix in flour mixture just until a soft dough forms.", u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'id': 19336, u'name': u'powdered sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Put sanding sugars in different bowls.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Roll a scant teaspoon of dough into a ball and drop into sugar, turning to coat. Reshape if necessary and transfer to a baking sheet. Repeat, spacing balls 3/4 inch apart, until baking sheet is filled.', u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'Bake until tops are slightly cracked but still pale (bottoms will be pale golden), 12 to 15 minutes.', u'length': {u'number': 12, u'unit': u'minutes'}, u'number': 6, u'ingredients': []}, {u'equipment': [], u'step': u'Transfer cookies on parchment to a rack to cool completely.', u'number': 7, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Form and bake more cookies on second baking sheet.', u'number': 8, u'ingredients': []}], u'name': u'Make Cookies'}, {u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Beat together all filling ingredients in a large bowl with an electric mixer at medium speed until combined well.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Transfer to sealable bag and snip off a corner.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Turn over half of cookies and pipe about 1/2 teaspoon filling on flat side of each. Sandwich with remaining cookies, pressing gently.', u'number': 3, u'ingredients': []}, {u'equipment': [], u'step': u'Recipe Notes', u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'The sugared dough balls should be chilled for 30 minutes or frozen for 5 to 10 minutes to help preserve their round shape. Keep the sandwiched cookies chilled.2. The unsandwiched cookies keep, in a metal cookie tin, at a cool room temperature for up to 4 days.', u'length': {u'number': 35, u'unit': u'minutes'}, u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'From The Gourmet Cookie Book: The Single Best Recipe from Each Year 1941-2009 by Cond\xe9 Nast Publications. Copyright \xa9 2010 by Cond\xe9 Nast Publications; photographs copyright \xa9 2010 by Cond\xe9 Nast Publications. Published by Houghton Mifflin Harcourt Publishing Company.', u'number': 6, u'ingredients': []}], u'name': u'Make Filling and Sandwich Cookies'}], u'winePairing': {u'pairingText': u"Cream Sherry, Madeira, and Prosecco are my top picks for Cookies. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. The NV Johnson Estate Cream Sherry with a 5 out of 5 star rating seems like a good match. It costs about 19 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'oak , vanilla , hazelnut', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 181, u'instructions': u"Preparation                                        Make Cookies:                Preheat oven to 350\xb0F with rack in middle. Line 2 large baking sheets with parchment paper.                                                                Whisk together flour, cornstarch, and salt.                                                                Beat together butter and confectioners' sugar with an electric mixer until pale and fluffy, then beat in zest and vanilla. At low speed, mix in flour mixture just until a soft dough forms.                                                                Put sanding sugars in different bowls. Roll a scant teaspoon of dough into a ball and drop into sugar, turning to coat. Reshape if necessary and transfer to a baking sheet. Repeat, spacing balls 3/4 inch apart, until baking sheet is filled.                                                                Bake until tops are slightly cracked but still pale (bottoms will be pale golden), 12 to 15 minutes. Transfer cookies on parchment to a rack to cool completely.                                                                Form and bake more cookies on second baking sheet.                                                            Make Filling and Sandwich Cookies:                Beat together all filling ingredients in a large bowl with an electric mixer at medium speed until combined well. Transfer to sealable bag and snip off a corner.                                                                Turn over half of cookies and pipe about 1/2 teaspoon filling on flat side of each. Sandwich with remaining cookies, pressing gently.                                                Recipe Notes            1. The sugared dough balls should be chilled for 30 minutes or frozen for 5 to 10 minutes to help preserve their round shape. Keep the sandwiched cookies chilled.2. The unsandwiched cookies keep, in a metal cookie tin, at a cool room temperature for up to 4 days.                                                            From The Gourmet Cookie Book: The Single Best Recipe from Each Year 1941-2009 by Cond\xe9 Nast Publications. Copyright \xa9 2010 by Cond\xe9 Nast Publications; photographs copyright \xa9 2010 by Cond\xe9 Nast Publications. Published by Houghton Mifflin Harcourt Publishing Company.", u'spoonacularSourceUrl': u'https://spoonacular.com/glittering-lemon-sandwich-cookies-207539', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 48, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}