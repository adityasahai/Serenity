{u'recipes': [{u'sourceName': u'Rachel Cooks', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/612646-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 60, u'id': 612646, u'veryPopular': False, u'lowFodmap': True, u'title': u'Raspberry Chipotle Candied Pecans', u'diets': [u'gluten free', u'dairy free', u'lacto ovo vegetarian', u'fodmap friendly'], u'preparationMinutes': 5, u'pricePerServing': 329.42, u'creditText': u'Rachel Cooks', u'extendedIngredients': [{u'originalString': u'1/4 teaspoon chipotle pepper powder (regular chili powder will work in a pinch)', u'aisle': u'Spices and Seasonings', u'name': u'chipotle powder', u'metaInformation': [u'(regular chili powder will work in a pinch)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chili-powder.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 1052009, u'unit': u'teaspoon'}, {u'originalString': u'1 egg white', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'egg white', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 1124, u'unit': u''}, {u'originalString': u'1 cup granulated sugar', u'aisle': u'Baking', u'name': u'granulated sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 19335, u'unit': u'cup'}, {u'originalString': u'4 cups unsalted raw pecans', u'aisle': u'Nuts;Baking', u'name': u'pecans', u'metaInformation': [u'raw', u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 4.0, u'consistency': u'solid', u'id': 12142, u'unit': u'cups'}, {u'originalString': u'4 teaspoons raspberry preserves', u'aisle': u'Nut butters, Jams, and Honey', u'name': u'raspberry preserves', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raspberry-jam.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 4.0, u'consistency': u'solid', u'id': 10719297, u'unit': u'teaspoons'}], u'healthScore': 21.0, u'imageType': u'jpg', u'spoonacularScore': 77.0, u'sourceUrl': u'http://www.rachelcooks.com/2014/09/19/raspberry-chipotle-candied-pecans/', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 34, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 250.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat the oven to 250F and grease a large rimmed baking sheet.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Whisk the sugar and chipotle pepper powder together in a small bowl.In a medium mixing bowl, beat the egg white until it is white and forms soft peaks. Fold in the raspberry preserves.Fold in the pecans, stirring until they are all coated with the egg white.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/raspberry-jam.jpg', u'id': 10719297, u'name': u'raspberry preserves'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'id': 1124, u'name': u'egg whites'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'id': 12142, u'name': u'pecans'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [], u'step': u'Add in the sugar mixture, again stirring until all the pecans are coated.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'id': 12142, u'name': u'pecans'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u'Spread the nuts into a single layer on the baking sheet.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Bake at 250F for one hour, stirring every 15 minutes, until the pecans are dry.Allow to cool completely before transferring to a bowl or container. They will stay fresh in an airtight container for up to 2 weeks (if you dont eat them all first!).', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'id': 12142, u'name': u'pecans'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Fingerfood works really well with Sparkling Wine and Sparkling ros\xe9. If you're serving a selection of appetizers, you can't go wrong with these. Both are very food friendly and complement a variety of flavors. The NV ONEHOPE Brut with a 4.7 out of 5 star rating seems like a good match. It costs about 20 dollars per bottle.", u'pairedWines': [u'sparkling wine', u'sparkling rose'], u'productMatches': [{u'description': u'ONEHOPE California Brut Champagne Sparkling Wine is light in color and displays aromas of fresh green apple and nectarine with a hint of fresh baked bread. Crisp, intriguing fresh fruit reminiscent of succulent peach, sweet berries and pear linger on the plate, finishing with creamy and refreshing bubbles. This Brut Sparkling Wine can be paired with appetizers, seafood, sushi as well as a wide range of desserts and cheeses.', u'title': u'NV ONEHOPE Brut', u'price': u'$19.99', u'id': 428232, u'link': u'https://www.amazon.com/ONEHOPE-California-Brut-Sparkling-750/dp/B00C3164HC', u'imageUrl': u'https://spoonacular.com/productImages/428232-312x231.jpg'}]}, u'aggregateLikes': 22, u'instructions': u'Preheat the oven to 250F and grease a large rimmed baking sheet.Whisk the sugar and chipotle pepper powder together in a small bowl.In a medium mixing bowl, beat the egg white until it is white and forms soft peaks. Fold in the raspberry preserves.Fold in the pecans, stirring until they are all coated with the egg white. Add in the sugar mixture, again stirring until all the pecans are coated.Spread the nuts into a single layer on the baking sheet. Bake at 250F for one hour, stirring every 15 minutes, until the pecans are dry.Allow to cool completely before transferring to a bowl or container. They will stay fresh in an airtight container for up to 2 weeks (if you dont eat them all first!).', u'spoonacularSourceUrl': u'https://spoonacular.com/raspberry-chipotle-candied-pecans-612646', u'dishTypes': [u'fingerfood', u'antipasti', u'starter', u'snack', u'appetizer', u'antipasto', u"hor d'oeuvre"], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 65, u'servings': 4, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}