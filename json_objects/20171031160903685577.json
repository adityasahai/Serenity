ion': [u'fat free', u'crushed'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/saltine-crackers.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 12.0, u'consistency': u'solid', u'id': 18228, u'unit': u''}, {u'originalString': u'8 strips raw turkey bacon (I used Oscar Meyer brand)', u'aisle': u'Meat', u'name': u'turkey bacon', u'metaInformation': [u'raw', u'(I used Oscar Meyer brand)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/bacon-turkey.jpg', u'unitLong': u'strips', u'unitShort': u'strips', u'amount': 8.0, u'consistency': u'solid', u'id': 42130, u'unit': u'strips'}], u'healthScore': 10.0, u'imageType': u'jpg', u'spoonacularScore': 72.0, u'sourceUrl': u'http://www.emilybites.com/2013/05/bacon-wrapped-bbq-meatloaf.html', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 5, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Pre-heat the oven to 350. Line a baking sheet with aluminum foil and set aside.In a large bowl, combine the beef and turkey and mush together with clean hands until combined.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Add the onion, 3 tablespoons of the barbecue sauce, mustard, chili powder, garlic powder, salt, pepper, saltine crumbs and egg and lightly mix together. Do not over handle.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/barbecue-sauce.jpg', u'id': 6150, u'name': u'barbecue sauce'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic-powder.jpg', u'id': 1022020, u'name': u'garlic powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chili-powder.jpg', u'id': 2009, u'name': u'chili powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/regular-mustard.jpg', u'id': 2046, u'name': u'mustard'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pepper.jpg', u'id': 1002030, u'name': u'pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pastry-brush.jpg', u'id': 404764, u'name': u'pastry brush'}], u'step': u'Transfer the meat mixture to the lined baking sheet and form into a 9\u201d x 5\u201d loaf. Lay the strips of bacon in a row to cover the top of the meatloaf, each one slightly overlapping the piece before it. Tuck the ends of the bacon strips underneath the bottom of the meatloaf. Using a silicone pastry brush, brush the remaining 2 tablespoons of barbecue sauce over the entire loaf.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/barbecue-sauce.jpg', u'id': 6150, u'name': u'barbecue sauce'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-thermometer.jpg', u'id': 404789, u'name': u'kitchen thermometer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Bake in the oven for 50-60 minutes or until your meat thermometer reads 160 degrees.', u'length': {u'number': 60, u'unit': u'minutes'}, u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Cut into 12 even slices and serve.', u'number': 5, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cabernet Sauvignon and Zinfandel are great choices for Meatloaf. Meatloaf will work with many red wines. We recommend something bold and fruity, like a Cabernet Sauvignon or Zinfandel. The Vincent Bouquet Cabernet Sauvignon Vin de Pays d'Oc with a 4.2 out of 5 star rating seems like a good match. It costs about 8 dollars per bottle.", u'pairedWines': [u'cabernet sauvignon', u'zinfandel'], u'productMatches': [{u'description': u'Cabernet Sauvignon 100%', u'title': u"Vincent Bouquet Cabernet Sauvignon Vin de Pays d'Oc", u'price': u'$7.99', u'id': 454922, u'link': u'https://www.wine.com/product/vincent-bouquet-cabernet-sauvignon-vin-de-pays-doc-2006/101798', u'imageUrl': u'https://spoonacular.com/productImages/454922-312x231.jpg'}]}, u'aggregateLikes': 4287, u'instructions': u'Pre-heat the oven to 350. Line a baking sheet with aluminum foil and set aside.In a large bowl, combine the beef and turkey and mush together with clean hands until combined. Add the onion, 3 tablespoons of the barbecue sauce, mustard, chili powder, garlic powder, salt, pepper, saltine crumbs and egg and lightly mix together. Do not over handle.Transfer the meat mixture to the lined baking sheet and form into a 9\u201d x 5\u201d loaf. Lay the strips of bacon in a row to cover the top of the meatloaf, each one slightly overlapping the piece before it. Tuck the ends of the bacon strips underneath the bottom of the meatloaf. Using a silicone pastry brush, brush the remaining 2 tablespoons of barbecue sauce over the entire loaf. Bake in the oven for 50-60 minutes or until your meat thermometer reads 160 degrees. Cut into 12 even slices and serve.', u'spoonacularSourceUrl': u'https://spoonacular.com/bacon-wrapped-bbq-meatloaf-570210', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 6, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}