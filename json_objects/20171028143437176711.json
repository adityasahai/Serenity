steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}], u'step': u'Loosely fill main cavity and neck cavity of turkey with stuffing. Generously butter baking dish. Spoon remaining stuffing into prepared dish. Cover dish with buttered foil, buttered side down.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Bake stuffing in dish \u2014 alongside turkey or while turkey is resting \u2014 until heated through, about 25 minutes. Uncover stuffing in dish.', u'length': {u'number': 25, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Bake until top of stuffing is slightly crisp and golden, about 15 minutes longer.', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 3, u'ingredients': []}], u'name': u'To bake stuffing in turkey'}, {u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/glass-baking-tray.jpg', u'id': 406921, u'name': u'glass baking pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350\xb0F. Generously butter 13x9x2-inch glass baking dish.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Transfer stuffing to prepared dish.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Bake uncovered until heated through, about 35 minutes.', u'length': {u'number': 35, u'unit': u'minutes'}, u'number': 3, u'ingredients': []}], u'name': u'To bake stuffing in dish'}], u'winePairing': {}, u'aggregateLikes': 50, u'instructions': u'Preparation                                                        Melt 2 tablespoons butter in large skillet over medium-high heat. Add pears; sprinkle with sugar. Saut\xe9 until pears are golden, about 5 minutes. Transfer to very large bowl; mix in cinnamon. Melt remaining 10 tablespoons butter in same skillet over medium heat. Add onions and celery; saut\xe9 until golden, about 15 minutes. Add wine and next 5 ingredients. Simmer until liquid is reduced almost to glaze and mixture is soft and moist, stirring occasionally, about 15 minutes. Mix into pears. (Can be made 1 day ahead. Cover and chill. Reheat to lukewarm before continuing.)                                                                Mix bread and pecans into fruit mixture. Season stuffing with salt and pepper.                                                            To bake stuffing in turkey:                Loosely fill main cavity and neck cavity of turkey with stuffing. Generously butter baking dish. Spoon remaining stuffing into prepared dish. Cover dish with buttered foil, buttered side down. Bake stuffing in dish \u2014 alongside turkey or while turkey is resting \u2014 until heated through, about 25 minutes. Uncover stuffing in dish. Bake until top of stuffing is slightly crisp and golden, about 15 minutes longer.                                                            To bake stuffing in dish:                Preheat oven to 350\xb0F. Generously butter 13x9x2-inch glass baking dish. Transfer stuffing to prepared dish. Bake uncovered until heated through, about 35 minutes.', u'spoonacularSourceUrl': u'https://spoonacular.com/winter-fruit-and-nut-stuffing-209396', u'dishTypes': [u'side dish'], u'occasions': [u'thanksgiving', u'winter'], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 12, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}