{u'recipes': [{u'sourceName': u'Epicurious', u'cuisines': [], u'cheap': False, u'gaps': u'no', u'id': 725613, u'veryPopular': False, u'lowFodmap': False, u'title': u'Steak with Shallot-Merlot Sauce', u'diets': [u'gluten free'], u'pricePerServing': 365.96, u'creditText': u'Epicurious', u'extendedIngredients': [{u'originalString': u'3/4 cup beef stock or canned broth', u'aisle': u'Canned and Jarred', u'name': u'beef stock', u'metaInformation': [u'canned'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.75, u'consistency': u'liquid', u'id': 6170, u'unit': u'cup'}, {u'originalString': u'6 tablespoons Merlot', u'aisle': u'Alcoholic Beverages', u'name': u'merlot', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-wine.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 6.0, u'consistency': u'liquid', u'id': 14602, u'unit': u'tablespoons'}, {u'originalString': u'1 1/2 tablespoons finely chopped shallots', u'aisle': u'Produce', u'name': u'shallots', u'metaInformation': [u'finely chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 1.5, u'consistency': u'solid', u'id': 11677, u'unit': u'tablespoons'}, {u'originalString': u'1 12- to 14-ounce sirloin steak', u'aisle': u'Meat', u'name': u'sirloin steak', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sirloin-raw.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 12.0, u'consistency': u'solid', u'id': 23625, u'unit': u'ounce'}, {u'originalString': u'3 tablespoons unsalted butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'unsalted butter', u'metaInformation': [u'unsalted'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 1145, u'unit': u'tablespoons'}, {u'originalString': u'1 tablespoon vegetable oil', u'aisle': u'Oil, Vinegar, Salad Dressing', u'name': u'vegetable oil', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vegetable-oil.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 4513, u'unit': u'tablespoon'}], u'healthScore': 13.0, u'spoonacularScore': 64.0, u'sourceUrl': u'http://www.epicurious.com/recipes/food/views/Steak-with-Shallot-Merlot-Sauce-100643', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 16, u'analyzedInstructions': [{u'steps': [{u'equipment': [], u'step': u'Sprinkle steak with salt and pepper.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Heat oil in heavy large skillet over high heat.', u'number': 2, u'ingredients': []}, {u'equipment': [], u'step': u'Add steak and cook until brown, about 1 minute per side. Reduce heat to medium and cook steak to desired doneness, about 3 minutes per side for medium-rare.', u'length': {u'number': 4, u'unit': u'minutes'}, u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/aluminum-foil.png', u'id': 404765, u'name': u'aluminum foil'}], u'step': u'Transfer steak to plate; tent with foil.', u'number': 4, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Add shallots to same skillet and saut over medium-high heat 1 minute.', u'length': {u'number': 1, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'id': 11677, u'name': u'shallot'}]}, {u'equipment': [], u'step': u'Add Merlot; boil until reduced by half, about 2 minutes.', u'length': {u'number': 2, u'unit': u'minutes'}, u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-wine.jpg', u'id': 14602, u'name': u'merlot'}]}, {u'equipment': [], u'step': u'Add stock; boil until syrupy, about 8 minutes.', u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Remove skillet from heat.', u'number': 8, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Add butter and whisk until melted. Season with salt and pepper.', u'number': 9, u'ingredients': []}, {u'equipment': [], u'step': u'Cut steak into 2 pieces; place on plates. Spoon sauce over and serve.', u'number': 10, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Steak on the menu? Try pairing with Merlot, Cabernet Sauvignon, and Pinot Noir. After all, beef and red wine are a classic combination. Generally, leaner steaks go well with light or medium-bodied reds, such as pinot noir or merlot, while fattier steaks can handle a bold red, such as cabernet sauvingnon. The Ghost Pines Napa County Sonoma County Merlot with a 5 out of 5 star rating seems like a good match. It costs about 27 dollars per bottle.', u'pairedWines': [u'merlot', u'cabernet sauvignon', u'pinot noir'], u'productMatches': [{u'description': u"Working without sourcing constraints, our winemakers selected Merlot clusters contributing layered flavors of dark berries, licorice and espresso alongside lots of richness, full-bodied structure, and desirable length. In addition to these flavors, you'll taste hints of cigar paper, cocoa, caramel and nutmeg wrapped around a remarkably smooth mid-palate full of velvety texture and depth. On the long finish, you'll find suggestions of toasty oak, mouth-filling black fruit, and cola framed by plush tannins.", u'title': u'Ghost Pines Napa County Sonoma County Merlot', u'price': u'$26.65', u'id': 428399, u'link': u'https://www.amazon.com/Ghost-Pines-County-Sonoma-Merlot/dp/B01A8WOVRW', u'imageUrl': u'https://spoonacular.com/productImages/428399-312x231.jpg'}]}, u'aggregateLikes': 23, u'instructions': u'Preparation                                        Sprinkle steak with salt and pepper. Heat oil in heavy large skillet over high heat. Add steak and cook until brown, about 1 minute per side. Reduce heat to medium and cook steak to desired doneness, about 3 minutes per side for medium-rare. Transfer steak to plate; tent with foil.                                                                            Add shallots to same skillet and saut over medium-high heat 1 minute. Add Merlot; boil until reduced by half, about 2 minutes. Add stock; boil until syrupy, about 8 minutes. Remove skillet from heat. Add butter and whisk until melted. Season with salt and pepper.                                                                            Cut steak into 2 pieces; place on plates. Spoon sauce over and serve.', u'spoonacularSourceUrl': u'https://spoonacular.com/steak-with-shallot-merlot-sauce-725613', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [u"valentine's day", u"father's day"], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 2, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}