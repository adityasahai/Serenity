{u'recipes': [{u'sourceName': u'Go Dairy Free', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/249096-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 15, u'id': 249096, u'veryPopular': False, u'lowFodmap': True, u'title': u'Pumpkin Butter (Vegan, Gluten-Free, Nut-Free, Soy-Free)', u'diets': [u'gluten free', u'dairy free', u'fodmap friendly'], u'preparationMinutes': 5, u'pricePerServing': 40.56, u'creditText': u'Go Dairy Free', u'extendedIngredients': [{u'originalString': u'1 Teaspoon Cinnamon', u'aisle': u'Spices and Seasonings', u'name': u'cinnamon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'unitLong': u'Teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 2010, u'unit': u'Teaspoon'}, {u'originalString': u'\xbc Teaspoon Ground Cloves', u'aisle': u'Spices and Seasonings', u'name': u'ground cloves', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cloves.jpg', u'unitLong': u'Teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2011, u'unit': u'Teaspoon'}, {u'originalString': u'\xbc cup Maple Syrup', u'aisle': u'Cereal', u'name': u'maple syrup', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/maple-syrup-or-agave-nectar.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'liquid', u'id': 19911, u'unit': u'cup'}, {u'originalString': u'Dash Nutmeg', u'aisle': u'Spices and Seasonings', u'name': u'nutmeg', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'unitLong': u'Dash', u'unitShort': u'Dash', u'amount': 1.0, u'consistency': u'solid', u'id': 2025, u'unit': u'Dash'}, {u'originalString': u'15 oz Can of Pumpkin', u'aisle': u'Produce', u'name': u'pumpkin', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pumpkin.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 15.0, u'consistency': u'solid', u'id': 11422, u'unit': u'oz'}, {u'originalString': u'\xbd Teaspoon All Spice', u'aisle': u'Spices and Seasonings', u'name': u'spice mix', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spices.png', u'unitLong': u'Teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'solid', u'id': 1032027, u'unit': u'Teaspoon'}], u'healthScore': 26.0, u'imageType': u'jpg', u'spoonacularScore': 88.0, u'sourceUrl': u'http://www.godairyfree.org/recipes/condiments-and-salad-dressings/pumpkin-butter-vegan-gluten-free-nut-free-soy-free', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 1, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/sauce-pan.jpg', u'id': 404669, u'name': u'sauce pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'In a small saucepan over medium-low heat, mix together the pumpkin mush and maple syrup until they\u2019re both fully combined. If you prefer your spread to be sweeter, don\u2019t be shy and feel free to add in as much syrup as it takes to satisfy that sweet tooth. Have fun with it \u2013 It\u2019s pretty hard to screw this recipe up.Continue to stir the pumpkin slowly for about 10 \u2013 15 minutes, or until the mixture had thickened to a desirable consistency. The stirring is important though, so DON\u2019T walk away! If you do, your pumpkin may scorch and get burnt onto the bottom of the pan, and that wouldn\u2019t be so tasty. The black bits really don\u2019t look to attractive, either.Anyway, once you\u2019ve determined that it\u2019s nice and thick, take your pan off the heat and you can go ahead and mix in all your spices. I also add in just a pinch of salt, because I believe that it helps to round out all the flavors and make them a bit brighter, but you don\u2019t need to by all means. Enjoy!', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/maple-syrup-or-agave-nectar.jpg', u'id': 19911, u'name': u'maple syrup'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pumpkin.jpg', u'id': 11422, u'name': u'pumpkin'}]}], u'name': u''}], u'winePairing': {u'pairingText': u'', u'pairedWines': [], u'productMatches': []}, u'aggregateLikes': 30, u'instructions': u'In a small saucepan over medium-low heat, mix together the pumpkin mush and maple syrup until they\u2019re both fully combined. If you prefer your spread to be sweeter, don\u2019t be shy and feel free to add in as much syrup as it takes to satisfy that sweet tooth. Have fun with it \u2013 It\u2019s pretty hard to screw this recipe up.Continue to stir the pumpkin slowly for about 10 \u2013 15 minutes, or until the mixture had thickened to a desirable consistency. The stirring is important though, so DON\u2019T walk away! If you do, your pumpkin may scorch and get burnt onto the bottom of the pan, and that wouldn\u2019t be so tasty. The black bits really don\u2019t look to attractive, either.Anyway, once you\u2019ve determined that it\u2019s nice and thick, take your pan off the heat and you can go ahead and mix in all your spices. I also add in just a pinch of salt, because I believe that it helps to round out all the flavors and make them a bit brighter, but you don\u2019t need to by all means. Enjoy!', u'spoonacularSourceUrl': u'https://spoonacular.com/pumpkin-butter-vegan-gluten-free-nut-free-soy-free-249096', u'dishTypes': [u'condiment', u'dip', u'spread'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 20, u'servings': 8, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}