on': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flank-steak.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'liquid', u'id': 23167, u'unit': u'lbs'}, {u'originalString': u'1 loaf Sourdough Bread, sliced', u'aisle': u'Bakery/Bread', u'name': u'sourdough bread', u'metaInformation': [u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sourdough-bread.jpg', u'unitLong': u'loaf', u'unitShort': u'loaf', u'amount': 1.0, u'consistency': u'solid', u'id': 10118029, u'unit': u'loaf'}], u'healthScore': 59.0, u'imageType': u'jpg', u'spoonacularScore': 98.0, u'sourceUrl': u'http://www.joyfulhealthyeats.com/steak-fontina-cheese-panini-chimichurri-sauce/', u'glutenFree': False, u'vegan': False, u'veryHealthy': True, u'weightWatcherSmartPoints': 33, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/blender.png', u'id': 404726, u'name': u'blender'}], u'step': u'Prepare fajita skirt steak and slice. {for grilled fajita skirt steak recipe - click here}. Set aside.Prepare Chimichurri Sauce. In a blender add fresh parsley, fresh cilantro, fresh oregano, garlic cloves, red pepper flakes, lime juice, red wine vinegar, olive oil, and salt & pepper. Blend until completely combined.To assemble the panini, layer  cup of sliced fajita steak, 2 slices of fontina cheese, and 1 tablespoons of chimichurri sauce on a slice of bread. Top with another slice of sourdough bread.Repeat this process for the other 3 sandwiches.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-pepper-flakes.jpg', u'id': 1032009, u'name': u'red pepper flakes'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/red-wine-vinegar.jpg', u'id': 1022068, u'name': u'red wine vinegar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sourdough-bread.jpg', u'id': 10118029, u'name': u'sourdough bread'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/fontina.jpg', u'id': 1020, u'name': u'fontina cheese'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cilantro.png', u'id': 11165, u'name': u'fresh cilantro'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/parsley.jpg', u'id': 11297, u'name': u'fresh parsley'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garlic.jpg', u'id': 11215, u'name': u'whole garlic cloves'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flank-steak.jpg', u'id': 23167, u'name': u'skirt steak'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/lime-juice.jpg', u'id': 9160, u'name': u'lime juice'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/panini-maker.jpg', u'id': 415613, u'name': u'panini press'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/grill.jpg', u'id': 404706, u'name': u'grill'}], u'step': u'Place in panini press and grill for 3-4 minutes or until cheese is melted. {if no panini press use an indoor grill or skillet to cook panini}', u'length': {u'number': 4, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Steak works really well with Merlot, Cabernet Sauvignon, and Pinot Noir. After all, beef and red wine are a classic combination. Generally, leaner steaks go well with light or medium-bodied reds, such as pinot noir or merlot, while fattier steaks can handle a bold red, such as cabernet sauvingnon. You could try Franciscan Estate Merlot. Reviewers quite like it with a 4.3 out of 5 star rating and a price of about 25 dollars per bottle.', u'pairedWines': [u'merlot', u'cabernet sauvignon', u'pinot noir'], u'productMatches': [{u'description': u'Our Oakville estate produces distinctive, supple Merlot characterized by flavors of cherries and plums. This wine shows rich, lush and velvety fruit flavors of cherries, plums and blackberries that merge with light toasted oak. Blended with a samll percentage of Cabernet Sauvignon and Cabernet Franc this wine is well balanced and structured with a lingering finish.Alcohol: 13.5%', u'title': u'Franciscan Estate Merlot', u'price': u'$25.49', u'id': 443286, u'link': u'https://www.wine.com/product/franciscan-estate-merlot-1998/7308', u'imageUrl': u'https://spoonacular.com/productImages/443286-312x231.jpg'}]}, u'aggregateLikes': 538, u'instructions': u'Prepare fajita skirt steak and slice. {for grilled fajita skirt steak recipe - click here}. Set aside.Prepare Chimichurri Sauce. In a blender add fresh parsley, fresh cilantro, fresh oregano, garlic cloves, red pepper flakes, lime juice, red wine vinegar, olive oil, and salt & pepper. Blend until completely combined.To assemble the panini, layer  cup of sliced fajita steak, 2 slices of fontina cheese, and 1 tablespoons of chimichurri sauce on a slice of bread. Top with another slice of sourdough bread.Repeat this process for the other 3 sandwiches.Place in panini press and grill for 3-4 minutes or until cheese is melted. {if no panini press use an indoor grill or skillet to cook panini}', u'spoonacularSourceUrl': u'https://spoonacular.com/real-men-cook-steak-fontina-cheese-panini-with-chimichurri-sauce-574699', u'dishTypes': [u'lunch', u'main course', u'main dish', u'dinner'], u'occasions': [u"valentine's day", u"father's day"], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 40, u'servings': 4, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}