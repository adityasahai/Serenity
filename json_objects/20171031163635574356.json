{u'recipes': [{u'sourceName': u'The Healthy Maven', u'cuisines': [u'southern'], u'image': u'https://spoonacular.com/recipeImages/681814-556x370.png', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 5, u'id': 681814, u'veryPopular': False, u'lowFodmap': False, u'title': u'Pecan Pie Energy Balls + Introducing Energy Ball Week', u'diets': [u'caveman', u'gluten free', u'dairy free', u'lacto ovo vegetarian', u'primal', u'whole 30', u'vegan'], u'preparationMinutes': 10, u'pricePerServing': 31.55, u'creditText': u'The Healthy Maven', u'extendedIngredients': [{u'originalString': u'1/2 tsp cinnamon', u'aisle': u'Spices and Seasonings', u'name': u'cinnamon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'solid', u'id': 2010, u'unit': u'tsp'}, {u'originalString': u'1 cup pitted dates (I used honey dates but medjool work too)*', u'aisle': u'Dried Fruits;Produce', u'name': u'dates', u'metaInformation': [u'pitted', u'(I used honey dates but medjool work too)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dates.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 9087, u'unit': u'cup'}, {u'originalString': u'1/8 tsp dried ginger', u'aisle': u'Spices and Seasonings', u'name': u'dried ginger', u'metaInformation': [u'dried'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ginger.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.125, u'consistency': u'solid', u'id': 2021, u'unit': u'tsp'}, {u'originalString': u'1/8 tsp nutmeg', u'aisle': u'Spices and Seasonings', u'name': u'nutmeg', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.125, u'consistency': u'solid', u'id': 2025, u'unit': u'tsp'}, {u'originalString': u'1 cup pecans', u'aisle': u'Nuts;Baking', u'name': u'pecans', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 12142, u'unit': u'cup'}, {u'originalString': u'optional: sprinkle of sea salt', u'aisle': u'Spices and Seasonings', u'name': u'sea-salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 16.0, u'consistency': u'solid', u'id': 1012047, u'unit': u'servings'}, {u'originalString': u'1/2 tsp vanilla extract', u'aisle': u'Baking', u'name': u'vanilla extract', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.5, u'consistency': u'liquid', u'id': 2050, u'unit': u'tsp'}], u'healthScore': 2.0, u'imageType': u'png', u'spoonacularScore': 32.0, u'sourceUrl': u'http://www.thehealthymaven.com/2015/10/pecan-pie-energy-balls.html', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 1, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'temperature': {u'number': 350.0, u'unit': u'Fahrenheit'}, u'name': u'oven'}], u'step': u'Preheat oven to 350 degrees F.', u'number': 1, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}], u'step': u"Place pecans on a baking sheet and toast for 6-8 minutes, watching to ensure they don't burn.", u'length': {u'number': 8, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'id': 12142, u'name': u'pecans'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}], u'step': u'Add dates to a food processor and process until broken up into bits.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dates.jpg', u'id': 9087, u'name': u'dates'}]}, {u'equipment': [], u'step': u'Add in pecans and spices and process until well combined (about 3 minutes).', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pecans.jpg', u'id': 12142, u'name': u'pecans'}]}, {u'equipment': [], u'step': u'Mixture is done when dough easily sticks together.', u'number': 5, u'ingredients': []}, {u'equipment': [], u'step': u'Roll into 16 balls.', u'number': 6, u'ingredients': []}, {u'equipment': [], u'step': u'Place in freezer for 15 minutes before storing in refrigerator.', u'length': {u'number': 15, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u'Madeira, Pedro Xim\xe9nez, and Tawny Port are my top picks for Pecan Pie. Sweet, nutty pecan pie deserves a wine to match - these dessert wines all have the right sweetness and the nutty notes. The Kenneth Volk El Pomar Junction Vineyard Verdelho with a 1.7999999999999998 out of 5 star rating seems like a good match. It costs about 12 dollars per bottle.', u'pairedWines': [u'madeira', u'pedro ximenez', u'tawny port'], u'productMatches': [{u'description': u"Verdelho is a semi-aromatic white grape that was traditionally used in the production of the revered wines of the island of Madeira. Verdelho has found popularity with forward thinking vintners from around the world in the production of delicious table wines. This bottling is full bodied with tropical and citrus fruit aromas with grapefruit, melon and apple flavors. This refreshing wine can be enjoyed as an aperitif or served with seafoods or Pacific Rim cuisine. Verdelho's aroma and bouquet is unique and reminds me of citrus flowers and ripe Casaba melon.Its palate weight is more full-bodied than many other white wines.", u'title': u'Kenneth Volk El Pomar Junction Vineyard Verdelho', u'price': u'$11.99', u'id': 444264, u'link': u'https://www.wine.com/product/kenneth-volk-el-pomar-junction-vineyard-verdelho-2008/105981', u'imageUrl': u'https://spoonacular.com/productImages/444264-312x231.jpg'}]}, u'aggregateLikes': 92, u'instructions': u"Preheat oven to 350 degrees F.Place pecans on a baking sheet and toast for 6-8 minutes, watching to ensure they don't burn.Add dates to a food processor and process until broken up into bits.Add in pecans and spices and process until well combined (about 3 minutes).Mixture is done when dough easily sticks together.Roll into 16 balls.Place in freezer for 15 minutes before storing in refrigerator.", u'spoonacularSourceUrl': u'https://spoonacular.com/pecan-pie-energy-balls-+-introducing-energy-ball-week-681814', u'dishTypes': [], u'occasions': [u'thanksgiving'], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 15, u'servings': 16, u'sustainable': False, u'dairyFree': True, u'whole30': True}]}