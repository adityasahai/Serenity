{u'recipes': [{u'sourceName': u'Casaveneracion', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/522100-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 30, u'id': 522100, u'veryPopular': False, u'lowFodmap': False, u'title': u'Chunky ham and mushroom soup', u'diets': [], u'preparationMinutes': 10, u'pricePerServing': 298.82, u'creditText': u'Casaveneracion', u'extendedIngredients': [{u'originalString': u'3 to 4 c. of broth, preferably homemade', u'aisle': u'Canned and Jarred', u'name': u'broth', u'metaInformation': [u'homemade'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.0, u'consistency': u'liquid', u'id': 1006615, u'unit': u'c'}, {u'originalString': u'4 tbsps. of butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 4.0, u'consistency': u'solid', u'id': 1001, u'unit': u'tbsps'}, {u'originalString': u'about 100 g. of mushrooms (I used button but you can use whatever you prefer), quartered', u'aisle': u'Canned and Jarred', u'name': u'canned mushrooms', u'metaInformation': [u'quartered', u'canned', u'(I used button but you can use whatever you prefer)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/mushrooms.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 100.0, u'consistency': u'solid', u'id': 11264, u'unit': u'g'}, {u'originalString': u'1 carrot, cut into 1/2-inch cubes', u'aisle': u'Produce', u'name': u'carrot', u'metaInformation': [u'cut into 1/2-inch cubes'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11124, u'unit': u''}, {u'originalString': u'2 celery stalks, sliced', u'aisle': u'Produce', u'name': u'celery stalks', u'metaInformation': [u'sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/celery-leaves.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 11143, u'unit': u''}, {u'originalString': u'2 tbsps. of flour', u'aisle': u'Baking', u'name': u'flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 20081, u'unit': u'tbsps'}, {u'originalString': u'300 g. of ham, cut into 1/2-inch cubes', u'aisle': u'Meat', u'name': u'ham', u'metaInformation': [u'cut into 1/2-inch cubes'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ham-whole.jpg', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 300.0, u'consistency': u'liquid', u'id': 10151, u'unit': u'g'}, {u'originalString': u'1 onion, chopped', u'aisle': u'Produce', u'name': u'onion', u'metaInformation': [u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 1.0, u'consistency': u'solid', u'id': 11282, u'unit': u''}, {u'originalString': u'salt and pepper, to taste', u'aisle': u'Spices and Seasonings', u'name': u'salt and pepper', u'metaInformation': [u'to taste'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 2.0, u'consistency': u'solid', u'id': 1102047, u'unit': u'servings'}], u'healthScore': 15.0, u'imageType': u'jpg', u'spoonacularScore': 64.0, u'sourceUrl': u'http://casaveneracion.com/chunky-ham-and-mushroom-soup/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 23, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Melt 2 tbsps. of butter in a frying pan.Throw in the onion, carrot and celery. Sprinkle lightly with salt and pepper. Cook for about a minute.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/carrots.jpg', u'id': 11124, u'name': u'carrot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/celery.jpg', u'id': 11143, u'name': u'celery'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-onion.jpg', u'id': 11282, u'name': u'onion'}]}, {u'equipment': [], u'step': u'Add the ham. Cook, stirring occasionally, for another minute.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ham-whole.jpg', u'id': 10151, u'name': u'ham'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Add the mushrooms. Sprinkle lightly with more salt and pepper.Cook, stirring occasionally, just until the mushrooms start to brown lightly. Take the pan off the heat.In a pot, melt the remaining 2 tbsps. of butter.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Add the flour, all at once, stirring quickly.Cook the butter and flour gently, stirring occasionally, over medium-low heat for about five minutes.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/ladle.jpg', u'id': 404630, u'name': u'ladle'}], u'step': u'Pour in the broth in a thin stream (start with 3 cups only), stirring as you pour.If the mixture is too thick for you, you can add more broth (that\u2019s why the ingredients list says 3 to 4 c. of broth).Depending on whether you\u2019re using seasoned or unseasoned broth, you might want to add more salt and pepper at this point.Take the vegetables, ham and mushrooms and add to the thickened broth. Cover and simmer gently for 20 minutes to allow all the flavors to come together.It is possible that the soup will thicken some more after simmering. Check the texture \u2014 if too thick, add more broth. Taste once more and add salt and pepper, if needed.Ladle the soup into bowls. I recommend serving it with grilled cheese sandwiches.', u'length': {u'number': 20, u'unit': u'minutes'}, u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt-and-pepper.jpg', u'id': 1102047, u'name': u'salt and pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/broth.jpg', u'id': 1006615, u'name': u'broth'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ham-whole.jpg', u'id': 10151, u'name': u'ham'}]}], u'name': u''}], u'aggregateLikes': 9, u'instructions': u'InstructionsMelt 2 tbsps. of butter in a frying pan.Throw in the onion, carrot and celery. Sprinkle lightly with salt and pepper. Cook for about a minute.Add the ham. Cook, stirring occasionally, for another minute.Add the mushrooms. Sprinkle lightly with more salt and pepper.Cook, stirring occasionally, just until the mushrooms start to brown lightly. Take the pan off the heat.In a pot, melt the remaining 2 tbsps. of butter.Add the flour, all at once, stirring quickly.Cook the butter and flour gently, stirring occasionally, over medium-low heat for about five minutes.Pour in the broth in a thin stream (start with 3 cups only), stirring as you pour.If the mixture is too thick for you, you can add more broth (that\u2019s why the ingredients list says 3 to 4 c. of broth).Depending on whether you\u2019re using seasoned or unseasoned broth, you might want to add more salt and pepper at this point.Take the vegetables, ham and mushrooms and add to the thickened broth. Cover and simmer gently for 20 minutes to allow all the flavors to come together.It is possible that the soup will thicken some more after simmering. Check the texture \u2014 if too thick, add more broth. Taste once more and add salt and pepper, if needed.Ladle the soup into bowls. I recommend serving it with grilled cheese sandwiches.', u'spoonacularSourceUrl': u'https://spoonacular.com/chunky-ham-and-mushroom-soup-522100', u'dishTypes': [u'lunch', u'soup', u'main course', u'main dish', u'dinner'], u'occasions': [u'fall', u'winter'], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 40, u'servings': 2, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}