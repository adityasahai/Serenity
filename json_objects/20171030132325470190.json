tWatcherSmartPoints': 19, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}], u'step': u'Spray 9x13\u201d pan or baking dish with cooking spray.', u'number': 1, u'ingredients': []}, {u'equipment': [], u'step': u'Mix pudding and milk until thickened.', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/milk.jpg', u'id': 1077, u'name': u'milk'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}], u'step': u'Add cool whip.Take one package of the graham crackers and spread over bottom of baking dish.Cover with half of the pudding mixture.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/graham-crackers.jpg', u'id': 18617, u'name': u'graham crackers'}]}, {u'equipment': [], u'step': u'Spread with another layer of graham crackers, followed by the remaining pudding mixture.Top with the last layer of graham crackers.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/graham-crackers.jpg', u'id': 18617, u'name': u'graham crackers'}]}, {u'equipment': [], u'step': u'Spread chocolate frosting over all.Refrigerate a few hours before serving.Melt chocolate with butter.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chocolate-frosting.jpg', u'id': 19226, u'name': u'chocolate frosting'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Add remaining ingredients.', u'number': 6, u'ingredients': []}, {u'equipment': [], u'step': u'Mix together and pour over top of graham cracker layer.Refrigerate 8 hours before serving.', u'number': 7, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/graham-crackers.jpg', u'id': 18617, u'name': u'graham crackers'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Eclair Dessert works really well with Cream Sherry, Moscato d'Asti, and Port. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. You could try NV Johnson Estate Cream Sherry. Reviewers quite like it with a 5 out of 5 star rating and a price of about 19 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'Very aromatic with notes of hazelnut, vanilla, and a touch of oak followed by sweet raisins and a touch of yeast. Clean lasting finish. Good now but will reward those allow it to age"". A favorite pre-prandial beverage. Consider it with nuts before dinner as an aperitif, or after dinner with dessert, especially chocolates and fruit-based desserts. Also wonderful on cold afternoons, served with biscotti to dip in ""Italian-style"". "', u'title': u'NV Johnson Estate Cream Sherry', u'price': u'$19.49', u'id': 430626, u'link': u'https://www.amazon.com/Johnson-Estate-Cream-Sherry-750/dp/B00D3GQSRW', u'imageUrl': u'https://spoonacular.com/productImages/430626-312x231.jpg'}]}, u'aggregateLikes': 472, u'instructions': u'Spray 9x13\u201d pan or baking dish with cooking spray.Mix pudding and milk until thickened.Add cool whip.Take one package of the graham crackers and spread over bottom of baking dish.Cover with half of the pudding mixture.Spread with another layer of graham crackers, followed by the remaining pudding mixture.Top with the last layer of graham crackers.Spread chocolate frosting over all.Refrigerate a few hours before serving.Melt chocolate with butter.Add remaining ingredients.Mix together and pour over top of graham cracker layer.Refrigerate 8 hours before serving.', u'spoonacularSourceUrl': u'https://spoonacular.com/eclair-dessert-499915', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 20, u'servings': 15, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}