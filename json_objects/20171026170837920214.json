, u'unit': u''}, {u'originalString': u'Poppy seeds, sesame seeds, caraway seeds, or other toppings', u'aisle': u'Spices and Seasonings;Baking', u'name': u'poppy seeds', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/poppyseed.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 12.0, u'consistency': u'solid', u'id': 2033, u'unit': u'servings'}, {u'originalString': u'2 pounds russet potatoes (about 4 potatoes), scrubbed clean', u'aisle': u'Produce', u'name': u'russet potatoes', u'metaInformation': [u'scrubbed', u'( 4 potatoes)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/russet-or-Idaho-potatoes.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 2.0, u'consistency': u'solid', u'id': 11353, u'unit': u'pounds'}, {u'originalString': u"Spicy brown mustard, such as Gulden's, for serving", u'aisle': u'Condiments', u'name': u'spicy brown mustard', u'metaInformation': [u" such as gulden's, for serving"], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dijon-mustard.jpg', u'unitLong': u'servings', u'unitShort': u'servings', u'amount': 12.0, u'consistency': u'liquid', u'id': 2046, u'unit': u'servings'}, {u'originalString': u'2 medium white onions, roughly chopped', u'aisle': u'Produce', u'name': u'white onions', u'metaInformation': [u'white', u'medium', u'roughly chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-onion.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 2.0, u'consistency': u'solid', u'id': 10611282, u'unit': u''}, {u'originalString': u'1 1/2 pounds yukon gold potatoes (6 to 8 potatoes), scrubbed clean', u'aisle': u'Produce', u'name': u'yukon gold potatoes', u'metaInformation': [u'scrubbed', u'(6 to 8 potatoes)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/potatoes-yukon-gold.jpg', u'unitLong': u'pounds', u'unitShort': u'lb', u'amount': 1.5, u'consistency': u'solid', u'id': 10211362, u'unit': u'pounds'}], u'healthScore': 2