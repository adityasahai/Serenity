{u'recipes': [{u'sourceName': u'Beyond Frosting', u'cuisines': [u'american'], u'image': u'https://spoonacular.com/recipeImages/590845-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 15, u'id': 590845, u'veryPopular': True, u'lowFodmap': False, u'title': u'Salted Caramel Hot Chocolate Cupcakes', u'diets': [], u'preparationMinutes': 45, u'pricePerServing': 85.87, u'creditText': u'Beyond Frosting', u'extendedIngredients': [{u'originalString': u'2 tbsp Caramel sauce (Smuckers or other)', u'aisle': u'Nut butters, Jams, and Honey;Frozen;Baking', u'name': u'caramel sauce', u'metaInformation': [u'(Smuckers or other)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sauce-caramel.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 19364, u'unit': u'tbsp'}, {u'originalString': u'3 tbsp Caramel sauce', u'aisle': u'Nut butters, Jams, and Honey;Frozen;Baking', u'name': u'caramel sauce', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sauce-caramel.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 3.0, u'consistency': u'solid', u'id': 19364, u'unit': u'tbsp'}, {u'originalString': u'1 Box Chocolate cake mix', u'aisle': u'Gluten Free', u'name': u'gf chocolate cake mix', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/chocolate-cake.jpg', u'unitLong': u'Box', u'unitShort': u'Box', u'amount': 1.0, u'consistency': u'solid', u'id': 99040, u'unit': u'Box'}, {u'originalString': u'5 Large egg whites', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'egg whites', u'metaInformation': [u'large'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg-white.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 5.0, u'consistency': u'solid', u'id': 1124, u'unit': u''}, {u'originalString': u'3 Large eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [u'large'], u'image': u'https:/