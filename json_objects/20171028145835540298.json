tion': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'liquid', u'id': 4053, u'unit': u'tablespoon'}, {u'originalString': u'\xbc tsp salt', u'aisle': u'Spices and Seasonings', u'name': u'salt', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2047, u'unit': u'tsp'}, {u'originalString': u'1 cup thinly sliced shallots', u'aisle': u'Produce', u'name': u'shallots', u'metaInformation': [u'thinly sliced'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 11677, u'unit': u'cup'}, {u'originalString': u'1 pound fresh spinach', u'aisle': u'Produce', u'name': u'spinach', u'metaInformation': [u'fresh'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'unitLong': u'pound', u'unitShort': u'lb', u'amount': 1.0, u'consistency': u'solid', u'id': 10011457, u'unit': u'pound'}, {u'originalString': u'\xbc tsp white pepper', u'aisle': u'Spices and Seasonings', u'name': u'white pepper', u'metaInformation': [u'white'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-pepper.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2032, u'unit': u'tsp'}], u'healthScore': 20.0, u'imageType': u'jpg', u'spoonacularScore': 86.0, u'sourceUrl': u'http://www.seasonalandsavory.com/2013/11/creamy-parmesan-spinach-and-mushrooms.html', u'glutenFree': True, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 4, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/slotted-spoon.jpg', u'id': 404636, u'name': u'slotted spoon'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/roasting-pan.jpg', u'id': 404646, u'name': u'baking pan'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/stock-pot.jpg', u'id': 404752, u'name': u'pot'}], u'step': u'Preheat the oven to 350 degrees. Lightly grease a 2-or-3-quart baking dish.In a large pot with a lid, cook the mushrooms in the two tablespoons of butter until the mushrooms are soft. Use a slotted spoon to transfer the cooked mushrooms to the prepared baking dish.Using the same pot, cook the spinach over medium-high heat in batches, and cover while each batch wilts, adding more as space allows. When all of the spinach has been wilted, place it in a food processor along with the green onions, white pepper, salt, creme fraiche, and parmesan cheese. You can do this in batches if your food processor is small. Blend until smooth, and then transfer the mixture into the baking dish with the mushrooms. Stir to blend in the mushrooms.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sour-cream.jpg', u'id': 1001056, u'name': u'creme fraiche'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spring-onions.jpg', u'id': 11291, u'name': u'green onions'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-pepper.jpg', u'id': 2032, u'name': u'white pepper'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [], u'step': u'Bake the spinach mixture for 20 minutes, or until bubbly around the edges. While it is baking, prepare the shallots.', u'length': {u'number': 20, u'unit': u'minutes'}, u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'id': 11677, u'name': u'shallot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Heat the tablespoon of butter and the olive oil in a large skillet over medium-high heat.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/olive-oil.jpg', u'id': 4053, u'name': u'olive oil'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'id': 1001, u'name': u'butter'}]}, {u'equipment': [], u'step': u'Add the shallots and cook, stirring occasionally, until they begin to turn golden brown and some of them look crispy. Top the spinach casserole with the shallots and serve.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/shallots.jpg', u'id': 11677, u'name': u'shallot'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/spinach.jpg', u'id': 10011457, u'name': u'spinach'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 184, u'instructions': u'Preheat the oven to 350 degrees. Lightly grease a 2-or-3-quart baking dish.In a large pot with a lid, cook the mushrooms in the two tablespoons of butter until the mushrooms are soft. Use a slotted spoon to transfer the cooked mushrooms to the prepared baking dish.Using the same pot, cook the spinach over medium-high heat in batches, and cover while each batch wilts, adding more as space allows. When all of the spinach has been wilted, place it in a food processor along with the green onions, white pepper, salt, creme fraiche, and parmesan cheese. You can do this in batches if your food processor is small. Blend until smooth, and then transfer the mixture into the baking dish with the mushrooms. Stir to blend in the mushrooms.Bake the spinach mixture for 20 minutes, or until bubbly around the edges. While it is baking, prepare the shallots. Heat the tablespoon of butter and the olive oil in a large skillet over medium-high heat. Add the shallots and cook, stirring occasionally, until they begin to turn golden brown and some of them look crispy. Top the spinach casserole with the shallots and serve.', u'spoonacularSourceUrl': u'https://spoonacular.com/creamy-parmesan-spinach-and-mushrooms-478552', u'dishTypes': [u'side dish'], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 65, u'servings': 8, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}