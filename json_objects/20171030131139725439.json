{u'recipes': [{u'sourceName': u'Picky Eater Blog', u'cuisines': [], u'image': u'https://spoonacular.com/recipeImages/516542-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 516542, u'veryPopular': True, u'lowFodmap': False, u'title': u'Cocoa Oat Truffles', u'diets': [u'gluten free', u'dairy free', u'lacto ovo vegetarian', u'vegan'], u'pricePerServing': 13.3, u'creditText': u'Picky Eater Blog', u'extendedIngredients': [{u'originalString': u'1/2 cup raw, natural almond butter', u'aisle': u'Nut butters, Jams, and Honey', u'name': u'almond butter', u'metaInformation': [u'raw'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/peanut-butter.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.5, u'consistency': u'solid', u'id': 12195, u'unit': u'cup'}, {u'originalString': u'1 cup pitted dates, chopped', u'aisle': u'Dried Fruits;Produce', u'name': u'dates', u'metaInformation': [u'pitted', u'chopped'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dates.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 9087, u'unit': u'cup'}, {u'originalString': u'1/4 tsp ground cinnamon', u'aisle': u'Spices and Seasonings', u'name': u'ground cinnamon', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2010, u'unit': u'tsp'}, {u'originalString': u'1/4 tsp ground nutmeg', u'aisle': u'Spices and Seasonings', u'name': u'ground nutmeg', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 0.25, u'consistency': u'solid', u'id': 2025, u'unit': u'tsp'}, {u'originalString': u'1 cup rolled oats', u'aisle': u'Cereal', u'name': u'rolled oats', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rolled-oats.jpg', u'unitLong': u'cup', u'unitShort': u'cup', u'amount': 1.0, u'consistency': u'solid', u'id': 8120, u'unit': u'cup'}, {u'originalString': u'1/4 cup unsweetened cocoa powder', u'aisle': u'Baking', u'name': u'unsweetened cocoa powder', u'metaInformation': [u'unsweetened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 0.25, u'consistency': u'solid', u'id': 19165, u'unit': u'cup'}, {u'originalString': u'2 tsp vanilla extract', u'aisle': u'Baking', u'name': u'vanilla extract', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'unitLong': u'teaspoons', u'unitShort': u'tsp', u'amount': 2.0, u'consistency': u'liquid', u'id': 2050, u'unit': u'tsp'}], u'healthScore': 3.0, u'imageType': u'jpg', u'spoonacularScore': 47.0, u'sourceUrl': u'http://pickyeaterblog.com/cocoa-oat-truffles/', u'glutenFree': True, u'vegan': True, u'veryHealthy': False, u'weightWatcherSmartPoints': 0, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/food-processor.png', u'id': 404771, u'name': u'food processor'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place the oats in the bowl of a food processor and process until finely ground.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/rolled-oats.jpg', u'id': 8120, u'name': u'oats'}]}, {u'equipment': [], u'step': u'Add dates, 2 Tbsp cocoa powder, almond butter, vanilla, nutmeg and cinnamon and process until finely ground and slightly sticking together (it will form a loose batter).', u'number': 2, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/almond-butter.jpg', u'id': 12195, u'name': u'almond butter'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cinnamon.jpg', u'id': 2010, u'name': u'cinnamon'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla.jpg', u'id': 2050, u'name': u'vanilla'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/ground-nutmeg.jpg', u'id': 2025, u'name': u'nutmeg'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/dates.jpg', u'id': 9087, u'name': u'dates'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place remaining cocoa powder in a shallow bowl.', u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}]}, {u'equipment': [], u'step': u'Roll truffle mixture into balls (2 or 3 tsp per truffle), pressing firmly for mixture to stick together. Note: if you use 2 tsp per truffle, you will end up with 40 calories per truffle. If you use 3 tsp per truffle, you will end up with 60 calories per truffle. I also spread the \u201cbatter\u201d out on a plate because it was easier to scoop up with my 1 tsp measure. After a few presses in between my hands, I ended up with these perfect little cocoa balls. Note: do not roll the batter in your hands (to form a ball) \u2013 it will fall apart. Make sure to press it firmly with your fingers/hands until it binds together, kind of like if you were playing with play-dough.', u'number': 4, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}]}, {u'equipment': [], u'step': u'Roll truffles in cocoa powder and chill until ready to serve (I chilled mine for about 1 hour).', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cocoa-powder.jpg', u'id': 19165, u'name': u'cocoa powder'}]}], u'name': u''}], u'winePairing': {}, u'aggregateLikes': 596, u'instructions': u'Place the oats in the bowl of a food processor and process until finely ground.Add dates, 2 Tbsp cocoa powder, almond butter, vanilla, nutmeg and cinnamon and process until finely ground and slightly sticking together (it will form a loose batter).Place remaining cocoa powder in a shallow bowl. Roll truffle mixture into balls (2 or 3 tsp per truffle), pressing firmly for mixture to stick together. Note: if you use 2 tsp per truffle, you will end up with 40 calories per truffle. If you use 3 tsp per truffle, you will end up with 60 calories per truffle. I also spread the \u201cbatter\u201d out on a plate because it was easier to scoop up with my 1 tsp measure. After a few presses in between my hands, I ended up with these perfect little cocoa balls. Note: do not roll the batter in your hands (to form a ball) \u2013 it will fall apart. Make sure to press it firmly with your fingers/hands until it binds together, kind of like if you were playing with play-dough.Roll truffles in cocoa powder and chill until ready to serve (I chilled mine for about 1 hour).', u'spoonacularSourceUrl': u'https://spoonacular.com/cocoa-oat-truffles-516542', u'dishTypes': [u'antipasti', u'starter', u'snack', u'appetizer', u'antipasto', u"hor d'oeuvre"], u'occasions': [], u'vegetarian': True, u'ketogenic': False, u'readyInMinutes': 60, u'servings': 45, u'sustainable': False, u'dairyFree': True, u'whole30': False}]}