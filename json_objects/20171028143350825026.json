martPoints': 0, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/mixing-bowl.jpg', u'id': 405907, u'name': u'mixing bowl'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Preheat oven to 350 degrees. In a mixing bowl whisk together flour, cornstarch, baking soda and salt for 20 seconds, set aside.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 18372, u'name': u'baking soda'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/white-powder.jpg', u'id': 20027, u'name': u'corn starch'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'id': 20081, u'name': u'all purpose flour'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/salt.jpg', u'id': 2047, u'name': u'salt'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/microwave.jpg', u'id': 404762, u'name': u'microwave'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Place butter in a microwave safe bowl, heat in microwave on HIGH power until butter has melted halfway.', u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/whisk.png', u'id': 404661, u'name': u'whisk'}], u'step': u'Remove and whisk until fully melted.', u'number': 3, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/stand-mixer.jpg', u'id': 404665, u'name': u'stand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/bowl.jpg', u'id': 404783, u'name': u'bowl'}], u'step': u'Pour butter into the bowl of an electric stand mixer.', u'number': 4, u'ingredients': []}, {u'equipment': [], u'step': u'Add brown sugar and granulated sugar, fit mixer with paddle attachment and blend mixture on medium speed until well combined.', u'number': 5, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/sugar-cubes.jpg', u'id': 19335, u'name': u'granulated sugar'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-dark.jpg', u'id': 19334, u'name': u'brown sugar'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-paper.jpg', u'id': 404770, u'name': u'baking paper'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/wax-paper.jpg', u'id': 404739, u'name': u'wax paper'}], u'step': u'Mix in egg and vanilla extract. With mixer set on low speed, slowly add in dry ingredients and mix until combined. Stir in chocolate chips. Scoop dough out a slightly heaping 1/2 tsp at a time and drop onto plates (wax paper, whatever). Shape each into a ball, then if desired press 3 - 4 more chocolate chips into top (totally optional, mostly for looks...and more chocolate). Align on two Silpat or parchment paper lined baking sheets, spacing 1-inch apart.', u'number': 6, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/vanilla-extract.jpg', u'id': 2050, u'name': u'vanilla extract'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'id': 1123, u'name': u'egg'}]}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u'Bake in preheated oven about 5 - 6 minutes (they should still fill soft and slightly under-baked, they will continue to cook slightly once removed from oven so carefully not to over-bake).', u'length': {u'number': 6, u'unit': u'minutes'}, u'number': 7, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/baking-sheet.jpg', u'id': 404727, u'name': u'baking sheet'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/wire-rack.jpg', u'id': 405900, u'name': u'wire rack'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}], u'step': u"Remove from oven and cool on baking sheet several minutes then transfer to a wire rack to cool. Store in an airtight container.Recipe Source: inspired by Yammie's Noshery", u'number': 8, u'ingredients': []}], u'name': u''}], u'winePairing': {u'pairingText': u"Cookies can be paired with Cream Sherry, Madeira, and Prosecco. Sweet bubbly Prosecco doesn't overwhelm simple sugar or shortbread cookies, a sweet cream sherry complements spiced cookies, and madeira's nutty notes match cookies with nuts perfectly. You could try NV Solera Cream Sherry. Reviewers quite like it with a 4.5 out of 5 star rating and a price of about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'madeira', u'prosecco'], u'productMatches': [{u'description': u'nutty , butterscotch , caramel', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 1314, u'instructions': u"Preheat oven to 350 degrees. In a mixing bowl whisk together flour, cornstarch, baking soda and salt for 20 seconds, set aside.Place butter in a microwave safe bowl, heat in microwave on HIGH power until butter has melted halfway. Remove and whisk until fully melted. Pour butter into the bowl of an electric stand mixer. Add brown sugar and granulated sugar, fit mixer with paddle attachment and blend mixture on medium speed until well combined. Mix in egg and vanilla extract. With mixer set on low speed, slowly add in dry ingredients and mix until combined. Stir in chocolate chips. Scoop dough out a slightly heaping 1/2 tsp at a time and drop onto plates (wax paper, whatever). Shape each into a ball, then if desired press 3 - 4 more chocolate chips into top (totally optional, mostly for looks...and more chocolate). Align on two Silpat or parchment paper lined baking sheets, spacing 1-inch apart. Bake in preheated oven about 5 - 6 minutes (they should still fill soft and slightly under-baked, they will continue to cook slightly once removed from oven so carefully not to over-bake). Remove from oven and cool on baking sheet several minutes then transfer to a wire rack to cool. Store in an airtight container.Recipe Source: inspired by Yammie's Noshery", u'spoonacularSourceUrl': u'https://spoonacular.com/chocolate-chip-cookie-bites-605801', u'dishTypes': [u'dessert'], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 31, u'servings': 144, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}