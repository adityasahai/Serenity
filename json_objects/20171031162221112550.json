{u'recipes': [{u'sourceName': u'Roxanas Home Baking', u'cuisines': [u'american'], u'image': u'https://spoonacular.com/recipeImages/547352-556x370.jpg', u'cheap': False, u'gaps': u'no', u'cookingMinutes': 60, u'id': 547352, u'veryPopular': True, u'lowFodmap': False, u'title': u'Apple pie bars', u'diets': [u'lacto ovo vegetarian'], u'preparationMinutes': 30, u'pricePerServing': 639.89, u'creditText': u'Roxanas Home Baking', u'extendedIngredients': [{u'originalString': u'1 1 /4 cup (150 grams) all purpose flour', u'aisle': u'Baking', u'name': u'all purpose flour', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/flour.png', u'unitLong': u'grams', u'unitShort': u'g', u'amount': 150.0, u'consistency': u'solid', u'id': 20081, u'unit': u'grams'}, {u'originalString': u'3-4 big apples (preferably Granny Smith)', u'aisle': u'Produce', u'name': u'apples', u'metaInformation': [u'(preferably Granny Smith)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/apple.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 9003, u'unit': u''}, {u'originalString': u'2 tbsp. brown sugar', u'aisle': u'Baking', u'name': u'brown sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/brown-sugar-light.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 2.0, u'consistency': u'solid', u'id': 19334, u'unit': u'tbsp'}, {u'originalString': u'4 tbsp. cold diced butter', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [u'diced', u'cold'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/butter-sliced.jpg', u'unitLong': u'tablespoons', u'unitShort': u'Tbsp', u'amount': 4.0, u'consistency': u'solid', u'id': 1001, u'unit': u'tbsp'}, {u'originalString': u'7 tbsp. (70 grams) butter very very soft(almost melted but still in solid form)', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'butter', u'metaInformation': [u'melted'], u'