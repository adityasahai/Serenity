{u'recipes': [{u'sourceName': u'Who Needs a Cape', u'cuisines': [u'american'], u'image': u'https://spoonacular.com/recipeImages/520926-556x370.jpg', u'cheap': False, u'gaps': u'no', u'id': 520926, u'veryPopular': True, u'lowFodmap': False, u'title': u'Spiced Cream Cheese Frosted Pumpkin Ale Cupcakes', u'diets': [], u'pricePerServing': 44.25, u'creditText': u'Who Needs a Cape', u'extendedIngredients': [{u'originalString': u'1 1/3 cup Blue Moon Harvest Pumpkin Ale or any pumpkin ale)', u'aisle': u'Alcoholic Beverages', u'name': u'ale', u'metaInformation': [u'blue', u'or any pumpkin ale)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/no.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 1.3333333333333333, u'consistency': u'liquid', u'id': 10014006, u'unit': u'cup'}, {u'originalString': u'3 cups confectioners sugar', u'aisle': u'Baking', u'name': u'confectioners sugar', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/powdered-sugar.jpg', u'unitLong': u'cups', u'unitShort': u'cup', u'amount': 3.0, u'consistency': u'solid', u'id': 19336, u'unit': u'cups'}, {u'originalString': u'8 oz cream cheese, softened', u'aisle': u'Cheese', u'name': u'cream cheese', u'metaInformation': [u'softened'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'unitLong': u'ounces', u'unitShort': u'oz', u'amount': 8.0, u'consistency': u'solid', u'id': 1017, u'unit': u'oz'}, {u'originalString': u'3 eggs', u'aisle': u'Milk, Eggs, Other Dairy', u'name': u'eggs', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/egg.jpg', u'unitLong': u'', u'unitShort': u'', u'amount': 3.0, u'consistency': u'solid', u'id': 1123, u'unit': u''}, {u'originalString': u'1 t pumpkin pie spice', u'aisle': u'Spices and Seasonings', u'name': u'pumpkin pie spice', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/garam-masala.jpg', u'unitLong': u'teaspoon', u'unitShort': u'tsp', u'amount': 1.0, u'consistency': u'solid', u'id': 1002035, u'unit': u't'}, {u'originalString': u'1 T pumpkin puree', u'aisle': u'Baking', u'name': u'pumpkin puree', u'metaInformation': [], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pumpkin-puree.jpg', u'unitLong': u'tablespoon', u'unitShort': u'Tbsp', u'amount': 1.0, u'consistency': u'solid', u'id': 11424, u'unit': u'T'}, {u'originalString': u'1 box yellow cake mix (I used Betty Crocker)', u'aisle': u'Baking', u'name': u'yellow cake mix', u'metaInformation': [u'yellow', u'(I used Betty Crocker)'], u'image': u'https://spoonacular.com/cdn/ingredients_100x100/yellow-cake.jpg', u'unitLong': u'box', u'unitShort': u'box', u'amount': 1.0, u'consistency': u'solid', u'id': 18144, u'unit': u'box'}], u'healthScore': 1.0, u'imageType': u'jpg', u'spoonacularScore': 15.0, u'sourceUrl': u'http://whoneedsacape.com/2013/10/spiced-cream-cheese-frosted-pumpkin-ale-cupcakes/', u'glutenFree': False, u'vegan': False, u'veryHealthy': False, u'weightWatcherSmartPoints': 11, u'analyzedInstructions': [{u'steps': [{u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/muffin-or-cupcake-forms.png', u'id': 404676, u'name': u'muffin liners'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/oven.jpg', u'id': 404784, u'name': u'oven'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/pan.png', u'id': 404645, u'name': u'frying pan'}], u'step': u'Preheat oven according to package directions.  Line pan with cupcake liners.You are substituting the water and oil listed on the cake mix directions for the pumpkin puree and beer.  Do not add oil and water.', u'number': 1, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/pumpkin-puree.jpg', u'id': 11424, u'name': u'canned pumpkin'}, {u'image': u'https://spoonacular.com/cdn/ingredients_100x100/beer.jpg', u'id': 14006, u'name': u'beer'}]}, {u'equipment': [], u'step': u'Mix all ingredients on medium speed 2-3 minutes until well incorporated.', u'length': {u'number': 3, u'unit': u'minutes'}, u'number': 2, u'ingredients': []}, {u'equipment': [{u'image': u'https://spoonacular.com/cdn/equipment_100x100/hand-mixer.png', u'id': 404628, u'name': u'hand mixer'}, {u'image': u'https://spoonacular.com/cdn/equipment_100x100/toothpicks.jpg', u'id': 404644, u'name': u'toothpicks'}], u'step': u'Bake according to package directions, mine were done at 21 minutes when toothpick inserted came out clean.Allow to cool completely before frosting.Beat cream cheese with electric mixer 2-3 minutes until airy.  Gradually add in all other ingredients & mix until well blended & smooth.', u'length': {u'number': 24, u'unit': u'minutes'}, u'number': 3, u'ingredients': [{u'image': u'https://spoonacular.com/cdn/ingredients_100x100/cream-cheese.jpg', u'id': 1017, u'name': u'cream cheese'}]}], u'name': u''}], u'winePairing': {u'pairingText': u"Cream Sherry, Moscato d'Asti, and Port are my top picks for Cupcakes. A common wine pairing rule is to make sure your wine is sweeter than your food. Delicate desserts go well with Moscato d'Asti, nutty desserts with cream sherry, and caramel or chocolate desserts pair well with port. The NV Solera Cream Sherry with a 4.5 out of 5 star rating seems like a good match. It costs about 17 dollars per bottle.", u'pairedWines': [u'cream sherry', u'moscato dasti', u'port'], u'productMatches': [{u'description': u'The Solera Cream Sherry has a brilliant amber and deep copper hue. With butterscotch and pecan aromas, the sweet salted nut and brown spice aromas carry a complex caramel accent. A sweet entry leads to a rounded, lush, moderately full-bodied palate with a lengthy, flavorful finish.', u'title': u'NV Solera Cream Sherry', u'price': u'$16.99', u'id': 428475, u'link': u'https://www.amazon.com/NV-Solera-Cream-Sherry-750/dp/B00HSME8OW', u'imageUrl': u'https://spoonacular.com/productImages/428475-312x231.jpg'}]}, u'aggregateLikes': 620, u'instructions': u'Preheat oven according to package directions.  Line pan with cupcake liners.You are substituting the water and oil listed on the cake mix directions for the pumpkin puree and beer.  Do not add oil and water.Mix all ingredients on medium speed 2-3 minutes until well incorporated.Bake according to package directions, mine were done at 21 minutes when toothpick inserted came out clean.Allow to cool completely before frosting.Beat cream cheese with electric mixer 2-3 minutes until airy.  Gradually add in all other ingredients & mix until well blended & smooth.', u'spoonacularSourceUrl': u'https://spoonacular.com/spiced-cream-cheese-frosted-pumpkin-ale-cupcakes-520926', u'dishTypes': [], u'occasions': [], u'vegetarian': False, u'ketogenic': False, u'readyInMinutes': 45, u'servings': 18, u'sustainable': False, u'dairyFree': False, u'whole30': False}]}